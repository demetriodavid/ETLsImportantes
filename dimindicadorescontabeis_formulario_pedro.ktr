    <connection>Postgres</connection>
    <sql>select distinct '[' opening, ']' closure, ind.codigo, ind.grupo demonstrativo, ind.subgrupo indicador, 
1 operacao, 'N' OCULTAR
, '"'||pc.codigo ||'#'||ind.contas||'"' contas 
from dimindicadorescontascontabeisaux ind 
inner join dimcontacontabil cc on ind.contas = cc.codigo
inner join dimplanocontas pc on cc.planocontas = pc.codigo

where pc.datainicio&lt;=current_date and pc.datafim >current_date
 
order by ind.codigo, '"'||pc.codigo ||'#'||ind.contas||'"'</sql>
