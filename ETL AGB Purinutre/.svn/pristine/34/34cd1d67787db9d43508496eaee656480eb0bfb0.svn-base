    <connection>[DB] - Oracle - AgriBusiness ERP</connection>
    <sql>WITH
    PARC_REC AS
    (
        SELECT
            PR.CTRL_REC,
            PR.CTRL_CBR,
            PR.CODI_IND,
            (
                CASE
                    WHEN PR.CODI_IND IS NOT NULL
                    THEN (PR.VLOR_REC / IV.VLOR_VLR)
                    ELSE PR.VLOR_REC
                END
            ) AS VLR_PARCELA
        FROM
            RECEBER PR
            LEFT OUTER JOIN INDVALOR IV ON IV.CODI_EMP = PR.EMPR_VLR
                AND IV.CODI_IND = PR.CODI_IND
                AND IV.DATA_VLR = PR.DATA_VLR
    ),
    PARC_PAGA AS
    (
        SELECT
            CB.CTRL_REC,
            MAX(CB.DPAG_BAI) KEEP(DENSE_RANK FIRST ORDER BY CB.CTRL_REC) AS DPAG_BAI,
            SUM(
                CASE
                    WHEN PR.CODI_IND IS NOT NULL
                    THEN (CB.VLOR_BAI / IV.VLOR_VLR)
                    ELSE CB.VLOR_BAI
                END
            ) AS VLR_PAGO
        FROM
            CRCBAIXA CB
            INNER JOIN RECEBER PR ON PR.CTRL_REC = CB.CTRL_REC
            LEFT OUTER JOIN INDVALOR IV ON IV.CODI_EMP = PR.EMPR_VLR
                AND IV.CODI_IND = PR.CODI_IND
                AND IV.DATA_VLR = CB.DATA_VLR
        WHERE CB.SITU_BAI = 'N'
        GROUP BY CB.CTRL_REC
    ),
    PARCELA AS
    (
        SELECT
            PR.CTRL_REC,
            PP.DPAG_BAI,
            PR.VLR_PARCELA,
            PP.VLR_PAGO,
            (
                CASE
                    WHEN PP.VLR_PAGO IS NULL
                    THEN PR.VLR_PARCELA
                    ELSE
                        CASE
                            WHEN PR.CODI_IND IS NULL
                            THEN ROUND(PR.VLR_PARCELA - PP.VLR_PAGO, 2)
                            ELSE (PR.VLR_PARCELA - PP.VLR_PAGO)
                        END
                END
            ) AS VLR_RECEBER
        FROM
            PARC_REC PR
            LEFT OUTER JOIN PARC_PAGA PP ON PR.CTRL_REC = PP.CTRL_REC
    ),
    AVISTA AS
    (
        SELECT
            CODI_EMP,
            NOTA_NOT,
            SUM(VLOR_ENO) AS VLR_PAGO
        FROM ENTRNOTA
        GROUP BY CODI_EMP, NOTA_NOT
    ),
    PROR AS
    (
        SELECT
            CTRL_REC,
            MAX(VENC_IPR) AS VENC_IPR,
            MAX(MULT_IPR) KEEP(DENSE_RANK FIRST ORDER BY DUMANUT DESC) AS MULT_IPR,
            MAX(VLOR_IPR) KEEP(DENSE_RANK FIRST ORDER BY DUMANUT DESC) AS VLOR_IPR
        FROM IPRORROGACRC
        GROUP BY CTRL_REC
    )

SELECT
    TO_CHAR(N.CODI_EMP) AS NK_EMPRESA,
    TO_CHAR(N.CODI_TRA) AS NK_CLIENTE,
    TO_CHAR(DR.CODI_TDO) AS NK_TIPO_DOCUMENTO,
    TO_CHAR(DR.DATA_CBR, UPPER('DD/MM/YYYY')) AS NK_TEMPO_DIA_INCLUSAO,
    TO_CHAR(COALESCE(PRO.VENC_IPR, PDR.VENC_REC), UPPER('DD/MM/YYYY')) AS NK_TEMPO_DIA_VENC,
    (
        CASE
            WHEN P.CTRL_REC IS NOT NULL AND P.VLR_RECEBER = 0
            THEN TO_CHAR(P.DPAG_BAI, UPPER('DD/MM/YYYY'))
            ELSE '31/12/9999'
        END
    ) AS NK_TEMPO_DIA_BAIXA,
    TO_CHAR(N.NOTA_NOT) AS DD_NOTA_FISCAL,
    DR.NUME_CBR AS NUMERO_DOCUMENTO,
    PDR.NPAR_REC AS NUMERO_PARCELA,
    DR.TOTA_CBR AS VLR_PRINCIPAL,
    AV.VLR_PAGO AS VLR_AVISTA,
    PDR.JAPV_REC AS VLR_JUROS,
    PDR.VDOF_REC AS VLR_DESCONTO,
    COALESCE(PRO.MULT_IPR * PRO.VLOR_IPR, 0) AS VLR_MULTA,
    P.VLR_RECEBER AS VLR_SALDO
FROM
    NOTA N
    INNER JOIN TRANSAC T ON T.CODI_TRA = N.CODI_TRA
