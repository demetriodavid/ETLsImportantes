  <step>
    <name>[DB] - ERP - Estoque Fisico - Solicitacao de Transferencia</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>[DB] - Oracle - AgriBusiness ERP</connection>
    <sql>SELECT /* Transferencia - Origem */
    UPPER('SOL_TRANSF#ORIG#') || TO_CHAR(ST.CTRL_SLT) AS NK_MOVIMENTO_ESTOQUE,
    CAST(P.CODI_EMP AS INTEGER) AS CODI_EMP,
    ST.CODI_PSV,
	'ZZ' AS ESTA_MUN,
    CAST(ST.DATA_SLT AS DATE) AS DATA_MOV,
    (
        CASE
            WHEN TP.SOLT_TCT = UPPER('S') THEN -1
            ELSE 1
        END
    ) * COALESCE(ST.QTDE_SLT, 0) AS QTDE
FROM
    SOLTRANSF ST
    INNER JOIN PRODSERV PS ON PS.CODI_PSV = ST.CODI_PSV
    INNER JOIN PARAMGER P ON ST.EORI_EMP = P.CODI_EMP
    INNER JOIN
        (
            CTRLSALDO CS
            INNER JOIN TOPCTRL TP ON TP.CODI_CTR = CS.CODI_CTR AND
                CS.DESC_CTR = 'ESTOQUE FISICO'
        ) ON TP.CODI_TOP = P.TOTO_PAR
WHERE
    TP.SOLT_TCT IN ('A', 'S')
    AND PS.PRSE_PSV &lt;&gt; 'S'

UNION ALL

SELECT /* Transferencia - Destino */
    UPPER('SOL_TRANSF#DEST#') || TO_CHAR(ST.CTRL_SLT) AS NK_MOVIMENTO_ESTOQUE,
    CAST(P.CODI_EMP AS INTEGER) AS CODI_EMP,
    ST.CODI_PSV,
	'ZZ' AS ESTA_MUN,
    CAST(ST.DATA_SLT AS DATE) AS DATA_MOV,
    (
        CASE
            WHEN TP.SOLT_TCT = UPPER('S') THEN -1
            ELSE 1
        END
    ) * COALESCE(ST.QTDE_SLT, 0) AS QTDE
FROM
    SOLTRANSF ST
    INNER JOIN PRODSERV PS ON PS.CODI_PSV = ST.CODI_PSV
    INNER JOIN PARAMGER P ON ST.EDES_EMP = P.CODI_EMP
    INNER JOIN
        (
            CTRLSALDO CS
            INNER JOIN TOPCTRL TP ON TP.CODI_CTR = CS.CODI_CTR AND
                CS.DESC_CTR = 'ESTOQUE FISICO'
        ) ON TP.CODI_TOP = P.TOTD_PAR
WHERE
    TP.SOLT_TCT IN ('A', 'S')
    AND PS.PRSE_PSV &lt;&gt; 'S'
</sql>
