    <connection>[DB] - Oracle - AgriBusiness ERP</connection>
    <sql>SELECT /* Nota Fiscal emitida pela empresa - Sem pedido */
    UPPER('NF_EMP#SEMPEDIDO#') || TO_CHAR(NT.NPRE_NOT) AS NK_MOVIMENTO_ESTOQUE,
    (
        CAST(
            CASE
                WHEN (IPED.LRET_IPE IS NULL OR (D.CTRL_DCP IS NOT NULL AND TP.CODI_CTR = 1)) AND TP.PDVE_TCT = 'N' AND COALESCE(TP.ATED_TCT, 'N') = 'N'
                THEN NT.CODI_EMP
                WHEN (IPED.LRET_IPE IS NULL OR (D.CTRL_DCP IS NOT NULL AND TP.CODI_CTR = 1)) AND TP.PDVE_TCT = 'A' AND COALESCE(TP.ATED_TCT, 'N') = 'N'
                THEN INO.EMPR_PED
                ELSE CA.CODI_EMP
            END AS INTEGER
        )
    ) AS CODI_EMP,
    INO.CODI_PSV,
    (
        CASE
            WHEN NT.PROP_PRO IS NULL
            THEN M.ESTA_MUN
            ELSE M2.ESTA_MUN
        END
    ) AS ESTA_MUN,
    CAST(INO.DEMI_NOT AS DATE) AS DATA_MOV,
    (
        CASE
            WHEN TP.NFEM_TCT = UPPER('S') THEN -1
            ELSE 1
        END
    ) * COALESCE(INO.QTDE_INO, 0) AS QTDE
FROM
    INOTA INO
    INNER JOIN PRODSERV PS ON PS.CODI_PSV = INO.CODI_PSV
    INNER JOIN NOTA NT       ON (INO.NPRE_NOT = NT.NPRE_NOT)
    INNER JOIN TRANSAC T     ON (T.CODI_TRA = NT.CODI_TRA)
    LEFT JOIN
        (
            CTRLSALDO CS
            INNER JOIN TOPCTRL TP ON TP.CODI_CTR = CS.CODI_CTR AND
                CS.DESC_CTR = 'ESTOQUE FISICO'
        ) ON TP.CODI_TOP = INO.CODI_TOP
    LEFT JOIN CADEMP CA      ON (CA.COD1_TRA = NT.CODI_TRA AND COALESCE(TP.ATED_TCT, 'N') = 'S')
    LEFT JOIN MUNICIPIO M    ON (M.CODI_MUN = T.CODI_MUN)
    LEFT JOIN PROPRIED PRO   ON (NT.PROP_PRO = PRO.PROP_PRO)
    LEFT JOIN MUNICIPIO M2   ON (M2.CODI_MUN = PRO.CODI_MUN)
    LEFT JOIN IPEDIDO IPED   ON (IPED.CODI_EMP = INO.EMPR_PED AND IPED.PEDI_PED = INO.PEDI_PED AND IPED.SERI_PED = INO.SERI_PED AND IPED.CODI_PSV = INO.CODI_PSV)
    LEFT JOIN DOCSCAPTACAO D ON (NT.NPRE_NOT = D.NPRE_NOT)
WHERE
    PS.PRSE_PSV &lt;&gt; 'S' AND
    ((NT.CODI_EMP IS NOT NULL AND (IPED.LRET_IPE IS NULL OR (D.CTRL_DCP IS NOT NULL AND TP.CODI_CTR = 1)) AND TP.PDVE_TCT = 'N' AND COALESCE(TP.ATED_TCT, 'N') = 'N') OR
    (INO.EMPR_PED IS NOT NULL AND (IPED.LRET_IPE IS NULL OR (D.CTRL_DCP IS NOT NULL AND TP.CODI_CTR = 1)) AND TP.PDVE_TCT = 'A' AND COALESCE(TP.ATED_TCT, 'N') = 'N') OR
