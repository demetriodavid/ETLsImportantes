  <step>
    <name>[DB] - ERP - Estoque Fisico - Comprovante de Entrega</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>[DB] - Oracle - AgriBusiness ERP</connection>
    <sql>SELECT
    UPPER('COMP_ENT#') || TO_CHAR(C.NUME_COM) AS NK_MOVIMENTO_ESTOQUE,
    (
        CAST(
            CASE
                WHEN TP.PDVE_TCT = UPPER('N') AND COALESCE(TP.ATED_TCT, 'N') = 'N'
                THEN IC.CODI_EMP
                WHEN TP.PDVE_TCT = UPPER('A') AND COALESCE(TP.ATED_TCT, 'N') = 'N'
                THEN IC.EMPR_PED
                ELSE CA.CODI_EMP
            END AS INTEGER
        )
    ) AS CODI_EMP,
    IC.CODI_PSV AS CODI_PSV,
    (
        CASE
            WHEN C.PROP_PRO IS NULL THEN M.ESTA_MUN
            ELSE M2.ESTA_MUN
        END
    ) AS ESTA_MUN,
    CAST(IC.DATA_COM AS DATE) AS DATA_MOV,
    (
        CASE
            WHEN TP.CENT_TCT = UPPER('S') THEN -1
            ELSE 1
        END
    ) * COALESCE(IC.QTDE_ICO, 0) AS QTDE
FROM
    ICOMPROVA IC
    INNER JOIN COMPROVA C ON (C.CODI_EMP = IC.CODI_EMP AND C.NUME_COM = IC.NUME_COM)
    LEFT JOIN
        (
            CTRLSALDO CS
            INNER JOIN TOPCTRL TP ON TP.CODI_CTR = CS.CODI_CTR AND
                CS.DESC_CTR = 'ESTOQUE FISICO'
        ) ON TP.CODI_TOP = C.CODI_TOP
    LEFT JOIN CADEMP CA ON (CA.COD1_TRA = C.CODI_TRA AND COALESCE(TP.ATED_TCT, 'N') = 'S')
    INNER JOIN TRANSAC T ON (T.CODI_TRA = C.CODI_TRA)
    LEFT JOIN MUNICIPIO M ON (M.CODI_MUN = T.CODI_MUN)
    LEFT JOIN PROPRIED PRO ON (C.PROP_PRO = PRO.PROP_PRO)
    LEFT JOIN MUNICIPIO M2 ON (M2.CODI_MUN = PRO.CODI_MUN)
WHERE
      ((IC.CODI_EMP IS NOT NULL AND TP.PDVE_TCT = 'N' AND COALESCE(TP.ATED_TCT, 'N') = 'N') OR
       (IC.EMPR_PED IS NOT NULL AND TP.PDVE_TCT = 'A' AND COALESCE(TP.ATED_TCT, 'N') = 'N') OR
       (CA.CODI_EMP IS NOT NULL )) AND
      (TP.CENT_TCT IN ('A', 'S')) AND
      ((C.SITU_COM &lt;&gt; '9') OR (C.SITU_COM IS NULL))
</sql>
