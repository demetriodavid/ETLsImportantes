    INNER JOIN PEDCOM PE ON (PE.CODI_EMP = IPE.CODI_EMP AND PE.NUME_PEC = IPE.NUME_PEC AND PE.TPPC_PEC &lt;&gt; 'C')
    LEFT OUTER JOIN
