    <connection>[DB] - Oracle - AgriBusiness ERP</connection>
    <sql>SELECT /* Ordem de produção */
    UPPER('ORD_PROD#') || TO_CHAR(AO.CODI_ACO) AS NK_MOVIMENTO_ESTOQUE,
    CAST(O.CODI_EMP AS INTEGER) AS CODI_EMP,
    IO.CODI_PSV,
    'ZZ' AS ESTA_MUN,
    CAST(AO.DINI_ACO AS DATE) AS DATA_MOV,
    (
        CASE
            WHEN OC.ACAO_OCT = UPPER('S')
            THEN -1
            ELSE 1
        END
    ) * COALESCE(IO.QUTI_PRO, 0) AS QTDE
FROM
    IORDEM         IO
    LEFT JOIN   ACOMPANHAORDEM AO ON (AO.CODI_ACO = IO.CODI_ACO)
    LEFT JOIN   ORDEMPRODUCAO  O  ON (O.CODI_ORD  = AO.CODI_ORD)
    LEFT JOIN   PRODSERV       PS ON (PS.CODI_PSV = IO.CODI_PSV)
    LEFT JOIN   ETAPA          E  ON (E.CODI_ETA  = AO.CODI_ETA)
    LEFT JOIN
        (
            CTRLSALDO CS
            INNER JOIN ORDEMCTRL OC ON OC.CODI_CTR = CS.CODI_CTR AND
                CS.DESC_CTR = 'ESTOQUE FISICO'
        ) ON OC.CODI_ETA = E.CODI_ETA
WHERE
    (AO.STAT_ACO IN ('I','F'))
    AND   E.INSU_ETA IN ('A', 'E', 'P', 'R', 'S')
    AND   (OC.ACAO_OCT IS NOT NULL)
    AND   (IO.APON_PRO = 'S')

UNION ALL

SELECT /* ORDEM DE PRODUÇAO - ETAPA DA QUALIDADE */
    UPPER('ORD_PROD#') || TO_CHAR(AO.CODI_ACO) AS NK_MOVIMENTO_ESTOQUE,
    CAST(O.CODI_EMP AS INTEGER) AS CODI_EMP,
    IO.CODI_PSV,
    'ZZ' AS ESTA_MUN,
    CAST(AO.DINI_ACO AS DATE) AS DATA_MOV,
    (
        CASE
            WHEN OC.ACAO_OCQ = 'S'
            THEN -1
            ELSE 1
        END
    ) * COALESCE(IO.QUTI_PRO, 0) AS QTDE
FROM
    IORDEM         IO
    LEFT JOIN   ACOMPANHAORDEM AO ON (AO.CODI_ACO = IO.CODI_ACO)
    LEFT JOIN   ORDEMPRODUCAO  O  ON (O.CODI_ORD  = AO.CODI_ORD)
    LEFT JOIN   PRODSERV       PS ON (PS.CODI_PSV = IO.CODI_PSV)
    LEFT JOIN   ETAPA          E  ON (E.CODI_ETA  = AO.CODI_ETA)
    LEFT JOIN
        (
            CTRLSALDO CS
            INNER JOIN ORDEMCTRLQ OC ON OC.CODI_CTR = CS.CODI_CTR AND
                CS.DESC_CTR = 'ESTOQUE FISICO'
        ) ON OC.CODI_ETA = E.CODI_ETA
WHERE
    (AO.STAT_ACO IN ('I','F'))
    AND   E.INSU_ETA  = 'Q'
    AND   OC.ACAO_OCQ IS NOT NULL
    AND   IO.APON_PRO = 'S'
    AND   ((O.QUAP_ORD = 'S' AND OC.APRO_OCQ = 'S') OR
    (O.QUAP_ORD = 'N' AND OC.APRO_OCQ = 'N'))
</sql>
