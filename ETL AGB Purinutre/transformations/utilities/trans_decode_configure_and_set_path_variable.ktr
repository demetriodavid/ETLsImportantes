    <definitions>
      <definition>
        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>import org.pentaho.di.core.database.Database;
import org.pentaho.di.core.util.StringUtil;
import org.pentaho.di.job.Job;
import org.pentaho.di.trans.step.*;
import org.pentaho.di.trans.Trans;
import java.util.concurrent.TimeUnit;
import java.util.List;
import java.util.Arrays;
import java.util.Date;
import java.lang.System;
import java.util.Calendar;
import java.util.GregorianCalendar;

import java.net.URISyntaxException;
import java.net.URI;
import java.nio.file.Paths;
import java.nio.file.Path;


public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    String strVariableValue, strVariableName;
    String strParamName = "parametro";
    String strParamValue = "valor";
    String strSolvedPath = "";
    
    Trans tTransformation = null;
    Job jParentJob = null;
    
	// Let's look up parameters only once for performance reason.
    //
    if (first) {
      first=false;
    }

    // First, get a row from the default input hop
    //
    Object[] r = getRow();

    // If the row object is null, we are done processing.
    //
    if (r == null) {
      setOutputDone();
      return false;
    }

    // It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    //
    Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());

    strVariableValue = get(Fields.In, strParamValue).getString(r);
    strVariableName = get(Fields.In, strParamName).getString(r);

    try
    {
        if(strVariableName.equals("ETL_PATH"))
        {
            Path p = Paths.get(new URI(strVariableValue));
    
            strSolvedPath = p.normalize().toString();
            logBasic("Path worked and result is:" + strSolvedPath);

            System.setProperty(strVariableName, strVariableValue);
            setVariable(strVariableName, strVariableValue);
            
            tTransformation = getTrans();
            tTransformation.setVariable(strVariableName, strVariableValue);
            while(tTransformation.getParentTrans() != null)
            {
                tTransformation = tTransformation.getParentTrans();
                tTransformation.setVariable(strVariableName, strVariableValue);
            }
        
            jParentJob = tTransformation.getParentJob();
            while(jParentJob != null)
            {
                jParentJob.setVariable(strVariableName, strVariableValue);
                jParentJob = jParentJob.getParentJob();
            }
        }
        else
        {
            strSolvedPath = null;
        }
    }
    catch(URISyntaxException e)
    {
        logBasic("Exception " + e.toString());
    }

    setOutputDone();
    return true;
}</class_source>
