  <step>
    <name>[DW] - Extração Fato Fluxo Caixa Projetado Dia</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>[DW] - PostgreSQL - Vistra BI</connection>
    <sql>WITH DESCONSIDERA_FILIAL AS(
SELECT VALOR
  FROM DIM_JUNK_PARAMETRO 
 WHERE PARAMETRO = 'FILIAIS_A_DESCONSIDERAR' 
),

A_PAGAR AS (

select 	SK_TEMPO_DIA,
		SK_CONTA_FLUXO_CAIXA,
		SK_EMPRESA,
		SUM(VALOR) VALOR

		
from 
(
		SELECT DTD.SK_TEMPO_DIA,
		       DCFC.SK_CONTA_FLUXO_CAIXA,
		       DE.SK_EMPRESA,
		       AVG(SMFTP.vlr_saldo)  AS VALOR 
		  FROM STAGE.STG_MOV_FIN_TITULO_PAGAR SMFTP
		       INNER JOIN DIM_TEMPO_DIA DTD          ON DTD.NK_TEMPO_DIA          = SMFTP.NK_TEMPO_DIA_VENC 
		       INNER JOIN DIM_EMPRESA DE             ON DE.NK_EMPRESA             = SMFTP.NK_EMPRESA 
		       INNER JOIN DIM_CONTA_FLUXO_CAIXA DCFC ON DCFC.NK_CONTA_FLUXO_CAIXA = SMFTP.NK_CONTA_FLUXO_CAIXA
		  WHERE DE.NK_EMPRESA NOT IN (SELECT VALOR FROM DESCONSIDERA_FILIAL)
		    AND SMFTP.NK_TIPO_DOCUMENTO &lt;&gt; '312'
		 GROUP BY DTD.SK_TEMPO_DIA,
		       DCFC.SK_CONTA_FLUXO_CAIXA,
		       DE.SK_EMPRESA,
		       smftp.dd_id_parcela 
		       
) X group by 	SK_TEMPO_DIA,
				SK_CONTA_FLUXO_CAIXA,
				SK_EMPRESA
 ),

 PROVISAO_PAGAR AS (
	select SK_TEMPO_DIA,
		SK_CONTA_FLUXO_CAIXA,
		SK_EMPRESA,
		SUM(VALOR) as VALOR 
from (
    SELECT DTD.SK_TEMPO_DIA,
       115    AS  SK_CONTA_FLUXO_CAIXA,
       DE.SK_EMPRESA,
       SUM(STG.VALOR_PARCELA_PENDENTE) AS VALOR
     FROM STAGE.STG_PEDIDO_COMPRA_PARCELAS STG
    INNER JOIN DIM_TEMPO_DIA DTD ON DTD.NK_TEMPO_DIA = STG.NK_VENCIMENTO
    INNER JOIN DIM_EMPRESA   DE  ON DE.NK_EMPRESA    = STG.NK_EMPRESA
    WHERE DE.NK_EMPRESA NOT IN (SELECT VALOR FROM DESCONSIDERA_FILIAL)
      AND STG.DD_SITUACAO = 'A'
 GROUP BY 1,2,3

union all

select 	SK_TEMPO_DIA,
		SK_CONTA_FLUXO_CAIXA,
		SK_EMPRESA,
		SUM(VALOR) VALOR

		
from 
(
		SELECT DTD.SK_TEMPO_DIA,
		       DCFC.SK_CONTA_FLUXO_CAIXA,
		       DE.SK_EMPRESA,
		       AVG(SMFTP.vlr_saldo)  AS VALOR 
		  FROM STAGE.STG_MOV_FIN_TITULO_PAGAR SMFTP
		       INNER JOIN DIM_TEMPO_DIA DTD          ON DTD.NK_TEMPO_DIA          = SMFTP.NK_TEMPO_DIA_VENC 
		       INNER JOIN DIM_EMPRESA DE             ON DE.NK_EMPRESA             = SMFTP.NK_EMPRESA 
		       INNER JOIN DIM_CONTA_FLUXO_CAIXA DCFC ON DCFC.NK_CONTA_FLUXO_CAIXA = SMFTP.NK_CONTA_FLUXO_CAIXA
		  WHERE DE.NK_EMPRESA NOT IN (SELECT VALOR FROM DESCONSIDERA_FILIAL)
		    AND SMFTP.NK_TIPO_DOCUMENTO = '312'
		 GROUP BY DTD.SK_TEMPO_DIA,
		       DCFC.SK_CONTA_FLUXO_CAIXA,
		       DE.SK_EMPRESA,
		       smftp.dd_id_parcela 
		       
) X group by 	SK_TEMPO_DIA,
				SK_CONTA_FLUXO_CAIXA,
				SK_EMPRESA
 ) X group by X.SK_TEMPO_DIA,
		X.SK_CONTA_FLUXO_CAIXA,
		X.SK_EMPRESA

 )

 ,A_RECEBER AS (
SELECT DTD.SK_TEMPO_DIA,
       DCFC.SK_CONTA_FLUXO_CAIXA,
       DE.SK_EMPRESA,
       SUM(VLR_SALDO) AS VALOR
  FROM STAGE.STG_MOV_FIN_TITULO_A_RECEBER SMFTR
       INNER JOIN DIM_TEMPO_DIA DTD          ON DTD.NK_TEMPO_DIA          = SMFTR.NK_TEMPO_DIA_VENC 
       INNER JOIN DIM_EMPRESA DE             ON DE.NK_EMPRESA             = SMFTR.NK_EMPRESA 
       INNER JOIN DIM_CONTA_FLUXO_CAIXA DCFC ON DCFC.NK_CONTA_FLUXO_CAIXA = SMFTR.NK_CONTA_FLUXO_CAIXA
  WHERE DE.NK_EMPRESA NOT IN (SELECT VALOR FROM DESCONSIDERA_FILIAL)
    AND SMFTR.TIPO &lt;&gt; 'P'

 GROUP BY 1,2,3       
)
 ,PROVISAO_RECEBER AS (


SELECT DTD.SK_TEMPO_DIA,
       DCFC.SK_CONTA_FLUXO_CAIXA,
       DE.SK_EMPRESA,
       SUM(VLR_PARCELA) AS VALOR
  FROM STAGE.STG_MOV_FIN_TITULO_RECEBER SMFTR
       INNER JOIN DIM_TEMPO_DIA DTD          ON DTD.NK_TEMPO_DIA          = SMFTR.NK_TEMPO_DIA_VENC 
       INNER JOIN DIM_EMPRESA DE             ON DE.NK_EMPRESA             = SMFTR.NK_EMPRESA 
       INNER JOIN DIM_CONTA_FLUXO_CAIXA DCFC ON DCFC.NK_CONTA_FLUXO_CAIXA = SMFTR.NK_CONTA_FLUXO_CAIXA
  WHERE DE.NK_EMPRESA NOT IN (SELECT VALOR FROM DESCONSIDERA_FILIAL)
    AND SMFTR.TIPO = 'P'

 GROUP BY 1,2,3       
)

SELECT X.SK_TEMPO_DIA,
       X.SK_CONTA_FLUXO_CAIXA, 
       X.SK_EMPRESA,
       SUM(VALOR_ENTRADA)    AS VALOR_ENTRADA,
       SUM(PROVISAO_ENTRADA) AS PROVISAO_ENTRADA, 
       SUM(VALOR_SAIDA)      AS VALOR_SAIDA,
       SUM(PROVISAO_SAIDA)   AS PROVISAO_SAIDA
  FROM (
			SELECT COALESCE(COALESCE(COALESCE(AP.SK_TEMPO_DIA, AR.SK_TEMPO_DIA, AR.SK_TEMPO_DIA, AR.SK_TEMPO_DIA), PP.SK_TEMPO_DIA, AR.SK_TEMPO_DIA), PR.SK_TEMPO_DIA, PR.SK_TEMPO_DIA)                 AS SK_TEMPO_DIA,
			       COALESCE(COALESCE(COALESCE(AP.SK_CONTA_FLUXO_CAIXA, AR.SK_CONTA_FLUXO_CAIXA), PP.SK_CONTA_FLUXO_CAIXA), PR.SK_CONTA_FLUXO_CAIXA) AS SK_CONTA_FLUXO_CAIXA,
			       COALESCE(COALESCE(COALESCE(AP.SK_EMPRESA, AR.SK_EMPRESA), PP.SK_EMPRESA), PR.SK_EMPRESA)                     AS SK_EMPRESA,
			       COALESCE(AR.VALOR, 0)                                      AS VALOR_ENTRADA,
			       COALESCE(PR.VALOR, 0)                                      AS PROVISAO_ENTRADA,
			       COALESCE(AP.VALOR, 0)                                      AS VALOR_SAIDA,   
			       COALESCE(PP.VALOR, 0)                                      AS PROVISAO_SAIDA
			  FROM A_PAGAR AP
			       FULL OUTER JOIN A_RECEBER AR ON AR.SK_TEMPO_DIA = AP.SK_TEMPO_DIA 
			                   AND AR.SK_EMPRESA = AP.SK_EMPRESA 
			                   AND AR.SK_CONTA_FLUXO_CAIXA = AP.SK_CONTA_FLUXO_CAIXA
			       FULL OUTER JOIN PROVISAO_PAGAR PP ON PP.SK_TEMPO_DIA = AP.SK_TEMPO_DIA 
			                   AND PP.SK_EMPRESA = AP.SK_EMPRESA 
			                   AND PP.SK_CONTA_FLUXO_CAIXA = AP.SK_CONTA_FLUXO_CAIXA
			       FULL OUTER JOIN PROVISAO_RECEBER PR ON PR.SK_TEMPO_DIA = AP.SK_TEMPO_DIA 
			                   AND PR.SK_EMPRESA = AP.SK_EMPRESA 
			                   AND PR.SK_CONTA_FLUXO_CAIXA = AP.SK_CONTA_FLUXO_CAIXA

)X
GROUP BY 1,2,3


</sql>
