    <connection>[DW] - PostgreSQL - Vistra BI</connection>
    <sql>WITH DESCONSIDERA_FILIAL AS(
SELECT VALOR
  FROM DIM_JUNK_PARAMETRO 
 WHERE PARAMETRO = 'FILIAIS_A_DESCONSIDERAR' 
),
SEPARAR_ENTRADA_SAIDA AS(
SELECT DTD.SK_TEMPO_DIA,
       DCFC.SK_CONTA_FLUXO_CAIXA,
       DE.SK_EMPRESA,
	   DCC.SK_CENTRO_CUSTO,
       CASE WHEN SFCW.NK_ENTRADA_SAIDA = 'E' THEN COALESCE(SUM(SFCW.VALOR),0) END AS VALOR_ENTRADA,
       CASE WHEN SFCW.NK_ENTRADA_SAIDA = 'S' THEN COALESCE(SUM(SFCW.VALOR),0) END AS VALOR_SAIDA
  FROM STAGE.STG_FLUXO_CAIXA_REALIZADO SFCW
       INNER JOIN DIM_TEMPO_DIA         DTD  ON DTD.NK_TEMPO_DIA                   = SFCW.NK_TEMPO_DIA_VENC
       INNER JOIN DIM_EMPRESA           DE   ON DE.NK_EMPRESA::INTEGER             = SFCW.NK_EMPRESA
       INNER JOIN DIM_CONTA_FLUXO_CAIXA DCFC ON DCFC.NK_CONTA_FLUXO_CAIXA::INTEGER = SFCW.NK_CONTA_FLUXO_CAIXA
       INNER JOIN DIM_CENTRO_CUSTO      DCC  ON DCC.NK_CENTRO_CUSTO             = SFCW.NK_CENTRO_CUSTO
WHERE DE.NK_EMPRESA NOT IN (SELECT VALOR FROM DESCONSIDERA_FILIAL)

 GROUP BY SFCW.NK_ENTRADA_SAIDA, DCC.SK_CENTRO_CUSTO, DE.SK_EMPRESA, DCFC.SK_CONTA_FLUXO_CAIXA, DTD.SK_TEMPO_DIA
 ) 

SELECT SES.SK_TEMPO_DIA,
       SES.SK_CONTA_FLUXO_CAIXA,
       SES.SK_EMPRESA,
       COALESCE(SUM(SES.VALOR_ENTRADA),0) VALOR_ENTRADA,
       COALESCE(SUM(SES.VALOR_SAIDA),0) VALOR_SAIDA
  FROM SEPARAR_ENTRADA_SAIDA SES
 GROUP BY SES.SK_EMPRESA, SES.SK_CONTA_FLUXO_CAIXA, SES.SK_TEMPO_DIA

