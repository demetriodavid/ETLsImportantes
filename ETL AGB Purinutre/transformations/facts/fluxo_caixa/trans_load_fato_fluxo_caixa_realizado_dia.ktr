       FULL OUTER JOIN A_RECEBER AR ON AR.SK_TEMPO_DIA = AP.SK_TEMPO_DIA AND AR.SK_EMPRESA = AP.SK_EMPRESA AND AR.SK_CONTA_FLUXO_CAIXA = AP.SK_CONTA_FLUXO_CAIXA
 GROUP BY 1,2,3
),
MIN_MAX AS (
SELECT MAX(DTD.NK_TEMPO_DIA) AS MAX,
       MIN(DTD.NK_TEMPO_DIA) AS MIN
  FROM FLUXO
       INNER JOIN DIM_TEMPO_DIA DTD USING(SK_TEMPO_DIA)
),
FLUXO_SALDO AS (
