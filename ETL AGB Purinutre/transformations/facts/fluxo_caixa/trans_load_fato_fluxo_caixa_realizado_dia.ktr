       COALESCE(AP.SK_CONTA_FLUXO_CAIXA, AR.SK_CONTA_FLUXO_CAIXA) AS SK_CONTA_FLUXO_CAIXA,
       COALESCE(AP.SK_EMPRESA, AR.SK_EMPRESA)                     AS SK_EMPRESA,
       COALESCE(SUM(AR.VALOR), 0)                                 AS VALOR_ENTRADA,
