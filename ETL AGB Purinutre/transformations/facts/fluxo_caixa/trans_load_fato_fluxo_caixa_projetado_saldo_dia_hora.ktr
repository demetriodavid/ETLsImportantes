    <connection>[DW] - PostgreSQL - Vistra BI</connection>
    <sql>WITH A_PAGAR AS (
SELECT DTD.SK_TEMPO_DIA,
       DCFC.SK_CONTA_FLUXO_CAIXA,
       DE.SK_EMPRESA,
       SUM(VLR_CALCULADO)  AS VALOR 
  FROM STAGE.STG_MOV_FIN_TITULO_PAGAR SMFTP
       INNER JOIN DIM_TEMPO_DIA DTD          ON DTD.NK_TEMPO_DIA          = SMFTP.NK_TEMPO_DIA_VENC 
       INNER JOIN DIM_EMPRESA DE             ON DE.NK_EMPRESA             = SMFTP.NK_EMPRESA 
       INNER JOIN DIM_CONTA_FLUXO_CAIXA DCFC ON DCFC.NK_CONTA_FLUXO_CAIXA = SMFTP.NK_CONTA_FLUXO_CAIXA
 GROUP BY 1,2,3
),

A_RECEBER AS (
SELECT DTD.SK_TEMPO_DIA,
       DCFC.SK_CONTA_FLUXO_CAIXA,
       DE.SK_EMPRESA,
       SUM(VLR_PARCELA) AS VALOR
  FROM STAGE.STG_MOV_FIN_TITULO_RECEBER SMFTR
       INNER JOIN DIM_TEMPO_DIA DTD          ON DTD.NK_TEMPO_DIA          = SMFTR.NK_TEMPO_DIA_VENC 
       INNER JOIN DIM_EMPRESA DE             ON DE.NK_EMPRESA             = SMFTR.NK_EMPRESA 
       INNER JOIN DIM_CONTA_FLUXO_CAIXA DCFC ON DCFC.NK_CONTA_FLUXO_CAIXA = SMFTR.NK_CONTA_FLUXO_CAIXA
 GROUP BY 1,2,3	   
),

FLUXO AS (
SELECT COALESCE(AP.SK_TEMPO_DIA, AR.SK_TEMPO_DIA)                 AS SK_TEMPO_DIA,
       COALESCE(AP.SK_CONTA_FLUXO_CAIXA, AR.SK_CONTA_FLUXO_CAIXA) AS SK_CONTA_FLUXO_CAIXA,
       COALESCE(AP.SK_EMPRESA, AR.SK_EMPRESA)                     AS SK_EMPRESA,
       COALESCE(AR.VALOR,0)                                       AS VALOR_ENTRADA,
       COALESCE(AP.VALOR,0)                                       AS VALOR_SAIDA       
  FROM A_PAGAR AP
       FULL OUTER JOIN A_RECEBER AR ON AR.SK_TEMPO_DIA = AP.SK_TEMPO_DIA AND AR.SK_EMPRESA = AP.SK_EMPRESA AND AR.SK_CONTA_FLUXO_CAIXA = AP.SK_CONTA_FLUXO_CAIXA
),
MIN_MAX AS (
SELECT MAX(DTD.NK_TEMPO_DIA) AS MAX,
       MIN(DTD.NK_TEMPO_DIA) AS MIN
  FROM FLUXO
       INNER JOIN DIM_TEMPO_DIA DTD USING(SK_TEMPO_DIA)
),
FLUXO_SALDO AS (
SELECT SK_TEMPO_DIA,
       SK_CONTA_FLUXO_CAIXA,
       SK_EMPRESA,
       SUM(VALOR_ENTRADA - VALOR_SAIDA) AS SALDO
  FROM FLUXO
 GROUP BY 
       SK_TEMPO_DIA,
       SK_CONTA_FLUXO_CAIXA,
       SK_EMPRESA
 UNION ALL
SELECT DTD.SK_TEMPO_DIA,
       DCFC.SK_CONTA_FLUXO_CAIXA,
       DE.SK_EMPRESA,
       0 AS SALDO
  FROM DIM_TEMPO_DIA DTD, DIM_EMPRESA DE, DIM_CONTA_FLUXO_CAIXA DCFC
  WHERE DTD.NK_TEMPO_DIA BETWEEN (SELECT MIN FROM MIN_MAX) AND (SELECT MAX FROM MIN_MAX)
),
SALDO_ACUMULADO AS(
SELECT FATO.SK_TEMPO_DIA,
       DTD.NK_TEMPO_DIA,
       FATO.SK_CONTA_FLUXO_CAIXA,
       FATO.SK_EMPRESA,
       SUM(SALDO) OVER(PARTITION BY FATO.SK_EMPRESA, FATO.SK_CONTA_FLUXO_CAIXA ORDER BY DTD.NK_TEMPO_DIA, FATO.SK_EMPRESA, FATO.SK_CONTA_FLUXO_CAIXA) AS SALDO_FINAL
  FROM FLUXO_SALDO FATO
       INNER JOIN DIM_TEMPO_DIA DTD USING(SK_TEMPO_DIA)
 GROUP BY 
       SK_TEMPO_DIA,
       SK_CONTA_FLUXO_CAIXA,
       SK_EMPRESA,
       SALDO,
       DTD.NK_TEMPO_DIA
)

SELECT FATO.SK_TEMPO_DIA,
       FATO.SK_CONTA_FLUXO_CAIXA,
       FATO.SK_EMPRESA,
       LAG(FATO.SALDO_FINAL) OVER(PARTITION BY FATO.SK_EMPRESA, FATO.SK_CONTA_FLUXO_CAIXA ORDER BY NK_TEMPO_DIA) AS SALDO_INICIAL,
       FATO.SALDO_FINAL
       
  FROM SALDO_ACUMULADO FATO
 GROUP BY 
       SK_TEMPO_DIA,
       SK_CONTA_FLUXO_CAIXA,
       SK_EMPRESA,
       FATO.SALDO_FINAL,
       NK_TEMPO_DIA 




