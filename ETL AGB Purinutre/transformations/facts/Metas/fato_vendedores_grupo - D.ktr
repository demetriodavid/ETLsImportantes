    <sql>WITH FATURAMENTO_META AS(
SELECT CAST(DTD.ANO AS INTEGER) AS ANO,
       DG.NK_GRUPO,
       DV.NK_VENDEDOR,
       CASE FMVD.DD_NF_SERIE WHEN 'DZ' THEN 
           SUM(FMVD.VLR_TOTAL_NOTA)
       ELSE
           SUM(FMVD.QTD_ITEM * FMVD.VLR_LIQ_UNIT)
       END AS FATURAMENTO,
       0                                      AS META,
       0                                      AS PURIPOINTS
  FROM FATO_MOVIMENTO_VENDA_DIA FMVD
       INNER JOIN DIM_TEMPO_DIA    DTD ON FMVD.SK_TEMPO_DIA = DTD.SK_TEMPO_DIA
       INNER JOIN DIM_VENDEDOR     DV  ON FMVD.SK_VENDEDOR  = DV.SK_VENDEDOR
       INNER JOIN DIM_PRODUTO      DP  ON FMVD.SK_PRODUTO   = DP.SK_PRODUTO
       INNER JOIN DIM_GRUPO        DG  ON DP.NK_GRUPO       = DG.NK_GRUPO
       INNER JOIN DIM_EMPRESA      DE  ON DE.SK_EMPRESA     = FMVD.SK_EMPRESA 
       INNER JOIN DIM_TABELA_PRECO DTP ON DTP.SK_TABELA_PRECO = FMVD.SK_TABELA_PRECO
  WHERE FMVD.TIPO = '112'
 GROUP BY 1,2,3, FMVD.DD_NF_SERIE
UNION ALL

SELECT CAST(FATO.ANO AS INTEGER) AS ANO, 
        DG.NK_GRUPO, 
        DV.NK_VENDEDOR,
        0                 AS FATURAMENTO,
        SUM(FATO.COTA)    AS METAS,
        SUM(FATO.PONTOS)  AS PURIPOINTS
   FROM FATO_METAS_VENDAS_GRUPO FATO
  INNER JOIN DIM_GRUPO    DG ON FATO.NK_GRUPO    = DG.NK_GRUPO
  INNER JOIN DIM_VENDEDOR DV ON FATO.SK_VENDEDOR = DV.SK_VENDEDOR 
  GROUP BY 1,2,3

UNION ALL 
   SELECT CAST(DTD.ANO AS INTEGER) AS ANO,
       DG.NK_GRUPO,
       '10000060'            AS NK_VENDEDOR,
       CASE FMVD.DD_NF_SERIE WHEN 'DZ' THEN 
           SUM(FMVD.VLR_TOTAL_NOTA)
       ELSE
           SUM(FMVD.QTD_ITEM * FMVD.VLR_LIQ_UNIT)
       END AS FATURAMENTO,
       0                                      AS META,
       0                                      AS PURIPOINTS
   FROM FATO_MOVIMENTO_VENDA_DIA FMVD
       INNER JOIN DIM_TEMPO_DIA    DTD ON FMVD.SK_TEMPO_DIA = DTD.SK_TEMPO_DIA
       INNER JOIN DIM_VENDEDOR     DV  ON DV.SK_VENDEDOR    = FMVD.SK_VENDEDOR 
       INNER JOIN DIM_PRODUTO      DP  ON FMVD.SK_PRODUTO   = DP.SK_PRODUTO
       INNER JOIN DIM_GRUPO        DG  ON DG.NK_GRUPO       = DP.NK_GRUPO
       INNER JOIN DIM_EMPRESA      DE  ON DE.SK_EMPRESA     = FMVD.SK_EMPRESA 
       INNER JOIN DIM_TABELA_PRECO DTP ON DTP.SK_TABELA_PRECO = FMVD.SK_TABELA_PRECO 
       WHERE DG.COD_GRUPO IN ('17','18','19') AND DP.NK_SUBGRUPO IN ('62','64','68','70','20000116','20000118') 
         AND DE.COD_FILIAL IN ('1','2','3','4','5','6','7','8')
         AND FMVD.TIPO = '112'
 GROUP BY 1,2,3,FMVD.DD_NF_SERIE
  
  UNION ALL 

   SELECT CAST(DTD.ANO AS INTEGER) AS ANO,
       DG.NK_GRUPO,
       '10000241'             AS NK_VENDEDOR,
       CASE FMVD.DD_NF_SERIE WHEN 'DZ' THEN 
           SUM(FMVD.VLR_TOTAL_NOTA)
       ELSE
           SUM(FMVD.QTD_ITEM * FMVD.VLR_LIQ_UNIT)
       END AS FATURAMENTO,
       0                                      AS META,
       0                                      AS PURIPOINTS
   FROM FATO_MOVIMENTO_VENDA_DIA FMVD
       INNER JOIN DIM_TEMPO_DIA    DTD ON FMVD.SK_TEMPO_DIA = DTD.SK_TEMPO_DIA
       INNER JOIN DIM_VENDEDOR     DV  ON FMVD.SK_VENDEDOR  = DV.SK_VENDEDOR
       INNER JOIN DIM_PRODUTO      DP  ON FMVD.SK_PRODUTO   = DP.SK_PRODUTO
       INNER JOIN DIM_GRUPO        DG  ON DG.NK_GRUPO       = DP.NK_GRUPO
       INNER JOIN DIM_EMPRESA      DE  ON DE.SK_EMPRESA     = FMVD.SK_EMPRESA 
       INNER JOIN DIM_TABELA_PRECO DTP ON DTP.SK_TABELA_PRECO = FMVD.SK_TABELA_PRECO 
       WHERE DG.COD_GRUPO IN ('6','7') AND DP.NK_SUBGRUPO IN ( '34','76','10000106','10000107','10000142','38','39','40','41','42','77','78','10000139','10000140','10000141') 
         AND DE.COD_FILIAL IN ('1','2','3','4','5','6','7','8')
         AND FMVD.TIPO = '112'
  
 GROUP BY 1,2,3,FMVD.DD_NF_SERIE
  
 UNION ALL

   SELECT CAST(DTD.ANO AS INTEGER) AS ANO,
       DG.NK_GRUPO,
       '50000369'            AS NK_VENDEDOR,
       CASE FMVD.DD_NF_SERIE WHEN 'DZ' THEN 
           SUM(FMVD.VLR_TOTAL_NOTA)
       ELSE
           SUM(FMVD.QTD_ITEM * FMVD.VLR_LIQ_UNIT)
       END AS FATURAMENTO,
       0                                      AS META,
       0                                      AS PURIPOINTS
FROM FATO_MOVIMENTO_VENDA_DIA FMVD
       INNER JOIN DIM_TEMPO_DIA    DTD ON FMVD.SK_TEMPO_DIA = DTD.SK_TEMPO_DIA
       INNER JOIN DIM_VENDEDOR     DV  ON FMVD.SK_VENDEDOR  = DV.SK_VENDEDOR
       INNER JOIN DIM_PRODUTO      DP  ON FMVD.SK_PRODUTO   = DP.SK_PRODUTO
       INNER JOIN DIM_GRUPO        DG  ON DG.NK_GRUPO       = DP.NK_GRUPO
       INNER JOIN DIM_EMPRESA      DE  ON DE.SK_EMPRESA     = FMVD.SK_EMPRESA 
       INNER JOIN DIM_TABELA_PRECO DTP ON DTP.SK_TABELA_PRECO = FMVD.SK_TABELA_PRECO 
       WHERE DE.COD_FILIAL IN ('1','2','3','4','5','6','7','8')
        AND DG.COD_GRUPO IN('1','2')
        AND FMVD.TIPO = '112'
         --AND DTD.ANO = EXTRACT(YEAR FROM CURRENT_DATE) 
 GROUP BY 1,2,3,FMVD.DD_NF_SERIE
  
 UNION ALL

   SELECT CAST(DTD.ANO AS INTEGER) AS ANO,
       DG.NK_GRUPO,
       '10000363'              AS NK_VENDEDOR,
       CASE FMVD.DD_NF_SERIE WHEN 'DZ' THEN 
           SUM(FMVD.VLR_TOTAL_NOTA)
       ELSE
           SUM(FMVD.QTD_ITEM * FMVD.VLR_LIQ_UNIT)
       END AS FATURAMENTO,
       0                                      AS META,
       0                                      AS PURIPOINTS
  FROM FATO_MOVIMENTO_VENDA_DIA FMVD
       INNER JOIN DIM_TEMPO_DIA    DTD ON FMVD.SK_TEMPO_DIA = DTD.SK_TEMPO_DIA
       INNER JOIN DIM_VENDEDOR     DV  ON FMVD.SK_VENDEDOR  = DV.SK_VENDEDOR
       INNER JOIN DIM_PRODUTO      DP  ON FMVD.SK_PRODUTO   = DP.SK_PRODUTO
       INNER JOIN DIM_GRUPO        DG  ON DP.NK_GRUPO       = DG.NK_GRUPO
       INNER JOIN DIM_EMPRESA      DE  ON DE.SK_EMPRESA     = FMVD.SK_EMPRESA 
       INNER JOIN DIM_TABELA_PRECO DTP ON DTP.SK_TABELA_PRECO = FMVD.SK_TABELA_PRECO 
