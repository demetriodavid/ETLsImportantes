    <sql>WITH FATURAMENTO_META AS(
SELECT DTD.ANO,
       DTD.MES,
       DG.GRUPO,
       DS.SUBGRUPO,
       DTD.MES_ABREVIADO                      AS MES_ABREVIADO,
       DTD.MES_EXTENSO                        AS MES_EXTENSO,
       DV.SK_VENDEDOR                         AS SK_VENDEDOR,
       DV.VENDEDOR,
       CASE FMVD.DD_NF_SERIE WHEN 'DZ' THEN 
           SUM(FMVD.VLR_TOTAL_NOTA)
       ELSE
           SUM(FMVD.QTD_ITEM * FMVD.VLR_LIQ_UNIT)
       END AS FATURAMENTO,
       0                                      AS META,
       0                                      AS PURIPOINTS
  FROM FATO_MOVIMENTO_VENDA_DIA FMVD
       INNER JOIN DIM_TEMPO_DIA DTD ON FMVD.SK_TEMPO_DIA = DTD.SK_TEMPO_DIA
       INNER JOIN DIM_VENDEDOR   DV ON FMVD.SK_VENDEDOR  = DV.SK_VENDEDOR
       INNER JOIN DIM_PRODUTO    DP ON FMVD.SK_PRODUTO   = DP.SK_PRODUTO 
       INNER JOIN DIM_GRUPO      DG ON DG.NK_GRUPO       = DP.NK_GRUPO 
       INNER JOIN DIM_SUBGRUPO   DS ON DS.NK_SUBGRUPO    = DP.NK_SUBGRUPO
  WHERE FMVD.TIPO = '112'
 GROUP BY 1,2,3,4,5,6,7,8,FMVD.DD_NF_SERIE

UNION ALL

SELECT DTD.ANO,
       DTD.MES,
       DG.GRUPO,
       DS.SUBGRUPO,
       DTD.MES_ABREVIADO                      AS MES_ABREVIADO,
       DTD.MES_EXTENSO                        AS MES_EXTENSO,
       1372                                   AS SK_VENDEDOR,
       'VENDEDOR NAO INFORMADO'               AS VENDEDOR,
       0                                      AS FATURAMENTO,
       0                                      AS META,
       0                                      AS PURIPOINTS
  FROM FATO_MOVIMENTO_VENDA_DIA FMVD
       INNER JOIN DIM_TEMPO_DIA DTD ON FMVD.SK_TEMPO_DIA = DTD.SK_TEMPO_DIA
       INNER JOIN DIM_VENDEDOR   DV ON FMVD.SK_VENDEDOR  = DV.SK_VENDEDOR
       INNER JOIN DIM_PRODUTO    DP ON FMVD.SK_PRODUTO   = DP.SK_PRODUTO 
       INNER JOIN DIM_GRUPO      DG ON DG.NK_GRUPO       = DP.NK_GRUPO 
       INNER JOIN DIM_SUBGRUPO   DS ON DS.NK_SUBGRUPO    = DP.NK_SUBGRUPO
  WHERE FMVD.TIPO = '112'
 GROUP BY 1,2,3,4,5,6,7,8,FMVD.DD_NF_SERIE

UNION ALL

SELECT FATO.ANO,
       1    AS MES,
       '-1'    AS GRUPO,
       SUB.SUBGRUPO, 
       ''                      AS MES_ABREVIADO,
       ''                      AS MES_EXTENSO,
       VE.SK_VENDEDOR          AS SK_VENDEDOR,
       VE.VENDEDOR             AS VENDEDOR,
       0                       AS FATURAMENTO,
       SUM(FATO.VALOR)         AS META,
       SUM(FATO.PURIPONTS)     AS PURIPOINTS
FROM purinutre_vendedor_metas_SUBGRUPO FATO
INNER JOIN DIM_SUBGRUPO SUB ON FATO.SK_SUBGRUPO = SUB.SK_SUBGRUPO
INNER JOIN DIM_VENDEDOR VE  USING(SK_VENDEDOR)
GROUP BY 1, 2, 3, 4, 5,6,7

UNION ALL 
-- FILIAL 
  SELECT      
        DTD.ANO,
       DTD.MES,
       DG.GRUPO,
       DS.SUBGRUPO,
       DTD.MES_ABREVIADO                      AS MES_ABREVIADO,
       DTD.MES_EXTENSO                        AS MES_EXTENSO,
       VEAT.SK_VENDEDOR                       AS SK_VENDEDOR,
       VEAT.VENDEDOR                          AS VENDEDOR,
       CASE FMVD.DD_NF_SERIE WHEN 'DZ' THEN 
           SUM(FMVD.VLR_TOTAL_NOTA)
       ELSE
           SUM(FMVD.QTD_ITEM * FMVD.VLR_LIQ_UNIT)
       END AS FATURAMENTO,
       0                                      AS META,
       0                                      AS PURIPOINTS
  FROM FATO_MOVIMENTO_VENDA_DIA FMVD
       INNER JOIN DIM_TEMPO_DIA    DTD ON FMVD.SK_TEMPO_DIA = DTD.SK_TEMPO_DIA
       INNER JOIN DIM_VENDEDOR     DV  ON DV.SK_VENDEDOR    = FMVD.SK_VENDEDOR 
       INNER JOIN DIM_PRODUTO      DP  ON FMVD.SK_PRODUTO   = DP.SK_PRODUTO
       INNER JOIN DIM_GRUPO        DG  ON DG.NK_GRUPO       = DP.NK_GRUPO
       INNER JOIN DIM_SUBGRUPO     DS  ON DS.NK_SUBGRUPO    = DP.NK_SUBGRUPO
       INNER JOIN DIM_EMPRESA      DE  ON DE.SK_EMPRESA     = FMVD.SK_EMPRESA 
       INNER JOIN DIM_TABELA_PRECO DTP ON DTP.SK_TABELA_PRECO = FMVD.SK_TABELA_PRECO 
       INNER JOIN AUX_FILIAL_VENDEDOR_AT FIL ON CAST(FIL.COD_FILIAIS AS INTEGER) = DE.COD_FILIAL 
       INNER JOIN DIM_VENDEDOR VEAT ON VEAT.NK_VENDEDOR = FIL.NK_VENDEDOR 
       WHERE FMVD.TIPO = '112'
         AND (SELECT COUNT(NK_GRUPOS) FROM aux_GRUPO_vendedor_at ATC WHERE ATC.NK_VENDEDOR = VEAT.NK_VENDEDOR) = 0
         AND (SELECT COUNT(NK_SUBGRUPOS) FROM AUX_SUBGRUPO_VENDEDOR_AT ATC WHERE ATC.NK_VENDEDOR = VEAT.NK_VENDEDOR) = 0
         AND DTD.ANO = 2024 
         --AND VEAT.NK_VENDEDOR = '25'
 GROUP BY 1,2,3,4,5,6,7,FMVD.DD_NF_SERIE
 
 UNION ALL
 
 -- FILIAL GRUPO 
   SELECT      
           DTD.ANO,
	       DTD.MES,
	       DG.GRUPO,
	       DS.SUBGRUPO,
	       DTD.MES_ABREVIADO                      AS MES_ABREVIADO,
	       DTD.MES_EXTENSO                        AS MES_EXTENSO,
	       VEAT.SK_VENDEDOR                       AS SK_VENDEDOR,
	       VEAT.VENDEDOR                          AS VENDEDOR,
	       CASE FMVD.DD_NF_SERIE WHEN 'DZ' THEN 
	           SUM(FMVD.VLR_TOTAL_NOTA)
	       ELSE
	           SUM(FMVD.QTD_ITEM * FMVD.VLR_LIQ_UNIT)
	       END AS FATURAMENTO,
	       0                                      AS META,
	       0                                      AS PURIPOINTS
  FROM FATO_MOVIMENTO_VENDA_DIA FMVD
       INNER JOIN DIM_TEMPO_DIA    DTD ON FMVD.SK_TEMPO_DIA = DTD.SK_TEMPO_DIA
       INNER JOIN DIM_VENDEDOR     DV  ON DV.SK_VENDEDOR    = FMVD.SK_VENDEDOR 
       INNER JOIN DIM_PRODUTO      DP  ON FMVD.SK_PRODUTO   = DP.SK_PRODUTO
       INNER JOIN DIM_GRUPO        DG  ON DG.NK_GRUPO       = DP.NK_GRUPO
       INNER JOIN DIM_SUBGRUPO     DS  ON DS.NK_SUBGRUPO    = DP.NK_SUBGRUPO
       INNER JOIN DIM_EMPRESA      DE  ON DE.SK_EMPRESA     = FMVD.SK_EMPRESA 
       INNER JOIN DIM_TABELA_PRECO DTP ON DTP.SK_TABELA_PRECO = FMVD.SK_TABELA_PRECO 
       INNER JOIN AUX_FILIAL_VENDEDOR_AT FIL ON CAST(FIL.COD_FILIAIS AS INTEGER) = DE.COD_FILIAL 
       INNER JOIN DIM_VENDEDOR VEAT ON VEAT.NK_VENDEDOR = FIL.NK_VENDEDOR 
       INNER JOIN AUX_GRUPO_VENDEDOR_AT GAT ON GAT.NK_VENDEDOR = VEAT.NK_VENDEDOR 
                                            AND GAT.NK_GRUPOS  = DG.NK_GRUPO
       WHERE FMVD.TIPO = '112'
         AND ( SELECT COUNT(NK_SUBGRUPOS) FROM AUX_SUBGRUPO_VENDEDOR_AT ATC WHERE ATC.NK_VENDEDOR = VEAT.NK_VENDEDOR ) = 0
         AND DTD.ANO = 2024 
 GROUP BY 1,2,3,4,5,6,7,FMVD.DD_NF_SERIE
 
 UNION ALL
 
 -- FILIAL GRUPO SUBGRUPO 
   SELECT      
        DTD.ANO,
       DTD.MES,
       DG.GRUPO,
       DS.SUBGRUPO,
       DTD.MES_ABREVIADO                      AS MES_ABREVIADO,
       DTD.MES_EXTENSO                        AS MES_EXTENSO,
       VEAT.SK_VENDEDOR                       AS SK_VENDEDOR,
       VEAT.VENDEDOR                          AS VENDEDOR,
       CASE FMVD.DD_NF_SERIE WHEN 'DZ' THEN 
           SUM(FMVD.VLR_TOTAL_NOTA)
       ELSE
           SUM(FMVD.QTD_ITEM * FMVD.VLR_LIQ_UNIT)
       END AS FATURAMENTO,
       0                                      AS META,
       0                                      AS PURIPOINTS
  FROM FATO_MOVIMENTO_VENDA_DIA FMVD
       INNER JOIN DIM_TEMPO_DIA    DTD ON FMVD.SK_TEMPO_DIA = DTD.SK_TEMPO_DIA
       INNER JOIN DIM_VENDEDOR     DV  ON DV.SK_VENDEDOR    = FMVD.SK_VENDEDOR 
       INNER JOIN DIM_PRODUTO      DP  ON FMVD.SK_PRODUTO   = DP.SK_PRODUTO
       INNER JOIN DIM_GRUPO        DG  ON DG.NK_GRUPO       = DP.NK_GRUPO
       INNER JOIN DIM_SUBGRUPO     DS  ON DS.NK_SUBGRUPO    = DP.NK_SUBGRUPO
       INNER JOIN DIM_EMPRESA      DE  ON DE.SK_EMPRESA     = FMVD.SK_EMPRESA 
       INNER JOIN DIM_TABELA_PRECO DTP ON DTP.SK_TABELA_PRECO = FMVD.SK_TABELA_PRECO 
       INNER JOIN AUX_FILIAL_VENDEDOR_AT FIL ON CAST(FIL.COD_FILIAIS AS INTEGER) = DE.COD_FILIAL 
