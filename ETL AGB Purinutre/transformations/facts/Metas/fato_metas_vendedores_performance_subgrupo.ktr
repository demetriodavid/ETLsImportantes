       SK_VENDEDOR,
       VENDEDOR,
       MES_EXTENSO,
       SUM(FATURAMENTO) AS FATURAMENTO,
       SUM(META)        AS META,
       SUM(PURIPOINTS)  AS PURIPOINTS
  FROM FATURAMENTO_META FM
 WHERE ANO = EXTRACT(YEAR FROM CURRENT_DATE) 
 --AND VENDEDOR LIKE '%OLIMP%'
--WHERE ANO = 2022
--  AND MES = 1
GROUP BY 1,2,3,4,5,6,7,8,9
--HAVING SUM(META) &lt;&gt; 0 
ORDER BY 1,2

