    <sql> WITH VENDEDOR AS(
					SELECT NK_VENDEDOR,
					       ''  AS COD_FILIAL,
					       NK_GRUPOS AS NK_GRUPO,
					       '' AS NK_SUBGRUPOS
					FROM aux_GRUPO_vendedor_at
					
					UNION ALL
					
					SELECT NK_VENDEDOR,
					       '' AS COD_FILIAL,
					       '' AS NK_GRUPO,
					       NK_SUBGRUPOS AS NK_SUBGRUPOS 
					  FROM aux_SUBGRUPO_vendedor_at
					
					UNION ALL
					
					SELECT NK_VENDEDOR,
					       COD_FILIAIS AS COD_FILIAL,
					       '' AS NK_GRUPO,
					       '' AS NK_SUBGRUPOS
					 FROM AUX_FILIAL_VENDEDOR_AT
) 



SELECT X.ANO,
       X.MES,
       X.GRUPO,
       X.SUBGRUPO,
       CASE
            WHEN X.MES BETWEEN 1 AND 3 THEN 'Q1'
            WHEN X.MES BETWEEN 4 AND 6 THEN 'Q2'
            WHEN X.MES BETWEEN 7 AND 9 THEN 'Q3'
            ELSE 'Q4'
       END             AS TRIMESTRE,
       X.MES_ABREVIADO,
       X.SK_VENDEDOR,
       X.VENDEDOR,
       X.MES_EXTENSO,
       SUM(X.FATURAMENTO) AS FATURAMENTO,
       SUM(X.META)        AS META,
       SUM(X.PURIPOINTS)  AS PURIPOINTS
       --SUM(X.TRIMESTRE) AS PURIPOINTS_TRIMESTRE
       FROM (
			SELECT DTD.ANO,
				   DTD.MES,
				   DG.GRUPO,
				   DS.SUBGRUPO,
				   DTD.MES_ABREVIADO                      AS MES_ABREVIADO,
				   DTD.MES_EXTENSO                        AS MES_EXTENSO,
				   DV.SK_VENDEDOR                         AS SK_VENDEDOR,
				   DV.VENDEDOR,
				   CASE FMVD.DD_NF_SERIE WHEN 'DZ' THEN 
				       SUM(FMVD.VLR_TOTAL_NOTA)
				   ELSE
				       SUM(FMVD.QTD_ITEM * FMVD.VLR_LIQ_UNIT)
				   END AS FATURAMENTO,
				   0                                      AS META,
				   0                                      AS PURIPOINTS
			  FROM FATO_MOVIMENTO_VENDA_DIA FMVD
			 INNER JOIN DIM_TEMPO_DIA DTD ON FMVD.SK_TEMPO_DIA = DTD.SK_TEMPO_DIA
			 INNER JOIN DIM_VENDEDOR   DV ON FMVD.SK_VENDEDOR  = DV.SK_VENDEDOR
			 INNER JOIN DIM_PRODUTO    DP ON FMVD.SK_PRODUTO   = DP.SK_PRODUTO 
			 INNER JOIN DIM_GRUPO      DG ON DG.NK_GRUPO       = DP.NK_GRUPO 
			 INNER JOIN DIM_SUBGRUPO   DS ON DS.NK_SUBGRUPO    = DP.NK_SUBGRUPO
			 WHERE FMVD.TIPO = '112'
			   AND DTD.ANO = EXTRACT(YEAR FROM CURRENT_DATE)
			GROUP BY 1,2,3,4,5,6,7,8,FMVD.DD_NF_SERIE

			 
			 UNION ALL 
			 
			  SELECT FATO.ANO,
  			         1    AS MES,
			         '-1'    AS GRUPO,
			         SUB.SUBGRUPO, 
			         ''                      AS MES_ABREVIADO,
			         ''                      AS MES_EXTENSO,
			         VE.SK_VENDEDOR          AS SK_VENDEDOR,
			         VE.VENDEDOR             AS VENDEDOR,
			         0                       AS FATURAMENTO,
			         SUM(FATO.VALOR)         AS META,
			         SUM(FATO.PURIPONTS)     AS PURIPOINTS
			FROM purinutre_vendedor_metas_SUBGRUPO FATO
			INNER JOIN DIM_SUBGRUPO SUB ON FATO.SK_SUBGRUPO = SUB.SK_SUBGRUPO
			INNER JOIN DIM_VENDEDOR VE  USING(SK_VENDEDOR)
			WHERE FATO.ANO = EXTRACT(YEAR FROM CURRENT_DATE)
			GROUP BY 1, 2, 3, 4, 5,6,7    
			 
			  UNION ALL
			  
			  SELECT DTD.ANO,
			         DTD.MES,
			         DG.GRUPO,
			         DS.SUBGRUPO,
			         DTD.MES_ABREVIADO                      AS MES_ABREVIADO,
			         DTD.MES_EXTENSO                        AS MES_EXTENSO,
			         VAT.SK_VENDEDOR                        AS SK_VENDEDOR,
			         VAT.VENDEDOR                           AS VENDEDOR,
			         CASE FMVD.DD_NF_SERIE WHEN 'DZ' THEN 
			             SUM(FMVD.VLR_TOTAL_NOTA)
			         ELSE
			             SUM(FMVD.QTD_ITEM * FMVD.VLR_LIQ_UNIT)
			         END AS FATURAMENTO,
			         0                                      AS META,
			         0                                      AS PURIPOINTS
			  FROM FATO_MOVIMENTO_VENDA_DIA FMVD 
			       CROSS JOIN VENDEDOR VENDE 
			       INNER JOIN DIM_VENDEDOR     VAT ON VENDE.NK_VENDEDOR = VAT.NK_VENDEDOR 
			       INNER JOIN DIM_TEMPO_DIA    DTD ON FMVD.SK_TEMPO_DIA = DTD.SK_TEMPO_DIA
			       INNER JOIN DIM_VENDEDOR     DV  ON DV.SK_VENDEDOR    = FMVD.SK_VENDEDOR 
			       INNER JOIN DIM_PRODUTO      DP  ON FMVD.SK_PRODUTO   = DP.SK_PRODUTO
			       INNER JOIN DIM_GRUPO        DG  ON DG.NK_GRUPO       = DP.NK_GRUPO
			       INNER JOIN DIM_SUBGRUPO   DS ON DS.NK_SUBGRUPO    = DP.NK_SUBGRUPO
			       INNER JOIN DIM_EMPRESA      DE  ON DE.SK_EMPRESA     = FMVD.SK_EMPRESA 
			       INNER JOIN DIM_TABELA_PRECO DTP ON DTP.SK_TABELA_PRECO = FMVD.SK_TABELA_PRECO 
			  WHERE (VENDE.COD_FILIAL NOTNULL OR VENDE.NK_GRUPO NOTNULL OR VENDE.NK_SUBGRUPOS NOTNULL)
			  AND (DG.NK_GRUPO IN (SELECT NK_GRUPOS FROM aux_GRUPO_vendedor_at GR WHERE GR.NK_VENDEDOR = VENDE.NK_VENDEDOR) OR DG.NK_GRUPO NOT IN (SELECT NK_GRUPOS FROM aux_GRUPO_vendedor_at GR WHERE GR.NK_VENDEDOR = VENDE.NK_VENDEDOR))
			  AND (DE.COD_FILIAL IN (SELECT CAST(COD_FILIAIS AS INTEGER) FROM AUX_FILIAL_VENDEDOR_AT FIL WHERE FIL.NK_VENDEDOR = VENDE.NK_VENDEDOR) OR DE.COD_FILIAL &lt;&gt; NULL)
			  AND (DP.NK_SUBGRUPO IN (SELECT NK_SUBGRUPOS FROM aux_SUBGRUPO_vendedor_at SUB WHERE SUB.NK_VENDEDOR  = VENDE.NK_VENDEDOR) OR DP.NK_SUBGRUPO NOT IN ((SELECT NK_SUBGRUPOS FROM aux_SUBGRUPO_vendedor_at SUB WHERE SUB.NK_VENDEDOR  = VENDE.NK_VENDEDOR)))
			  AND DTD.ANO = EXTRACT(YEAR FROM CURRENT_DATE)
			  --AND VAT.SK_VENDEDOR = 1219
			  GROUP BY 1,2,3,4,5,6,7,8,FMVD.DD_NF_SERIE 

  )X
  WHERE X.ANO = EXTRACT(YEAR FROM CURRENT_DATE) 
  GROUP BY 1,2,3,4,5,6,7,8, 9
  ORDER BY 1,2
  

