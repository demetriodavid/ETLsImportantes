    <connection>[DW] - PostgreSQL - Vistra BI</connection>
    <sql>WITH FATURAMENTO_META AS(
SELECT DTD.ANO,
       DTD.MES,
       DG.GRUPO,
       DS.SUBGRUPO,
       DTD.MES_ABREVIADO                      AS MES_ABREVIADO,
       DTD.MES_EXTENSO                        AS MES_EXTENSO,
       DV.SK_VENDEDOR                         AS SK_VENDEDOR,
       DV.VENDEDOR,
       CASE FMVD.DD_NF_SERIE WHEN 'DZ' THEN 
           SUM(FMVD.VLR_TOTAL_NOTA)
       ELSE
           SUM(FMVD.QTD_ITEM * FMVD.VLR_LIQ_UNIT)
       END AS FATURAMENTO,
       0                                      AS META,
       0                                      AS PURIPOINTS
  FROM FATO_MOVIMENTO_VENDA_DIA FMVD
       INNER JOIN DIM_TEMPO_DIA DTD ON FMVD.SK_TEMPO_DIA = DTD.SK_TEMPO_DIA
       INNER JOIN DIM_VENDEDOR   DV ON FMVD.SK_VENDEDOR  = DV.SK_VENDEDOR
       INNER JOIN DIM_PRODUTO    DP ON FMVD.SK_PRODUTO   = DP.SK_PRODUTO 
       INNER JOIN DIM_GRUPO      DG ON DG.NK_GRUPO       = DP.NK_GRUPO 
       INNER JOIN DIM_SUBGRUPO   DS ON DS.NK_SUBGRUPO    = DP.NK_SUBGRUPO
  WHERE FMVD.TIPO = '112'
 GROUP BY 1,2,3,4,5,6,7,8,FMVD.DD_NF_SERIE

UNION ALL

SELECT DTD.ANO,
       DTD.MES,
       DG.GRUPO,
       DS.SUBGRUPO,
       DTD.MES_ABREVIADO                      AS MES_ABREVIADO,
       DTD.MES_EXTENSO                        AS MES_EXTENSO,
       1372                                   AS SK_VENDEDOR,
       'VENDEDOR NAO INFORMADO'               AS VENDEDOR,
       0                                      AS FATURAMENTO,
       0                                      AS META,
       0                                      AS PURIPOINTS
  FROM FATO_MOVIMENTO_VENDA_DIA FMVD
       INNER JOIN DIM_TEMPO_DIA DTD ON FMVD.SK_TEMPO_DIA = DTD.SK_TEMPO_DIA
       INNER JOIN DIM_VENDEDOR   DV ON FMVD.SK_VENDEDOR  = DV.SK_VENDEDOR
       INNER JOIN DIM_PRODUTO    DP ON FMVD.SK_PRODUTO   = DP.SK_PRODUTO 
       INNER JOIN DIM_GRUPO      DG ON DG.NK_GRUPO       = DP.NK_GRUPO 
       INNER JOIN DIM_SUBGRUPO   DS ON DS.NK_SUBGRUPO    = DP.NK_SUBGRUPO
  WHERE FMVD.TIPO = '112'
 GROUP BY 1,2,3,4,5,6,7,8,FMVD.DD_NF_SERIE

UNION ALL

SELECT DTM.ANO,
       DTM.MES,
       DG.GRUPO,
       DS.SUBGRUPO,
       DTM.MES_ABREVIADO                      AS MES_ABREVIADO,
       DTM.MES_EXTENSO                        AS MES_EXTENSO,
       DV.SK_VENDEDOR                         AS SK_VENDEDOR,
       DV.VENDEDOR,
       0                                      AS FATURAMENTO,
       SUM(FMVM.VLR_META)                     AS META,
       SUM(FMVM.VLR_PONTOS)                   AS PURIPOINTS
  FROM FATO_META_VENDEDOR_MES FMVM
       INNER JOIN DIM_TEMPO_MES DTM ON FMVM.SK_TEMPO_MES = DTM.SK_TEMPO_MES
       INNER JOIN DIM_VENDEDOR  DV  ON FMVM.SK_VENDEDOR  = DV.SK_VENDEDOR
       INNER JOIN DIM_GRUPO     DG  ON FMVM.SK_GRUPO     = DG.SK_GRUPO 
       INNER JOIN DIM_SUBGRUPO   DS ON FMVM.SK_SUBGRUPO    = DS.SK_SUBGRUPO
 GROUP BY 1,2,3,4,5,6,7,8

UNION ALL 
   SELECT DTD.ANO,
       DTD.MES,
       DG.GRUPO,
       DS.SUBGRUPO,
       DTD.MES_ABREVIADO                      AS MES_ABREVIADO,
       DTD.MES_EXTENSO                        AS MES_EXTENSO,
       1230                                   AS SK_VENDEDOR,
       'PABBLO HENRIQUE BRITO SILVA'          AS VENDEDOR,
       CASE FMVD.DD_NF_SERIE WHEN 'DZ' THEN 
           SUM(FMVD.VLR_TOTAL_NOTA)
       ELSE
