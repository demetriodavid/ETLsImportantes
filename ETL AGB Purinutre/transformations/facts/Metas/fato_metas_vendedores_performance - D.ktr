   SELECT DTD.ANO,
       DTD.MES,
       DG.GRUPO,
       DTD.MES_ABREVIADO                      AS MES_ABREVIADO,
       DTD.MES_EXTENSO                        AS MES_EXTENSO,
       'MARCONE MORCI'                        AS VENDEDOR,
       CASE FMVD.DD_NF_SERIE WHEN 'DZ' THEN 
           SUM(FMVD.VLR_TOTAL_NOTA)
       ELSE
           SUM(FMVD.QTD_ITEM * FMVD.VLR_LIQ_UNIT)
       END AS FATURAMENTO,
       0                                      AS META,
       0                                      AS PURIPOINTS
  FROM FATO_MOVIMENTO_VENDA_DIA FMVD
       INNER JOIN DIM_TEMPO_DIA    DTD ON FMVD.SK_TEMPO_DIA = DTD.SK_TEMPO_DIA
       INNER JOIN DIM_VENDEDOR     DV  ON FMVD.SK_VENDEDOR  = DV.SK_VENDEDOR
       INNER JOIN DIM_PRODUTO      DP  ON FMVD.SK_PRODUTO   = DP.SK_PRODUTO
       INNER JOIN DIM_GRUPO        DG  ON DP.NK_GRUPO       = DG.NK_GRUPO
       INNER JOIN DIM_EMPRESA      DE  ON DE.SK_EMPRESA     = FMVD.SK_EMPRESA 
       INNER JOIN DIM_TABELA_PRECO DTP ON DTP.SK_TABELA_PRECO = FMVD.SK_TABELA_PRECO 
       WHERE DE.COD_FILIAL IN  ('2','1','3','4','8','5','6','7','50')
         AND FMVD.TIPO = '112'
 GROUP BY 1,2,3,4,5,7,FMVD.DD_NF_SERIE
*/
  
  )
  
SELECT ANO,
       MES,
       GRUPO,
       CASE
            WHEN MES BETWEEN 1 AND 3 THEN 'Q1'
            WHEN MES BETWEEN 4 AND 6 THEN 'Q2'
            WHEN MES BETWEEN 7 AND 9 THEN 'Q3'
            ELSE 'Q4'
       END             AS TRIMESTRE,
       MES_ABREVIADO,
       SK_VENDEDOR,
       VENDEDOR,
       MES_EXTENSO,
       SUM(FATURAMENTO) AS FATURAMENTO,
       SUM(META)        AS META,
       SUM(PURIPOINTS)  AS PURIPOINTS
  FROM FATURAMENTO_META FM
 WHERE ANO = EXTRACT(YEAR FROM CURRENT_DATE) 
 --AND VENDEDOR LIKE '%OLIMP%'
--WHERE ANO = 2022
--  AND MES = 1
GROUP BY 1,2,3,4,5,6,7,8
--HAVING SUM(META) &lt;&gt; 0 
ORDER BY 1,2

 
 
