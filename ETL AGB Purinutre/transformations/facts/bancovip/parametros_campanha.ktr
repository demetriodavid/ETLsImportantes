		when 'META_SUBGRUPO' then dbc.subgrupo 
		when 'META_FABRICANTE' then dbc.fabricante  
		when 'META_PRODUTO' then dbc.produto
		when 'VOLUME' then dbc.produto  
	end regra
, null as NULL
from dim_bancovip_campanha dbc 
