  <step>
    <name>[DW] - Extração Fato Movimento Vendas</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>[DW] - PostgreSQL - Vistra BI</connection>
    <sql>WITH DESCONSIDERA_FILIAL AS(
SELECT VALOR
  FROM DIM_JUNK_PARAMETRO 
 WHERE PARAMETRO = 'FILIAIS_A_DESCONSIDERAR' 
)

SELECT
    DTD.SK_TEMPO_DIA,
    DE.SK_EMPRESA,
    DV.SK_VENDEDOR,
    DP.SK_PRODUTO,
    DC.SK_CLIENTE,
    DOP.SK_OPERACAO_FISCAL,
    DTP.SK_TABELA_PRECO,
    DCN.SK_CONDICAO,
    STG.NUMERO_NOTA AS DD_NOTA_FISCAL,
    STG.SERIE_NOTA  AS DD_NF_SERIE,
    STG.TIPO,
    0 AS ESTOQUE,
    SUM(STG.VLR_CUSTO_MEDIO*STG.QUANTIDADE)/SUM(STG.QUANTIDADE) AS VLR_CUSTO_MEDIO,
    SUM(STG.VLR_DESC_NOTA)    AS VLR_DESC_NOTA,   
    SUM(STG.VLR_TOTAL_NOTA) AS VLR_TOTAL_NOTA,
    SUM(STG.VLR_LIQ_UNIT*STG.QUANTIDADE)/SUM(STG.QUANTIDADE)   AS VLR_LIQ_UNIT,
    SUM(STG.QUANTIDADE) AS QTD_ITEM,
    CASE STG.SERIE_NOTA  WHEN  'DZ' THEN
        SUM(STG.VLR_TOTAL_NOTA)
    ELSE
        SUM(STG.QUANTIDADE*STG.VLR_LIQ_UNIT)
    END AS VLR_TOTAL_PRODUTO

FROM
    STAGE.STG_MOVIMENTO_VENDA_DIA STG
    INNER JOIN DIM_EMPRESA DE USING (NK_EMPRESA)
    INNER JOIN DIM_PRODUTO DP USING (NK_PRODUTO)
    INNER JOIN DIM_CLIENTE DC USING (NK_CLIENTE)
    INNER JOIN DIM_OPERACAO_FISCAL DOP USING (NK_OPERACAO_FISCAL)
