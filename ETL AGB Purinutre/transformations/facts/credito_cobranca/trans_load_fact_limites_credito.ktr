  <step>
    <name>Table input STG LIMITE</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>[DW] - PostgreSQL - Vistra BI</connection>
    <sql>SELECT COALESCE(COALESCE(VENDA.SK_EMPRESA, AREC.SK_EMPRESA), REC.SK_EMPRESA) AS SK_EMPRESA,
       
       DC.SK_CLIENTE AS SK_CLIENTE,
       STG.LIMITEINCONDICIONAL, 
       STG.LIMITEGARANTIA, 
       STG.GARANTIACPR,
       CAST(STG.VENCIMENTO AS CHAR) AS VENCIMENTO,
       CASE 
            WHEN DV.NK_VENDEDOR = '-1'  AND VENDA.SK_VENDEDOR &lt;&gt; NULL THEN 
                 DV.SK_VENDEDOR 

            ELSE 
                DV.SK_VENDEDOR
       END SK_VENDEDOR,
       MIN(DIAVENDA.NK_TEMPO_DIA) AS PRIMEIRA_COMPRA,
       MAX(DIAVENDA.NK_TEMPO_DIA) AS ULTIMA_COMPRA,
      
       (SELECT 
         SUM(QTD_ITEM * VLR_LIQ_UNIT) AS VALOR
    FROM FATO_MOVIMENTO_VENDA_DIA FATO
    INNER JOIN DIM_TEMPO_DIA DTD USING(SK_TEMPO_DIA)
    WHERE SK_CLIENTE = DC.SK_CLIENTE AND FATO.TIPO = '112' AND DTD.NK_TEMPO_DIA = (SELECT 
                                                         MIN(DTD.NK_TEMPO_DIA) AS DATA_EMISSAO
                                                    FROM FATO_MOVIMENTO_VENDA_DIA FATO
                                                    INNER JOIN DIM_TEMPO_DIA DTD USING(SK_TEMPO_DIA)
                                                    WHERE SK_CLIENTE = DC.SK_CLIENTE  AND FATO.TIPO = '112'
                                                    ORDER BY 1)
    ORDER BY 1)  AS VLR_PRIMEIRA_COMPRA,
    
    SUM(VENDA.QTD_ITEM * VENDA.VLR_LIQ_UNIT) AS TOTAL_COMPRADO,
    
    COALESCE(SUM(AREC.VLR_SALDO), 0) AS VALOR_ABERTO,
    
    SUM(VENDA.QTD_ITEM * VENDA.VLR_LIQ_UNIT) / COUNT(DISTINCT(VENDA.DD_NOTA_FISCAL)) AS MEDIA_COMPRA,
    
    COUNT(DISTINCT(VENDA.DD_NOTA_FISCAL)) AS TOTAL_NOTAS,
    
    --SUM(DTV.NK_TEMPO_DIA - DTB.NK_TEMPO_DIA) AS DIAS_ATRAZO,
   (SELECT SUM(DTB.SK_TEMPO_DIA - DTV.SK_TEMPO_DIA) AS DIAS
  FROM FATO_MOV_FIN_TITULO_RECEBER FATO 
  INNER JOIN DIM_TEMPO_DIA DTV ON DTV.SK_TEMPO_DIA = FATO.SK_TEMPO_DIA_VENC
  INNER JOIN DIM_TEMPO_DIA DTB ON DTB.SK_TEMPO_DIA = FATO.SK_TEMPO_DIA_BAIXA 
  WHERE FATO.SK_CLIENTE = DC.SK_CLIENTE
    AND DTB.NK_TEMPO_DIA &lt;&gt; '9999-12-31'
    AND DTB.SK_TEMPO_DIA > DTV.SK_TEMPO_DIA) AS DIAS_ATRASO,
    
    SUM(AREC.DD_reng + REC.DD_reng) AS TITULOS_RENEGOCIADOS,
    
    SUM(CASE WHEN DTVA.NK_TEMPO_DIA &lt; CURRENT_DATE AND DTBA.NK_TEMPO_DIA = '31/12/9999' THEN 
         AREC.VLR_SALDO
     ELSE 
         0
     END) VALOR_ATRASADOS,
     
     COUNT(CASE WHEN DTVA.NK_TEMPO_DIA &lt; CURRENT_DATE AND DTBA.NK_TEMPO_DIA = '31/12/9999' THEN 
         AREC.DD_TITULO
     ELSE 
         0
     END) TITULOS_ATRASADOS,
     
     MAX(CASE 
             WHEN DTB.NK_TEMPO_DIA = '31/12/9999' THEN 
                  DIA.SK_TEMPO_DIA - DTV.SK_TEMPO_DIA
             ELSE
                   DTB.SK_TEMPO_DIA - DTV.SK_TEMPO_DIA
         END)          AS MAIOR_ATRASO,
     
    SUM(REC.VLR_JUROS + REC.VLR_MULTA) AS JUROS_MULTA
  FROM STAGE.STG_LIMITE_CREDITO STG 
  INNER JOIN DIM_CLIENTE DC                    ON DC.NK_CLIENTE = STG.NK_CLIENTE 
  LEFT JOIN DIM_VENDEDOR DV                   ON DV.NK_VENDEDOR = STG.NK_VENDEDOR
  LEFT  JOIN FATO_MOVIMENTO_VENDA_DIA VENDA    ON DC.SK_CLIENTE = VENDA.SK_CLIENTE 
                                              AND VENDA.TIPO  = '112'
  LEFT JOIN DIM_TEMPO_DIA DIAVENDA             ON DIAVENDA.SK_TEMPO_DIA = VENDA.SK_TEMPO_DIA 
  LEFT JOIN FATO_MOV_FIN_TITULO_A_RECEBER AREC ON AREC.SK_CLIENTE = DC.SK_CLIENTE 
  LEFT JOIN DIM_TEMPO_DIA DTVA                 ON AREC.SK_TEMPO_DIA_VENC = DTVA.SK_TEMPO_DIA
  LEFT JOIN DIM_TEMPO_DIA DTBA                 ON AREC.SK_TEMPO_DIA_BAIXA = DTBA.SK_TEMPO_DIA 
  LEFT JOIN FATO_MOV_FIN_TITULO_RECEBER REC    ON REC.SK_CLIENTE = DC.SK_CLIENTE
  LEFT JOIN DIM_TEMPO_DIA DTV                  ON REC.SK_TEMPO_DIA_VENC = DTV.SK_TEMPO_DIA 
