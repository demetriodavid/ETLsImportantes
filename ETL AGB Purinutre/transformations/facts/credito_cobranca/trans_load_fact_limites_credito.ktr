    <sql>  SELECT SK_CLIENTE AS CLIENTE1
FROM DIM_CLIENTE WHERE ATIVO = 1 AND SK_CLIENTE IN (SELECT SK_CLIENTE  FROM FATO_MOVIMENTO_VENDA_DIA WHERE TIPO = '112')
AND SK_CLIENTE IN (SELECT SK_CLIENTE FROM STAGE.STG_LIMITE_CREDITO)

</sql>
    <limit>0</limit>
