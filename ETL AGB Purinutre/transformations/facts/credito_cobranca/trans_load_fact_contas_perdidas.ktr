  <step>
    <name>Union Tabela vendas com contas perdidas</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>[DW] - PostgreSQL - Vistra BI</connection>
    <sql>SELECT DE.SK_EMPRESA, 
       DVEND.SK_TEMPO_DIA    AS DATA_VENDA,
       DVENC.SK_TEMPO_DIA    AS DATA_VENCIMENTO,
       DV.SK_VENDEDOR        AS SK_VENDEDOR,
       DP.SK_PRODUTO         AS SK_PRODUTO,
       DTP.SK_TABELA_PRECO   AS SK_TABELA_PRECO,
       DC.SK_CLIENTE         AS SK_CLIENTE , 
       FATO.LOCALIZACAO   AS LOCALIZACAO,
       (FATO.VALOR * ((((qtd_item * vlr_liq_unit)
          ) * 100)/ (SELECT SUM(qtd_item * vlr_liq_unit) 
          FROM FATO_MOVIMENTO_VENDA_DIA VENDA 
         WHERE VENDA.DD_NOTA_FISCAL = FATO.nota
           AND VENDA.SK_EMPRESA = DE.SK_EMPRESA
           AND VENDA.SK_CLIENTE = DC.SK_CLIENTE 
          )))/100 AS VLR_PRODUTO,
       FATO.NOTA,
       FATO.COBRANCA1,
       FATO.COBRANCA2,
       FATO.COBRANCA3, 
       CASE 
           WHEN FATO.TIPO_PAGAMENTO = 'd'
              THEN 'DINHEIRO'
           ELSE 'CHEQUE'
        END AS TIPO_PAGAMENTO,
       FATO.VALOR
  FROM FATO_CREDITO_COBRANCA FATO
  LEFT JOIN FATO_MOVIMENTO_VENDA_DIA VENDA ON FATO.SK_EMPRESA = VENDA.SK_EMPRESA 
                                          AND FATO.SK_CLIENTE = VENDA.SK_CLIENTE 
                                          AND FATO.NOTA       = VENDA.DD_NOTA_FISCAL 
  INNER JOIN DIM_EMPRESA DE ON FATO.SK_EMPRESA = DE.SK_EMPRESA 
  INNER JOIN DIM_TEMPO_DIA DVEND ON DVEND.SK_TEMPO_DIA = FATO.SK_DT_VENDA
  INNER JOIN DIM_TEMPO_DIA DVENC ON DVENC.SK_TEMPO_DIA = FATO.SK_DT_VENCI 
  INNER JOIN DIM_CLIENTE     DC  ON DC.SK_CLIENTE      = FATO.SK_CLIENTE
  LEFT JOIN DIM_VENDEDOR     DV  ON DV.SK_VENDEDOR     = VENDA.SK_VENDEDOR 
  LEFT JOIN DIM_PRODUTO      DP  ON DP.SK_PRODUTO      = VENDA.SK_PRODUTO 
  LEFT JOIN DIM_TABELA_PRECO DTP ON DTP.SK_TABELA_PRECO = VENDA.SK_TABELA_PRECO </sql>
