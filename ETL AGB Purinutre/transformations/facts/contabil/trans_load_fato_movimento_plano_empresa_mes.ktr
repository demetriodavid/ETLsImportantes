 WHERE DTM.DATA_ULTIMO_DIA_MES BETWEEN CURRENT_DATE -INTERVAL '10 YEARS' AND CURRENT_DATE)
 
SELECT COALESCE(AG.SK_TEMPO_MES,ADL.SK_TEMPO_MES)         AS SK_TEMPO_MES,
       COALESCE(AG.SK_EMPRESA,ADL.SK_EMPRESA)             AS SK_EMPRESA,
       COALESCE(AG.SK_PLANO_EMPRESA,ADL.SK_PLANO_EMPRESA) AS SK_PLANO_EMPRESA,
       COALESCE(AG.MOVIMENTO,ADL.MOVIMENTO)               AS MOVIMENTO
  FROM AUX_GERAL AG
       full OUTER JOIN AUX_ADD_LINHA ADL ON AG.SK_TEMPO_MES     = ADL.SK_TEMPO_MES
