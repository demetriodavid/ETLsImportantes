    <sql>WITH AUX_TRATADO AS(
SELECT D.CTRL_DDE||'#'||D.CODI_CDE AS NK_PLANO_EMPRESA_PERSONALIZADO,
	   D.CODI_CDE AS NK_CABECALHO_PERSONALIZADO,
       D.POSI_DDE AS POSICAO,
       D.SEQU_DDE AS SEQUENCIA,
       CASE WHEN D.TVAL_DDE IS NULL THEN 9 ELSE CAST(D.TIPO_DDE AS integer) END AS TIPO,
       REGEXP_REPLACE(REGEXP_REPLACE(REGEXP_REPLACE(D.FORM_DDE,'\*100|\*-1',''),'[^[:alnum:]|+-/*]', ''),'{1,}[+-/*]',',') AS CONTAS,
       REGEXP_REPLACE(REGEXP_REPLACE(REGEXP_REPLACE(REGEXP_REPLACE(REGEXP_REPLACE(REGEXP_REPLACE(REGEXP_REPLACE(REGEXP_REPLACE(REGEXP_REPLACE(
       D.FORM_DDE,'(\*100)','&amp;'),'(\*-1)','#'),'\/100','@'),'[^+-/@#&amp;*\[\{\(\)]', ''),'[\[\{]','%s'),'&amp;','*100'),'-','+'),'#','*-1'),'@','/100')     
       AS FORMULA_PREPARADA,
       D.FORM_DDE FORMULA,
       D.DESC_DDE AS DESCRICAO              
  FROM DETDEM D
 WHERE D.TIPO_DDE &lt;&gt; 9 
  -- AND D.CODI_CDE = '50000017'
)
SELECT AUX.NK_PLANO_EMPRESA_PERSONALIZADO,
	   AUX.NK_CABECALHO_PERSONALIZADO,
       AUX.POSICAO,
       AUX.SEQUENCIA,
       AUX.TIPO,
       CASE WHEN DBMS_LOB.SUBSTR(AUX.CONTAS,1,1) = ',' 
            THEN DBMS_LOB.SUBSTR(AUX.CONTAS,1000,2)
            WHEN DBMS_LOB.SUBSTR(AUX.CONTAS,1000) IS NULL OR DBMS_LOB.SUBSTR(AUX.CONTAS,1000) = '0'
            THEN '-1'
            ELSE DBMS_LOB.SUBSTR(AUX.CONTAS,1000) 
       END AS CONTAS,          
       CASE WHEN DBMS_LOB.SUBSTR(AUX.FORMULA_PREPARADA,1000) IS NULL
            THEN '%s'
            ELSE DBMS_LOB.SUBSTR(AUX.FORMULA_PREPARADA,1000)
       END AS FORMULA_PREPARADA,
       AUX.FORMULA,
       AUX.DESCRICAO
  FROM AUX_TRATADO AUX</sql>
