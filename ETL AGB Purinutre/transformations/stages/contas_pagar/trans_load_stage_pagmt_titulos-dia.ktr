   '26','312','10000305', '10000328', '304', '300','10000010', '10000327', '7','2', '10000326', '303', '302', '10000318', '10000308', '80', '10000306', '10000309') 
   AND (L.CODI_TPG in ('11','10000005', '12', '10000003', '10000004', '10000002', '10000001', '9', '15', '2', '13', '8', '10', '3', '1', '5', '7', '6', '14', '4', '10000007') OR L.CODI_TPG IS NULL)
   AND C.DPAG_CPB >= ?
