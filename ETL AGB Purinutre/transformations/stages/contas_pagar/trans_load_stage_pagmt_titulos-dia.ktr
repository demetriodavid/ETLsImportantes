    <connection>[DB] - Oracle - AgriBusiness ERP</connection>
    <sql>SELECT
    TO_CHAR(C.CODI_EMP) AS NK_EMPRESA,
    TO_CHAR(CP.CODI_TRA) AS NK_FORNECEDOR,
    TO_CHAR(C.CODI_TPG) AS NK_TIPO_PAGAMENTO,
    TO_CHAR(C.DPAG_CPB, UPPER('DD/MM/YYYY')) AS NK_TEMPO_DIA,
    CP.DOCU_CPG AS NUM_DOC_PAGAR,
    P.NPAR_PAG AS NUMERO_PARCELA,
    C.CTRL_PAG,
    (
        CASE
            WHEN C.CODI_IND IS NOT NULL
            THEN (C.DESC_CPB / IV.VLOR_VLR)
            ELSE C.DESC_CPB
        END
    ) AS VLR_DESC,
    (
        CASE
            WHEN C.CODI_IND IS NOT NULL
            THEN (C.JURO_CPB / IV.VLOR_VLR)
            ELSE C.JURO_CPB
        END
    ) AS VLR_JUROS,
    (
        CASE
            WHEN C.CODI_IND IS NOT NULL
            THEN (C.MULT_CPB / IV.VLOR_VLR)
            ELSE C.MULT_CPB
        END
    ) AS VLR_MULTA,
    (
        CASE
            WHEN C.CODI_IND IS NOT NULL
            THEN (C.VLOR_CPB / IV.VLOR_VLR)
            ELSE C.VLOR_CPB
        END
    ) AS VLR_PAGO
FROM
    CPGBAIXA C
    INNER JOIN PAGAR P ON P.CTRL_PAG = C.CTRL_PAG
    INNER JOIN CABPAGAR CP ON CP.CTRL_CPG = P.CTRL_CPG
    LEFT JOIN LANCAMEN L ON C.CTRL_LAN = L.CTRL_LAN
    LEFT OUTER JOIN INDVALOR IV ON IV.CODI_EMP = C.CODI_EMP
        AND IV.CODI_IND = C.CODI_IND
        AND IV.DATA_VLR = C.DATA_VLR
WHERE UPPER(C.SITU_CPB) = UPPER('N') AND CP.CODI_TDO IN ('3','12','10000307', '10000008' ,'10000011','10000324','11','10000310','10000325','40','41' ,'50','51','10000319','10000320','31' ,'32','33','35','36' ,'37' 
