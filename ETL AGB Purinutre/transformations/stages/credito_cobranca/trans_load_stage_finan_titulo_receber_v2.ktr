    <connection>[DB] - Oracle - AgriBusiness ERP</connection>
    <sql>select C.CODI_EMP, C.CODI_TRA, T.RAZA_TRA, C.NUME_CBR, 
          C.SERI_CBR, R.NPAR_REC,  R.HIST_REC as HISTOR, R.VENC_REC, 
          R.VLOR_REC as VALOR, C.DATA_CBR as EMISSAO, 
          TD.TIPO_TDO, C.CTRL_CBR, 
          R.CODI_IND, TD.CODI_TDO, TD.DESC_TDO, 
          (select VALOR_IND from  table(RET_VLORIND(C.CODI_EMP, R.CODI_IND, R.DATA_VLR))) as VLOR_VLR 
     , ( 
        select NOC.NDOC_NOC 
        from NOTACRC NOC 
        where (NOC.CTRL_CBR = C.CTRL_CBR) 
         and rownum &lt;= 1) as NDOC_NOC 
     , ( 
        select NOC.SDOC_NOC 
        from NOTACRC NOC 
        where (NOC.CTRL_CBR = C.CTRL_CBR) 
         and rownum &lt;= 1) as SDOC_NOC 
     , R.VLOR_REC, R.CTRL_REC
from CABREC C 
