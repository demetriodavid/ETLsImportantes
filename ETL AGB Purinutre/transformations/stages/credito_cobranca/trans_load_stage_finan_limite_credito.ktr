    <connection>[DB] - Oracle - AgriBusiness ERP</connection>
    <sql>WITH financeiro_aberto AS 
(SELECT X.CODI_TRA,SUM(X.VLAB_REC) VLAB_REC, SUM(VENCIDO_MAIS_CINCO_DIAS) VENCIDO_MAIS_CINCO_DIAS FROM (
SELECT 

 CBR.CODI_TRA, 
 sum((SELECT VALOR                                                               
         FROM  table(                                                            
                 VALOR_ABERTO_RECEBER(REC.CTRL_REC)                                 
              ))) AS VLAB_REC  ,                                             
0 VENCIDO_MAIS_CINCO_DIAS                                                         
 FROM CABREC CBR                                                                    
 JOIN RECEBER REC ON (REC.CTRL_CBR = CBR.CTRL_CBR)                                  
 JOIN TIPDOC TDO ON (TDO.CODI_TDO = CBR.CODI_TDO)                                   
 JOIN TIPCOB TCO ON (TCO.CODI_TCO = REC.CODI_TCO)                                   
 LEFT JOIN PARAMGER PAR ON (PAR.CODI_EMP = CBR.CODI_EMP)                            
 WHERE (REC.SITU_REC &lt;&gt; 'C')                   
   AND ((TDO.DDLC_TDO &lt;&gt; 'S') OR (TDO.DDLC_TDO IS NULL))     
   GROUP BY CBR.CODI_TRA
   
 UNION ALL                                                                          
 SELECT 
 
 CHR.CODI_TRA,
 sum(CHR.VLOR_CHR ) VLAB_REC   ,
 0 VENCIDO_MAIS_CINCO_DIAS
                                     
 FROM CHEQUEREC CHR                                                                 
 JOIN TIPDOC TDO ON (CHR.CODI_TDO = TDO.CODI_TDO)                                   
 JOIN CHQRECLANC CRL ON (CRL.CODI_EMP = CHR.CODI_EMP)                               
                    AND (CRL.CODI_AGE = CHR.CODI_AGE)                               
                    AND (CRL.CONT_CHR = CHR.CONT_CHR)                               
                    AND (CRL.NUME_CHR = CHR.NUME_CHR)                               
                    AND (CRL.SERI_CHR = CHR.SERI_CHR)                               
                    AND (CRL.SEQU_CRL = (SELECT MAX(CRL_MAX.SEQU_CRL)               
                                         FROM CHQRECLANC CRL_MAX                    
                                         WHERE (CRL_MAX.CODI_EMP = CRL.CODI_EMP)    
                                           AND (CRL_MAX.CODI_AGE = CRL.CODI_AGE)    
                                           AND (CRL_MAX.CONT_CHR = CRL.CONT_CHR)    
                                           AND (CRL_MAX.NUME_CHR = CRL.NUME_CHR)    
                                           AND (CRL_MAX.SERI_CHR = CRL.SERI_CHR)))  
                                                                                    
 LEFT JOIN LANCAMEN LAN ON (LAN.CTRL_LAN = CRL.CTRL_LAN)                            
                       AND (LAN.CREL_LAN IS NULL)                                   
                       AND (LAN.SITU_LAN &lt;&gt; 'X')                                  
 LEFT JOIN PARAMGERFINANC PRF ON (PRF.CODI_EMP = CHR.EMAC_CHR)                      
 LEFT JOIN PARAMGER PAR ON (PAR.CODI_EMP = CHR.EMAC_CHR)                            
 WHERE ((CHR.SITU_CHR IN ('A', 'D', 'T', 'M', 'J')) OR                    
        ((PRF.LLCC_PRF = 'S') AND                                                 
         (CHR.SITU_CHR IN ('B', 'T', 'M', 'F')) AND                         
         ((LAN.CONC_LAN = 'N') OR (LAN.CONC_LAN IS NULL)) AND                     
         (LAN.DCOM_LAN IS NULL)))                                                   
   AND ((TDO.DDLC_TDO &lt;&gt; 'S') OR (TDO.DDLC_TDO IS NULL))    
   GROUP BY CHR.CODI_TRA
   
UNION ALL
   
   SELECT 
 CBR.CODI_TRA, 
 0 AS VLAB_REC  ,                                             
sum((SELECT VALOR                                                               
         FROM  table(                                                            
                 VALOR_ABERTO_RECEBER(REC.CTRL_REC)                                 
              ))) VENCIDO_MAIS_CINCO_DIAS                                                         
 FROM CABREC CBR                                                                    
 JOIN RECEBER REC ON (REC.CTRL_CBR = CBR.CTRL_CBR)                                  
 JOIN TIPDOC TDO ON (TDO.CODI_TDO = CBR.CODI_TDO)                                   
 JOIN TIPCOB TCO ON (TCO.CODI_TCO = REC.CODI_TCO)                                   
 LEFT JOIN PARAMGER PAR ON (PAR.CODI_EMP = CBR.CODI_EMP)                            
 WHERE (REC.SITU_REC &lt;&gt; 'C')                   
   AND ((TDO.DDLC_TDO &lt;&gt; 'S') OR (TDO.DDLC_TDO IS NULL))    AND REC.VENC_REC &lt; (CURRENT_DATE -5)
   GROUP BY CBR.CODI_TRA
   
   ) X GROUP BY X.CODI_TRA

), ultimo_ano AS
(
select N.CODI_TRA, count(npre_not) qtd_compra, sum(n.tota_not) total, sum(n.tota_not)/count(npre_not) media, max(n.tota_not) maior_valor from NOTA N 
			inner join FUNCAOTOPER F ON (F.CODI_TOP = N.CODI_TOP) 
			where N.SITU_NOT = '5' and F.CODI_PTO = '101' AND n.DEMI_NOT BETWEEN CAST(SYSDATE AS DATE) -366 AND SYSDATE 
		GROUP BY n.codi_tra
), maior_compra AS (
select N.CODI_TRA, MAX(TOTA_NOT) as "MAIOR_COMPRA" from NOTA N 
			inner join FUNCAOTOPER F ON (F.CODI_TOP = N.CODI_TOP) 
			where N.SITU_NOT = '5' and F.CODI_PTO = '101'  
			GROUP BY N.CODI_TRA 
), 
data_maior_compra AS (
	SELECT n.codi_tra, max(n.DEMI_NOT) data_maior_compra, max(n.TOTA_NOT) maior_compra
	FROM NOTA n 
	INNER JOIN maior_compra m ON m.codi_tra=n.codi_tra AND m.maior_compra=n.tota_not
	inner join FUNCAOTOPER F ON (F.CODI_TOP = N.CODI_TOP) 
	where N.SITU_NOT = '5' and F.CODI_PTO = '101' 
	GROUP BY n.codi_tra
			
),
menor_compra AS (
select N.CODI_TRA, min(TOTA_NOT) as "MENOR_COMPRA" from NOTA N 
			inner join FUNCAOTOPER F ON (F.CODI_TOP = N.CODI_TOP) 
			where N.SITU_NOT = '5' and F.CODI_PTO = '101'  
			GROUP BY N.CODI_TRA 
), 
data_menor_compra AS (
	SELECT n.codi_tra, max(n.DEMI_NOT) data_menor_compra, min(n.TOTA_NOT) menor_compra
	FROM NOTA n 
	INNER JOIN menor_compra m ON m.codi_tra=n.codi_tra AND m.menor_compra=n.tota_not
	inner join FUNCAOTOPER F ON (F.CODI_TOP = N.CODI_TOP) 
	where N.SITU_NOT = '5' and F.CODI_PTO = '101' 
	GROUP BY n.codi_tra
),
primeira_compra_npre AS (
	SELECT N.CODI_TRA, MIN(npre_not) as NPRE_NOT  from NOTA N 
	inner join FUNCAOTOPER F ON (F.CODI_TOP = N.CODI_TOP) 
	where N.SITU_NOT = '5' and F.CODI_PTO = '101'
	GROUP BY N.CODI_TRA
),
primeira_compra AS (
	SELECT N.CODI_TRA, N.DEMI_NOT, N.TOTA_NOT FROM NOTA N
	INNER JOIN PRIMEIRA_COMPRA_NPRE N1 ON N.NPRE_NOT = N1.NPRE_NOT
),

ultima_compra_npre AS (
	SELECT N.CODI_TRA, max(npre_not) as NPRE_NOT  from NOTA N 
	inner join FUNCAOTOPER F ON (F.CODI_TOP = N.CODI_TOP) 
	where N.SITU_NOT = '5' and F.CODI_PTO = '101'
	GROUP BY N.CODI_TRA
),
ultima_compra AS (
	SELECT N.CODI_TRA, N.DEMI_NOT, N.TOTA_NOT FROM NOTA N
	INNER JOIN ULTIMA_COMPRA_NPRE N1 ON N.NPRE_NOT = N1.NPRE_NOT
)/*,
ultima_alteracao_limite AS (
SELECT codi_tra,max(data_lio) ultima_alteracao FROM limiteoc 
GROUP BY codi_tra
)*/


SELECT distinct
COALESCE(C.CODI_EMP,-1) NK_EMPRESA,
LC.DUMANUT data_ultima_alteracao,
LC.CODI_TRA NK_CLIENTE,
COALESCE(V.COD1_PES,-1) NK_VENDEDOR,
SUM(COALESCE(T.INSS_TRA,0)) APOSENTADORIA_ESPECIAL,
SUM(COALESCE(LC.LIMI_LIM,0)) LIMITE_CREDITO,
SUM(COALESCE(LC.LIMG_LIM,0)) LIMITE_GARANTIA,
sum(COALESCE(CAST(C.RATI_CLI AS NUMBER),0)) RATING_CREDITO,
SUM(coalesce(VALOR_ABERTO.VLAB_REC,0)) VALOR_OCUPADO_LIMITE,
sum(coalesce(media_atraso.nr_atrazos,0)) numero_atrasos,
sum(coalesce(media_atraso.MEDIA_ATRAZO,0)) media_atrasos,
sum(coalesce(maior_atraso.maior_atraso,0)) MAIOR_ATRASO,
sum(coalesce(aux_compras.QTDE,0)) qtde_compras,
sum(coalesce(aux_compras.TOTAL,0)) total_compras,
MENOR_COMPRA.data_menor_compra,
sum(coalesce(menor_compra.menor_compra,0)) menor_compra,
PRIMEIRA_COMPRA.DEMI_NOT DATA_PRIMEIRA_COMPRA,
sum(coalesce(PRIMEIRA_COMPRA.TOTA_NOT,0)) VALOR_PRIMEIRA_COMPRA,
MAIOR_COMPRA.data_maior_compra,
SUM(coalesce(MAIOR_COMPRA.MAIOR_COMPRA,0)) MAIOR_COMPRA,
ULTIMA_COMPRA.DEMI_NOT DATA_ULTIMA_COMPRA,
SUM(coalesce(ULTIMA_COMPRA.TOTA_NOT,0)) VALOR_ULTIMA_COMPRA,
sum(coalesce(VALOR_ABERTO.VLAB_REC,0)) VALOR_ABERTO,
sum(COALESCE(fat.valor_a_vista,0)) VALOR_A_VISTA,
sum(COALESCE(fat.valor_PRAZO,0)) VALOR_PRAZO,
sum(COALESCE(ua.qtd_compra,0)) QTDE_COMPRA_ULTIMO_ANO,
sum(COALESCE(ua.total,0)) total_COMPRA_ULTIMO_ANO,
sum(COALESCE(ua.MEDIA,0)) MEDIA_COMPRA_ULTIMO_ANO,
sum(COALESCE(ua.MAIOR_VALOR,0)) MAIOR_COMPRA_ULTIMO_ANO,
SUM(COALESCE(VALOR_ABERTO.VENCIDO_MAIS_CINCO_DIAS,0)) VENCIDO_MAIS_CINCO_DIAS,
--DBMS_LOB.substr(OCOR_LIO, 3000) ALTERACAO_LIMITE,
SUM(COALESCE(G.VLOR_GAR,0)) VALOR_GARANTIA
FROM LIMITECR LC 
INNER JOIN TRANSAC T ON LC.CODI_TRA = T.CODI_TRA
LEFT JOIN CLIENTE C ON T.CODI_TRA=C.CODI_TRA
LEFT JOIN VENDEDORCLI V ON C.CODI_TRA=V.CODI_TRA AND C.CODI_EMP=V.CODI_EMP 
LEFT JOIN financeiro_aberto VALOR_ABERTO ON C.CODI_TRA=VALOR_ABERTO.CODI_TRA
LEFT JOIN (select  
   			c.codi_tra, 
		   	max(cb.DPAG_BAI - r.VENC_REC) AS MAIOR_ATRASO  
			from transac t  
			inner join cabrec c on (T.CODI_TRA = C.CODI_TRA)  
			inner join receber r on (c.CTRL_CBR = r.CTRL_CBR) 
			inner join crcbaixa cb on (r.CTRL_REC = cb.ctrl_rec) 
			inner join tipdoc td on (c.CODI_TDO = td.CODI_TDO)  
			and TD.TIPO_TDO = 'D' 
			and CB.SITU_BAI &lt;&gt; 'E' 
			GROUP BY c.codi_tra) maior_atraso ON c.codi_tra=maior_atraso.codi_tra
LEFT JOIN (select   
		   t.codi_tra,
		   count(CB.CTRL_REC) AS NR_ATRAZOS,   
		   Sum(cb.DPAG_BAI - r.VENC_REC) / count(CB.CTRL_REC) AS MEDIA_ATRAZO  
		   from transac t  inner join cabrec c on (T.CODI_TRA = C.CODI_TRA)  
		   inner join receber r on (c.CTRL_CBR = r.CTRL_CBR)  
		   inner join crcbaixa cb on (r.CTRL_REC = cb.ctrl_rec)
		   inner join tipdoc td on (c.CODI_TDO = td.CODI_TDO)  
		   where (cb.DPAG_BAI - r.VENC_REC) > 0
		   and TD.TIPO_TDO = 'D' and CB.SITU_BAI &lt;&gt; 'E'
		   GROUP BY t.codi_tra
		  ) media_atraso ON c.codi_tra=media_atraso.codi_tra
LEFT JOIN (
			SELECT N.CODI_TRA, COUNT(TOTA_NOT) as "QTDE", SUM(TOTA_NOT) as "TOTAL", MAX(TOTA_NOT) as "MAIOR" 
			from NOTA N inner join FUNCAOTOPER F ON (F.CODI_TOP = N.CODI_TOP) 
			where N.SITU_NOT = '5' and F.CODI_PTO = '101' 
			GROUP BY N.CODI_TRA
			) AUX_COMPRAS ON C.CODI_TRA=AUX_COMPRAS.CODI_TRA
LEFT JOIN data_menor_compra menor_compra ON C.CODI_TRA=MENOR_COMPRA.codi_tra
LEFT JOIN primeira_compra PRIMEIRA_COMPRA ON c.codi_tra=primeira_compra.codi_tra
LEFT JOIN data_maior_compra MAIOR_COMPRA ON C.CODI_TRA=MAIOR_COMPRA.codi_tra
LEFT JOIN ultima_compra ULTIMA_COMPRA ON C.CODI_TRA=ULTIMA_COMPRA.codi_tra
LEFT JOIN (
			SELECT
			N.CODI_TRA,
			SUM(CASE 
				WHEN OPER_CON = '2' OR PRAZ_CON IS NOT NULL THEN TOTA_NOT * (CASE WHEN F.FUNC_TOP ='A' THEN 1 ELSE -1 END) 
				ELSE 0
			END)VALOR_PRAZO,
			SUM(CASE 
				WHEN OPER_CON='1' AND PRAZ_CON IS NULL THEN TOTA_NOT * (CASE WHEN F.FUNC_TOP ='A' THEN 1 ELSE -1 END) 
				ELSE 0
			END)VALOR_A_VISTA
			FROM
			NOTA N 
			INNER JOIN FUNCAOTOPER F ON N.CODI_TOP=F.CODI_TOP
			INNER JOIN CONDICAO C ON N.COND_CON=C.COND_CON 
			WHERE F.CODI_PTO = 102 AND N.SITU_NOT='5' 
			GROUP BY n.codi_tra
			) FAT ON C.CODI_TRA=FAT.CODI_TRA
LEFT JOIN ultimo_ano ua ON c.codi_tra=ua.codi_tra
--LEFT JOIN ULTIMA_ALTERACAO_LIMITE AL ON C.CODI_TRA=Al.CODI_TRA
--LEFT JOIN LIMITEOC ATL ON AL.CODI_TRA=ATL.CODI_TRA AND AL.ULTIMA_ALTERACAO=ATL.DATA_LIO
LEFT JOIN GARANTIA G ON C.CODI_TRA=G.CODI_TRA
GROUP BY 
C.CODI_EMP,
LC.CODI_TRA,
T.SITU_TRA,
V.COD1_PES,
PRIMEIRA_COMPRA.DEMI_NOT,
ULTIMA_COMPRA.DEMI_NOT,
MAIOR_COMPRA.data_maior_compra,
MENOR_COMPRA.data_menor_compra,
LC.DUMANUT
--DBMS_LOB.substr(OCOR_LIO, 3000) 
</sql>
