    <connection>[DB] - Oracle - AgriBusiness ERP</connection>
    <sql>
 
 WITH INFO_PEDIDO as ( 
select P.VCTO_PED 
     , P.SITU_PED 
     , I.VLIQ_IPE 
     , ((select QENT 
         from  table(QTDE_ENTR_PED_VEN(I.CODI_EMP, I.PEDI_PED, 
                                          I.SERI_PED, I.CODI_PSV))) + 
        coalesce(I.QPER_IPE, 0)) as QTDE_REC 
     , P.CODI_EMP 
     , P.PEDI_PED 
     , P.SERI_PED 
     , P.CODI_TRA 
     , P.DATA_VLR 
     , P.CODI_IND 
     , PES.NOME_PES 
     , T.RAZA_TRA 
     , P.DEMI_PED 
     , ID.ABRE_IND 
     , T.CGC_TRA 
     , P.COD1_PES 
     , I.CODI_PSV 
     , P.DINI_PED 
     , P.DANT_PED 
     , P.DPON_PED 
     , P.JAPV_PED 
     , P.JATV_PED 
     , P.JURO_PED 
     , I.QTDE_IPE 
     , P.CCFO_CFO 
     , PS.DESC_PSV 
     , CD.OPER_CON 
     , I.DSOF_IPE 
     , I.VDOF_IPE 
     , coalesce((select count(PRF.CODI_PFI) 
                 from PREFINAN PRF 
                 where (PRF.CODI_EMP = P.CODI_EMP) 
                   and (PRF.PEDI_PED = P.PEDI_PED) 
                   and (PRF.SERI_PED = P.SERI_PED)), 0) as QTD_PARC_PREF 
     , (P.DEMI_PED + coalesce(PRF.PRAZ_PFP, 0)) as VENC_PREF 
     , coalesce(PRF.VLOR_PFP, 0) as VLR_PARC_PREF 
     , coalesce((select sum(PRF.VLOR_PFP) 
                 from PREFINAN PRF 
                 where (PRF.CODI_EMP = P.CODI_EMP) 
                   and (PRF.PEDI_PED = P.PEDI_PED) 
                   and (PRF.SERI_PED = P.SERI_PED)), 0) as TOT_PREF, 
     CL.CODI_REG||'-'||REG.DESC_REG AS DESC_REG 
from IPEDIDO I 
join PEDIDO P on (I.PEDI_PED = P.PEDI_PED and 
                  I.SERI_PED = P.SERI_PED and 
                  I.CODI_EMP = P.CODI_EMP) 
left join CFO on (P.CCFO_CFO = CFO.CCFO_CFO) 
left join TRANSAC T on (T.CODI_TRA = P.CODI_TRA) 
left join PRODSERV PS on (PS.CODI_PSV = I.CODI_PSV) 
left join CLIENTE CL on (CL.CODI_TRA = P.CODI_TRA) 
left join INDEXADOR ID on (ID.CODI_IND = P.CODI_IND) 
left join CONDICAO CD on (CD.COND_CON = P.COND_CON) 
left join PESSOAL PES on (PES.CODI_PES = P.COD1_PES) 
left join PREFINAN PRF on (PRF.CODI_EMP = P.CODI_EMP) 
                      and (PRF.PEDI_PED = P.PEDI_PED) 
                      and (PRF.SERI_PED = P.SERI_PED) 
LEFT JOIN REGIAO REG ON (REG.CODI_REG = CL.CODI_REG)

where (P.TFAT_PED in ('1', '3')) 
  and ((CFO.GRFI_CFO = 'S') or (CFO.GRFI_CFO is null)) 
   /*and (P.DEMI_PED between TO_DATE('2020-12-03', 'YYYY-MM-DD') and TO_DATE('2023-12-03', 'YYYY-MM-DD'))*/  AND (P.SITU_PED = '1') and ((('N' = 'S') and                                                      
       (exists (select *                                                                 
                from PREFINAN PRF                                                        
                where (PRF.CODI_EMP = P.CODI_EMP)                                        
                  and (PRF.PEDI_PED = P.PEDI_PED)                                        
                  and (PRF.SERI_PED = P.SERI_PED))) /*and                                  
       ((P.DEMI_PED + coalesce(PRF.PRAZ_PFP, 0))                                         
        between TO_DATE('2020-12-01', 'YYYY-MM-DD') 
            and TO_DATE('2023-12-30', 'YYYY-MM-DD'))*/) or  
      ((('N' &lt;&gt; 'S') or                                                     
        (not exists (select *                                                            
                     from PREFINAN PRF                                                   
                     where (PRF.CODI_EMP = P.CODI_EMP)                                   
                       and (PRF.PEDI_PED = P.PEDI_PED)                                   
                       and (PRF.SERI_PED = P.SERI_PED)))) /*and                            
       (P.VCTO_PED between TO_DATE('2023-12-01', 'YYYY-MM-DD') 
                       and TO_DATE('2023-12-30', 'YYYY-MM-DD'))*/))   and (P.CODI_EMP in ('1','2','3','4','5','6','7','8','9','45','50','51')) 
), 
 
CTE_PROVISAO as ( 


select 
     case 
          when ('N' = 'S') and (INF.QTD_PARC_PREF > 0) then 
             INF.VENC_PREF 
          else 
             INF.VCTO_PED 
       end as VCTO_PED 
     , INF.SITU_PED 
     , case 
          when ('N' = 'S') and (INF.QTD_PARC_PREF > 0) then 
             coalesce(((INF.VLR_PARC_PREF / (case when (INF.TOT_PREF > 0) then INF.TOT_PREF else 1 end)) * ((INF.QTDE_IPE - INF.QTDE_REC) * INF.VLIQ_IPE)), 0) 
          else 
             coalesce(((INF.QTDE_IPE - INF.QTDE_REC) * INF.VLIQ_IPE), 0) / (case when (INF.QTD_PARC_PREF > 0) then INF.QTD_PARC_PREF else 1 end) 
       end as VALOR 
     , INF.CODI_EMP 
     , INF.PEDI_PED 
     , INF.SERI_PED 
     , INF.CODI_TRA 
     , INF.DATA_VLR 
     , INF.CODI_IND 
     , INF.NOME_PES 
     , INF.RAZA_TRA 
     , INF.DEMI_PED 
     , INF.ABRE_IND 
     , INF.CGC_TRA 
     , INF.COD1_PES 
     , INF.CODI_PSV 
     , INF.DINI_PED 
     , INF.DANT_PED 
     , INF.DPON_PED 
     , INF.JAPV_PED 
     , INF.JATV_PED 
     , INF.JURO_PED 
     , case 
          when ('N' = 'S') and (INF.QTD_PARC_PREF > 0) then 
             (INF.QTDE_IPE * (INF.VLR_PARC_PREF / (case when (INF.TOT_PREF > 0) then INF.TOT_PREF else 1 end))) 
          else 
             INF.QTDE_IPE / (case when (INF.QTD_PARC_PREF > 0) then INF.QTD_PARC_PREF else 1 end) 
       end as QTDE_IPE 
     , case 
          when ('N' = 'S') and (INF.QTD_PARC_PREF > 0) then 
             (INF.QTDE_REC * (INF.VLR_PARC_PREF / (case when (INF.TOT_PREF > 0) then INF.TOT_PREF else 1 end))) 
          else 
             INF.QTDE_REC / (case when (INF.QTD_PARC_PREF > 0) then INF.QTD_PARC_PREF else 1 end) 
       end as QTDE_REC 
     , INF.CCFO_CFO 
     , INF.DESC_PSV 
     , INF.OPER_CON 
     , INF.DSOF_IPE 
     , INF.VDOF_IPE 
     , INF.DESC_REG 
from INFO_PEDIDO INF 
) 
 
select 
        TO_CHAR(CTEP.CODI_EMP) AS NK_EMPRESA,
		TO_CHAR(CTEP.PEDI_PED) AS NR_DOC,
		TO_DATE(CTEP.DEMI_PED, 'DD/MM/YYY') AS NK_EMISSAO,
		TO_DATE(CTEP.VCTO_PED, 'DD/MM/YYY') AS NK_VENCIMENTO,
		TO_CHAR(CTEP.CODI_TRA) AS NK_CLIENTE,
		CTEP.QTDE_REC, 
		0              AS VALOR_LIQUIDO,
		SUM(CTEP.QTDE_IPE)       AS QUANTIDADE,
        SUM(CTEP.VALOR) as TOTAL 

from CTE_PROVISAO CTEP 
group by CTEP.CODI_EMP,
         CTEP.PEDI_PED,
