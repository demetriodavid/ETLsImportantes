    <connection>[DB] - Oracle - AgriBusiness ERP</connection>
    <sql>select TO_CHAR(C.CODI_EMP)                                                 AS NK_EMPRESA,
       TO_CHAR(C.CODI_TRA)                                                 AS NK_CLIENTE,
       TO_CHAR(COALESCE(R.COD1_PES, -1))                                   AS NK_VENDEDOR,
       TO_CHAR(R.COND_CON)                                                 AS NK_CONDICAO,
       '-1'                                                                AS NK_TIPO_PAGAMENTO,
       TO_CHAR(C.CODI_TDO)                                                 AS NK_TIPO_DOCUMENTO,
       TO_CHAR(C.DATA_CBR, UPPER('DD/MM/YYYY'))                            AS NK_TEMPO_DIA_INCLUSAO,
       TO_CHAR(R.VENC_REC, UPPER('DD/MM/YYYY'))                            AS NK_TEMPO_DIA_VENC,
       'T'                                                                 AS TIPO,
       '-1'                                                                AS NK_CONTA_FLUXO_CAIXA, 
       COALESCE(C.CTRL_RNC, 0)                                             AS RENEGOCIACAO,
       TO_CHAR('31/12/9999')                                               AS NK_TEMPO_DIA_BAIXA,
       COALESCE(TO_CHAR(N.NOTA_NOT), '-1')                                 AS DD_NOTA_FISCAL,
       C.NUME_CBR                                                          AS DD_TITULO,
       R.NPAR_REC                                                          AS DD_PARCELA,
       0                                                                   AS DD_RENG,
       0                                                                   AS VLR_PRINCIPAL,
       0                                                                   AS VLR_AVISTA,
       0                                                                   AS VLR_JUROS,                               
       0                                                                   AS VLR_DESCONTO,
       0                                                                   AS ACRESCIMO,
       R.VLOR_REC                                                          AS VLR_PARCELA,
       0                                                                   AS VLR_MULTA,
       0                                                                   AS VALOR_BAIXA,
       CASE WHEN TD.TIPO_TDO='C' THEN 
								(SELECT VALOR FROM TABLE(VALOR_ABERTO_RECEBER(R.CTRL_REC)))    *-1 
            ELSE (SELECT VALOR FROM TABLE(VALOR_ABERTO_RECEBER(R.CTRL_REC)))
	   END AS VLR_SALDO

from CABREC C 
        left join  RECEBER R  on (C.CTRL_CBR = R.CTRL_CBR) 
        left outer join TRANSAC T    on (T.CODI_TRA = C.CODI_TRA) 
        left outer join TIPDOC TD     on (TD.CODI_TDO = C.CODI_TDO) 
        left outer join CLIENTE CL    on (CL.CODI_TRA = T.CODI_TRA) 
        left outer join INDEXADOR I on (I.CODI_IND = R.CODI_IND) 
        left outer join PESSOAL P on (P.CODI_PES = R.COD1_PES) 
        left outer join BOLETO BL on (R.CTRL_BOL = BL.CTRL_BOL) 
        left outer join PORTADOR POR on (POR.CODI_POR = R.CODI_POR) 
        left outer join TIPCOB TC on (TC.CODI_TCO = R.CODI_TCO) 
        LEFT JOIN REGIAO REG ON (REG.CODI_REG = CL.CODI_REG) 
        LEFT JOIN NOTACRC  NC  ON NC.CTRL_CBR  = C.CTRL_CBR
        LEFT JOIN NOTA     N   ON N.CODI_EMP   = NC.CODI_EMP  
                               AND N.CODI_TRA  = NC.CODI_TRA 
                               AND N.NOTA_NOT  = NC.NDOC_NOC 
                               AND N.SERI_NOT = NC.SDOC_NOC AND NC.TDOC_NOC  = 'NE'
        
        WHERE --R.VENC_REC = TO_DATE('2023-11-28', 'YYYY-MM-DD') and 
( C.CODI_TDO NOT IN ('3', '6', '103', '106','10000330')) and (C.CODI_EMP in ('1','2','3','4','5','6','7','8','9','50','51')) and ((select VALOR from  table( VALOR_ABERTO_RECEBER(R.CTRL_REC))) > 0) and (R.SITU_REC &lt;&gt; 'C')
--      AND R.VENC_REC >= ?        


</sql>
