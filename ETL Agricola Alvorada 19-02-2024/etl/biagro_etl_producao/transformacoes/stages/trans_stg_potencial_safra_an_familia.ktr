    <connection>Origem_CRM</connection>
    <sql>SELECT DISTINCT &#xd;&#xa;       COALESCE&#x28;CLI.IDPARCEIRO&#x29;                                                                               AS IDPARCEIRO,&#xd;&#xa;       CLI.IDPARCEIRO &#x7c;&#x7c; &#x27;&#x23;&#x27; &#x7c;&#x7c; A.IDGRUPOEMPRESARIAL                                                          AS NK_CLIENTE,&#xd;&#xa;       A.ID_USUARIO_EVENTO                                                                                    AS IDPARCEIRO_CONSULTOR,&#xd;&#xa;       A.IDCULTURA                                                                                            AS IDCULTURA,&#xd;&#xa;       A.IDFILIAL_EVENTO                                                                                      AS IDFILIAL_FATO,&#xd;&#xa;       A.IDFILIAL_EVENTO&#x7c;&#x7c; &#x27;&#x23;&#x27; &#x7c;&#x7c; A.IDGRUPOEMPRESARIAL                                                        AS NK_EMPRESA,&#xd;&#xa;       A.IDFILIAL_COBERTURA                                                                                   AS IDFILIAL_COBERTURA,&#xd;&#xa;       A.IDGRUPOEMPRESARIAL                                                                                   AS IDGRUPOEMPRESARIAL_FATO,&#xd;&#xa;       A.IDGRUPOEMPRESARIAL                                                                                   AS IDGRUPOEMPRESARIAL_COBERTURA,&#xd;&#xa;       CLP.DESCRICAO                                                                                          AS CLASSE_PRODUTO,&#xd;&#xa;       CLP.IDCLASSEDEPRODUTO,&#xd;&#xa;       COALESCE&#x28;CAST&#x28;CLP.IDCLASSEDEPRODUTO AS VARCHAR&#x29; &#x7c;&#x7c;&#x27;&#x23;&#x27; &#x7c;&#x7c; CAST&#x28;A.IDGRUPOEMPRESARIAL AS VARCHAR&#x29;,&#x27;NI&#x23;0&#x27;&#x29; AS NK_FAMILIA, -- &#xd;&#xa;       CAST&#x28;A.DATA_REALIZADA AS DATE&#x29;                                                                         AS DATA_INICIO,&#xd;&#xa;       COALESCE&#x28;SAF.CODIGO &#x7c;&#x7c; &#x27;&#x23;&#x27; &#x7c;&#x7c; TO_CHAR&#x28;SAF.DATAINICIO, &#x27;YYYY&#x27;&#x29; &#x7c;&#x7c; &#x27;&#x23;&#x27; &#x7c;&#x7c; C.CODIGO, &#x27;NI&#x23;0&#x23;0&#x27;&#x29;            AS NK_TEMPO_SAFRA,&#xd;&#xa;       COALESCE&#x28;&#x28;A.ID_USUARIO_EVENTO &#x7c;&#x7c; &#x27;&#x23;&#x27; &#x7c;&#x7c; A.IDFILIAL_EVENTO &#x7c;&#x7c; &#x27;&#x23;&#x27; &#x7c;&#x7c; A.IDGRUPOEMPRESARIAL&#x29;,&#x27;NI&#x23;0&#x23;0&#x27;&#x29;    AS NK_USUARIO,&#xd;&#xa;       COALESCE&#x28;&#x28;A.ID_USUARIO_EVENTO &#x7c;&#x7c; &#x27;&#x23;&#x27; &#x7c;&#x7c; A.IDGRUPOEMPRESARIAL&#x29;,&#x27;NI&#x23;0&#x27;&#x29;                                  AS NK_CONSULTOR,&#xd;&#xa;       &#x28;COALESCE&#x28;CAST&#x28;P.IDPROPRIEDADE AS VARCHAR&#x29;,&#x27;NI&#x27;&#x29; &#x7c;&#x7c; &#x27;&#x23;&#x27; &#x7c;&#x7c; A.IDGRUPOEMPRESARIAL&#x29;                       AS NK_PROPRIEDADE,&#xd;&#xa;       COALESCE&#x28;CAST&#x28;AN.IDAREADENEGOCIO AS VARCHAR&#x29;, &#x27;NI&#x23;0&#x27;&#x29;                                                  AS NK_AREA_NEGOCIO,&#xd;&#xa;       CAST&#x28;A.DATA_REALIZADA AS DATE&#x29;                                                                         AS DATA_CADASTRO,&#xd;&#xa;       PI.IDPLANODEINSUMO,                                                                                    &#xd;&#xa;       &#x28;NT.IDNIVELTECNOLOGICO &#x7c;&#x7c;&#x27;&#x23;&#x27;&#x7c;&#x7c; NT.IDGRUPOEMPRESARIAL&#x29;                                                  AS NIVEL_TECNOLOGICO,&#xd;&#xa;       POSSE.IDPOSSE,&#xd;&#xa;       ARRENDAMENTO.IDARRENDAMENTO,&#xd;&#xa; &#xd;&#xa;        CASE WHEN POSSE.IDPOSSE IS NOT NULL&#xd;&#xa;             THEN PI.AREA &#xd;&#xa;             ELSE 0 END                                                                                       AS AREA_PROPRIA,&#xd;&#xa;         CASE WHEN ARRENDAMENTO.IDARRENDAMENTO IS NOT NULL&#xd;&#xa;             THEN PI.AREA &#xd;&#xa;             ELSE 0 END                                                                                       AS AREA_ARRENDADA,&#xd;&#xa;    &#xd;&#xa;        CASE WHEN POSSE.IDPOSSE IS NOT NULL &#xd;&#xa;             THEN &#x28; &#xd;&#xa;                     CASE &#x3f; &#xd;&#xa;                        WHEN &#x27;DOLAR&#x27; THEN &#x28;PI.AREA &#x2a; COALESCE&#x28;CPNT.VALOREMDOLAR&#x29;&#x29;&#xd;&#xa;                                     ELSE coalesce&#x28;&#x28;PI.AREA &#x2a; COALESCE&#x28;CPNT.VALOR,PNT.VALOR,PI.POTENCIALDECOMPRA,0&#x29;&#x29;, PI.POTENCIALDECOMPRATOTAL&#x29; &#xd;&#xa;                        END&#xd;&#xa;                   &#x29;&#xd;&#xa;             &#xd;&#xa;             ELSE 0 END                                                                                       AS POTENCIAL_COMPRA_PROPRIA,&#xd;&#xa;         CASE WHEN ARRENDAMENTO.IDARRENDAMENTO IS NOT NULL&#xd;&#xa;             THEN  &#x28; &#xd;&#xa;                     CASE &#x3f; &#xd;&#xa;                        WHEN &#x27;DOLAR&#x27; THEN &#x28;PI.AREA &#x2a; COALESCE&#x28;CPNT.VALOREMDOLAR&#x29;&#x29;&#xd;&#xa;                                     ELSE coalesce&#x28;&#x28;PI.AREA &#x2a; COALESCE&#x28;CPNT.VALOR,PNT.VALOR,PI.POTENCIALDECOMPRA,0&#x29;&#x29;, PI.POTENCIALDECOMPRATOTAL&#x29; &#xd;&#xa;                        END&#xd;&#xa;                   &#x29; &#xd;&#xa;             ELSE 0 END                                                                                       AS POTENCIAL_COMPRA_ARRENDADA,&#xd;&#xa;       &#x28;PI.AREA&#x29;                                                                                              AS AREA,&#xd;&#xa;       CASE &#x3f;&#xd;&#xa;            WHEN &#x27;DOLAR&#x27; THEN &#x28;PI.AREA &#x2a; COALESCE&#x28;CPNT.VALOREMDOLAR&#x29;&#x29; &#xd;&#xa;                         ELSE coalesce&#x28;&#x28;PI.AREA &#x2a; COALESCE&#x28;CPNT.VALOR,PNT.VALOR,PI.POTENCIALDECOMPRA,0&#x29;&#x29;, PI.POTENCIALDECOMPRATOTAL&#x29;&#xd;&#xa;            END                                                                                               AS POTENCIAL_COMPRA, -- POTENCIAL DE COMPRA DE INSUMO POR CLASSE DE PRODUTO&#xd;&#xa;       &#x28;PI.AREA &#x2a; COALESCE&#x28;CPNT.VALOREMDOLAR&#x29;&#x29;                                                                AS POTENCIAL_COMPRA_DOLAR,&#xd;&#xa;       &#x28;PI.METACIF&#x29;                                                                                           AS META_CIF, -- QUANTIDADE QUE PRODUTOR VAI ENTREGAR NA COOPERATIVA&#xd;&#xa;       &#x28;PI.METAFOB&#x29;                                                                                           AS META_FOB, -- QUANDO COOPERATIVA VAI BUSCAR NA LAVOURA &#x28;QTD&#x29;&#xd;&#xa;       &#x28;PI.METATERCEIRO&#x29;                                                                                      AS META_TERCEIRO, -- META DO DISPON&#xcd;VEL &#x28;PRODUTOR TEM SILO NA PROPRIEDADE&#x29;&#xd;&#xa;       COALESCE&#x28;PI.VOLUMETON,0&#x29;                                                                               AS POTENCIAL_VOLUME --  POTENCIAL EM TONELADAS&#xd;&#xa;  FROM PLANODEINSUMO PI&#xd;&#xa;        INNER JOIN VIEW_ATIVIDADES_SEM_ATENDIMENTO           A    ON PI.IDPLANODEINSUMO               &#x3d; A.IDATIVIDADE&#xd;&#xa;        --INNER JOIN SAFRA                                     S    ON S.IDSAFRA                        &#x3d; A.IDSAFRA&#xd;&#xa;         LEFT JOIN VINCULOIMOBILIARIO                        VI   ON VI.IDVINCULOIMOBILIARIO          &#x3d; PI.IDVINCULOIMOBILIARIO&#xd;&#xa;         LEFT JOIN ARRENDAMENTO                                   ON ARRENDAMENTO.IDARRENDAMENTO      &#x3d; VI.IDVINCULOIMOBILIARIO&#xd;&#xa;         LEFT JOIN POSSE                                          ON POSSE.IDPOSSE                    &#x3d; VI.IDVINCULOIMOBILIARIO&#xd;&#xa;         LEFT JOIN LOCAL                                     LVI  ON LVI.IDLOCAL                      &#x3d; VI.IDLOCAL&#xd;&#xa;         LEFT JOIN PROPRIEDADE                               P    ON P.IDPROPRIEDADE                  &#x3d; A.IDPROPRIEDADE&#xd;&#xa;         LEFT JOIN USUARIO                                   U    ON U.IDUSUARIO                      &#x3d; A.ID_USUARIO_EVENTO &#xd;&#xa;         LEFT JOIN PARCEIRO                                  CLI  ON CLI.IDPARCEIRO                   &#x3d; A.IDCLIENTE_EXIBICAO&#xd;&#xa;         LEFT JOIN SAFRA                                     SAF  ON SAF.IDSAFRA                      &#x3d; A.IDSAFRA&#xd;&#xa;         LEFT JOIN CULTURA                                   C    ON C.IDCULTURA                      &#x3d; A.IDCULTURA&#xd;&#xa;         LEFT JOIN AREADENEGOCIO                             AN   ON AN.IDAREADENEGOCIO               &#x3d; A.ID_AREA_DE_NEGOCIO&#xd;&#xa;         LEFT JOIN NIVELTECNOLOGICO                          NT   ON NT.IDNIVELTECNOLOGICO            &#x3d; PI.IDNIVELTECNOLOGICO&#xd;&#xa;         LEFT JOIN LOCAL                                      L  ON L.IDLOCAL                         &#x3d; A.IDLOCAL&#xd;&#xa;         LEFT JOIN POTENCIALNIVELTECNOLOGICO                PNT   ON PNT.IDSAFRA                      &#x3d; A.IDSAFRA&#xd;&#xa;                                                                 AND PNT.IDNIVELTECNOLOGICO           &#x3d; NT.IDNIVELTECNOLOGICO&#xd;&#xa;                                                                 AND PNT.IDFILIAL                     &#x3d; A.IDFILIAL_EVENTO&#xd;&#xa;         LEFT  JOIN CLASSEDEPRODUTOPOTENCIALNIVELTECNOLOGICO CPNT ON CPNT.IDPOTENCIALNIVELTECNOLOGICO &#x3d; PNT.IDPOTENCIALNIVELTECNOLOGICO&#xd;&#xa;         LEFT  JOIN CLASSEDEPRODUTO                          CLP  ON CLP.IDCLASSEDEPRODUTO            &#x3d; CPNT.IDCLASSEDEPRODUTO&#xd;&#xa; WHERE CAST&#x28;A.DATA_REALIZADA AS DATE&#x29; &#x3e;&#x3d; CAST&#x28; &#x3f; AS DATE&#x29;&#xd;&#xa;   AND &#x28;&#xd;&#xa;          &#x28;&#xd;&#xa;            NOT POSSE.IDPOSSE IS NULL&#xd;&#xa;            AND CURRENT_DATE &#x3c;&#x3d; COALESCE&#x28;POSSE.DATAVENDA, CURRENT_DATE&#x29;&#xd;&#xa;          &#x29;&#xd;&#xa;        OR&#x28;&#xd;&#xa;            NOT ARRENDAMENTO.IDARRENDAMENTO IS NULL&#xd;&#xa;            AND CURRENT_DATE &#x3c;&#x3d; COALESCE&#x28;ARRENDAMENTO.DATATERMINO, CURRENT_DATE&#x29;&#xd;&#xa;          &#x29;&#xd;&#xa;       &#x29;</sql>
