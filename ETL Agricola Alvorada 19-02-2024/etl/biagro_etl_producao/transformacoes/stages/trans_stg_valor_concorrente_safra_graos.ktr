    <connection>Vistra_dw</connection>
    <sql>WITH CONCORRENTES AS (
SELECT F.*
  FROM FORMULARIO_COLETA_MERCADO_CONCORRENTES_MILHO_GRAOS  F

UNION ALL

SELECT F.*
  FROM FORMULARIO_COLETA_MERCADO_CONCORRENTES_SOJA_GRAOS  F
 )

SELECT X.NK_CONSULTOR,
       X.NK_EMPRESA,
       X.NK_CLIENTE,
       X.NK_PROPRIEDADE,
       X.NK_TEMPO_SAFRA,
       X.CONCORRENTE,
       X.MERCADO,
       X.MERCADO_PARCIAL,
       SUM(X.VALOR)     AS VALOR,
       SUM(X.POTENCIAL) AS POTENCIAL
  FROM (
        SELECT DCO.NK_CONSULTOR,
               DE.NK_EMPRESA,
               DC.NK_CLIENTE,
               DPR.NK_PROPRIEDADE,
               DTS.NK_TEMPO_SAFRA,
               CO.CONCORRENTE1::INTEGER                                                           AS CONCORRENTE,
               CASE WHEN CO.PERC_FECHADO1 IS NULL
                       THEN 'SEM INFORMAÇÃO'
                    WHEN (COALESCE(CO.PERC_FECHADO1,0)) / 100 = 0 
                       THEN 'ABERTO'
                    WHEN (COALESCE(CO.PERC_FECHADO1,0)) / 100 > 0 AND (COALESCE(CO.PERC_FECHADO1,0)) / 100 &lt; 1
                       THEN 'FECHADO' -- DEPOIS JOGO TUDO PRO ELSE FECHADO
                    WHEN (COALESCE(CO.PERC_FECHADO1,0)) / 100 = 1 
                       THEN 'FECHADO'
               END                                                                                AS MERCADO,
               CASE WHEN (COALESCE(CO.PERC_FECHADO1,0)) / 100 > 0 AND (COALESCE(CO.PERC_FECHADO1,0)) / 100 &lt; 1
                       THEN 'PARCIAL'
                    ELSE NULL
               END                                                                                AS MERCADO_PARCIAL,              
               COALESCE(SUM(FPF.PRODUTIVIDADE_ESPERADA),0) * (COALESCE(CO.PERC_FECHADO1,0) / 100) AS VALOR,
               COALESCE(SUM(FPF.PRODUTIVIDADE_ESPERADA),0)                                        AS POTENCIAL
          FROM FATO_COBERTURA_CONSULTOR_LOCAL FC
               INNER JOIN DIM_CONSULTOR                               DCO ON DCO.SK_CONSULTOR     = FC.SK_CONSULTOR
               INNER JOIN DIM_EMPRESA                                  DE USING(SK_EMPRESA)
               INNER JOIN DIM_CLIENTE                                  DC USING(SK_CLIENTE)
               INNER JOIN FATO_PROPRIEDADE_LOCAL                      FPL ON FPL.SK_CLIENTE_LOCAL = FC.SK_CLIENTE_LOCAL
               INNER JOIN DIM_PROPRIEDADE                             DPR ON DPR.SK_PROPRIEDADE   = FPL.SK_PROPRIEDADE       
               INNER JOIN FATO_POTENCIAL_CONSULTOR_SAFRA_AREA_NEGOCIO FPF ON FPF.SK_CLIENTE       = FC.SK_CLIENTE
                                                                         AND FPF.SK_PROPRIEDADE   = FPL.SK_PROPRIEDADE
               INNER JOIN CONCORRENTES                                 CO ON CO.CONSULTOR         = FC.SK_CONSULTOR
                                                                         AND CO.CLIENTE           = FC.SK_CLIENTE
                                                                         AND CO.PROPRIEDADE       = FPL.SK_PROPRIEDADE
                                                                         AND CO.SAFRA             = FPF.SK_TEMPO_SAFRA
               INNER JOIN DIM_TEMPO_SAFRA                             DTS USING(SK_TEMPO_SAFRA)
         WHERE DCO.PERFIL_DE_ACESSO_WEB ILIKE 'ORIGINAÇÃO'
           AND DC.TIPO_REVENDA             IN ('VENDA DIRETA','POOL DE COMPRA','DISTRIBUIÇÃO','CIA AGRICOLA','ARRENDANTE')
         GROUP BY 1,2,3,4,5,6,CO.PERC_FECHADO1

     UNION ALL

        SELECT DCO.NK_CONSULTOR,
               DE.NK_EMPRESA,
               DC.NK_CLIENTE,
               DPR.NK_PROPRIEDADE,
               DTS.NK_TEMPO_SAFRA,
               CO.CONCORRENTE2::INTEGER                                                           AS CONCORRENTE,
               CASE WHEN CO.PERC_FECHADO2 IS NULL
                       THEN 'SEM INFORMAÇÃO'
                    WHEN (COALESCE(CO.PERC_FECHADO2,0)) / 100 = 0 
                       THEN 'ABERTO'
                    WHEN (COALESCE(CO.PERC_FECHADO2,0)) / 100 > 0 AND (COALESCE(CO.PERC_FECHADO2,0)) / 100 &lt; 1
                       THEN 'FECHADO' -- DEPOIS JOGO TUDO PRO ELSE FECHADO
                    WHEN (COALESCE(CO.PERC_FECHADO2,0)) / 100 = 1 
                       THEN 'FECHADO'
               END                                                                                AS MERCADO,
               CASE WHEN (COALESCE(CO.PERC_FECHADO2,0)) / 100 > 0 AND (COALESCE(CO.PERC_FECHADO2,0)) / 100 &lt; 1
                       THEN 'PARCIAL'
                    ELSE NULL
               END                                                                                AS MERCADO_PARCIAL,              
               COALESCE(SUM(FPF.PRODUTIVIDADE_ESPERADA),0) * (COALESCE(CO.PERC_FECHADO2,0) / 100) AS VALOR,
               0                                                                                  AS POTENCIAL
          FROM FATO_COBERTURA_CONSULTOR_LOCAL FC
               INNER JOIN DIM_CONSULTOR                               DCO ON DCO.SK_CONSULTOR     = FC.SK_CONSULTOR
               INNER JOIN DIM_EMPRESA                                  DE USING(SK_EMPRESA)
               INNER JOIN DIM_CLIENTE                                  DC USING(SK_CLIENTE)
               INNER JOIN FATO_PROPRIEDADE_LOCAL                      FPL ON FPL.SK_CLIENTE_LOCAL = FC.SK_CLIENTE_LOCAL
               INNER JOIN DIM_PROPRIEDADE                             DPR ON DPR.SK_PROPRIEDADE   = FPL.SK_PROPRIEDADE       
               INNER JOIN FATO_POTENCIAL_CONSULTOR_SAFRA_AREA_NEGOCIO FPF ON FPF.SK_CLIENTE       = FC.SK_CLIENTE
                                                                         AND FPF.SK_PROPRIEDADE   = FPL.SK_PROPRIEDADE
               INNER JOIN CONCORRENTES                                 CO ON CO.CONSULTOR         = FC.SK_CONSULTOR
                                                                         AND CO.CLIENTE           = FC.SK_CLIENTE
                                                                         AND CO.PROPRIEDADE       = FPL.SK_PROPRIEDADE
                                                                         AND CO.SAFRA             = FPF.SK_TEMPO_SAFRA
               INNER JOIN DIM_TEMPO_SAFRA                             DTS USING(SK_TEMPO_SAFRA)
         WHERE DCO.PERFIL_DE_ACESSO_WEB ILIKE 'ORIGINAÇÃO'
           AND DC.TIPO_REVENDA             IN ('VENDA DIRETA','POOL DE COMPRA','DISTRIBUIÇÃO','CIA AGRICOLA','ARRENDANTE')
         GROUP BY 1,2,3,4,5,6,CO.PERC_FECHADO2

       UNION ALL

        SELECT DCO.NK_CONSULTOR,
               DE.NK_EMPRESA,
               DC.NK_CLIENTE,
               DPR.NK_PROPRIEDADE,
               DTS.NK_TEMPO_SAFRA,
               CO.CONCORRENTE3::INTEGER                                                           AS CONCORRENTE,
               CASE WHEN CO.PERC_FECHADO3 IS NULL
                       THEN 'SEM INFORMAÇÃO'
                    WHEN (COALESCE(CO.PERC_FECHADO3,0)) / 100 = 0 
                       THEN 'ABERTO'
                    WHEN (COALESCE(CO.PERC_FECHADO3,0)) / 100 > 0 AND (COALESCE(CO.PERC_FECHADO3,0)) / 100 &lt; 1
                       THEN 'FECHADO' -- DEPOIS JOGO TUDO PRO ELSE FECHADO
                    WHEN (COALESCE(CO.PERC_FECHADO3,0)) / 100 = 1 
                       THEN 'FECHADO'
               END                                                                                AS MERCADO,
               CASE WHEN (COALESCE(CO.PERC_FECHADO3,0)) / 100 > 0 AND (COALESCE(CO.PERC_FECHADO3,0)) / 100 &lt; 1
                       THEN 'PARCIAL'
                    ELSE NULL
               END                                                                                AS MERCADO_PARCIAL,              
               COALESCE(SUM(FPF.PRODUTIVIDADE_ESPERADA),0) * (COALESCE(CO.PERC_FECHADO3,0) / 100) AS VALOR,
               0                                                                                  AS POTENCIAL
          FROM FATO_COBERTURA_CONSULTOR_LOCAL FC
               INNER JOIN DIM_CONSULTOR                               DCO ON DCO.SK_CONSULTOR     = FC.SK_CONSULTOR
               INNER JOIN DIM_EMPRESA                                  DE USING(SK_EMPRESA)
               INNER JOIN DIM_CLIENTE                                  DC USING(SK_CLIENTE)
               INNER JOIN FATO_PROPRIEDADE_LOCAL                      FPL ON FPL.SK_CLIENTE_LOCAL = FC.SK_CLIENTE_LOCAL
               INNER JOIN DIM_PROPRIEDADE                             DPR ON DPR.SK_PROPRIEDADE   = FPL.SK_PROPRIEDADE       
               INNER JOIN FATO_POTENCIAL_CONSULTOR_SAFRA_AREA_NEGOCIO FPF ON FPF.SK_CLIENTE       = FC.SK_CLIENTE
                                                                         AND FPF.SK_PROPRIEDADE   = FPL.SK_PROPRIEDADE
               INNER JOIN CONCORRENTES                                 CO ON CO.CONSULTOR         = FC.SK_CONSULTOR
                                                                         AND CO.CLIENTE           = FC.SK_CLIENTE
                                                                         AND CO.PROPRIEDADE       = FPL.SK_PROPRIEDADE
                                                                         AND CO.SAFRA             = FPF.SK_TEMPO_SAFRA
               INNER JOIN DIM_TEMPO_SAFRA                             DTS USING(SK_TEMPO_SAFRA)
         WHERE DCO.PERFIL_DE_ACESSO_WEB ILIKE 'ORIGINAÇÃO'
           AND DC.TIPO_REVENDA             IN ('VENDA DIRETA','POOL DE COMPRA','DISTRIBUIÇÃO','CIA AGRICOLA','ARRENDANTE')
         GROUP BY 1,2,3,4,5,6,CO.PERC_FECHADO3
       ) X
 GROUP BY 1,2,3,4,5,6,7,8</sql>
