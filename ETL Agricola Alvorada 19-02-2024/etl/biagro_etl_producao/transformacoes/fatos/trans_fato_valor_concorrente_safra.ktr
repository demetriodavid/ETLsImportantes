    <sql>


/*
SELECT DTS.SK_TEMPO_SAFRA,
       DE.SK_EMPRESA,
       DCO.SK_CONSULTOR,
       DC.SK_CLIENTE,
       DPR.SK_PROPRIEDADE,
       CO.SK_CONCORRENTE,
       S.MERCADO,
       CASE S.MERCADO 
           WHEN 'SEM INFORMAÇÃO' THEN SUM(S.POT_DEFENSIVOS)
           WHEN 'FECHADO'        THEN SUM(S.VALOR_DEFENSIVOS)
           WHEN 'ABERTO'         THEN SUM(S.POT_DEFENSIVOS) - COALESCE(SUM(S.VALOR_DEFENSIVOS),0)
           WHEN 'PARCIAL'        THEN SUM(S.POT_DEFENSIVOS) - COALESCE(SUM(S.VALOR_DEFENSIVOS),0)
       END                                             AS VALOR_DEFENSIVOS,
       CASE S.MERCADO 
           WHEN 'SEM INFORMAÇÃO' THEN SUM(S.POT_FERTILIZANTES)
           WHEN 'FECHADO'        THEN SUM(S.VALOR_FERTILIZANTES)
           WHEN 'ABERTO'         THEN SUM(S.POT_FERTILIZANTES) - COALESCE(SUM(S.VALOR_FERTILIZANTES),0)
           WHEN 'PARCIAL'        THEN SUM(S.POT_FERTILIZANTES) - COALESCE(SUM(S.VALOR_FERTILIZANTES),0)
       END                                             AS VALOR_FERTILIZANTES,
       CASE S.MERCADO 
           WHEN 'SEM INFORMAÇÃO' THEN SUM(S.POT_SEMENTES)
           WHEN 'FECHADO'        THEN SUM(S.VALOR_SEMENTES)
           WHEN 'ABERTO'         THEN SUM(S.POT_SEMENTES) - COALESCE(SUM(S.VALOR_SEMENTES),0)
           WHEN 'PARCIAL'        THEN SUM(S.POT_SEMENTES) - COALESCE(SUM(S.VALOR_SEMENTES),0)
       END                                             AS VALOR_SEMENTES,
       CASE S.MERCADO 
           WHEN 'SEM INFORMAÇÃO' THEN SUM(S.POT_ESPECIALIDADES)
           WHEN 'FECHADO'        THEN SUM(S.VALOR_ESPECIALIDADES)
           WHEN 'ABERTO'         THEN SUM(S.POT_ESPECIALIDADES) - COALESCE(SUM(S.VALOR_ESPECIALIDADES),0)
           WHEN 'PARCIAL'        THEN SUM(S.POT_ESPECIALIDADES) - COALESCE(SUM(S.VALOR_ESPECIALIDADES),0)
       END                                             AS VALOR_ESPECIALIDADES,
       COALESCE(SUM(S.POT_DEFENSIVOS),0)               AS POT_DEFENSIVOS,
       COALESCE(SUM(S.POT_FERTILIZANTES),0)            AS POT_FERTILIZANTES,
       COALESCE(SUM(S.POT_SEMENTES),0)                 AS POT_SEMENTES,
       COALESCE(SUM(S.POT_ESPECIALIDADES),0)           AS POT_ESPECIALIDADES
  FROM STG_VALOR_CONCORRENTE_SAFRA S
       INNER JOIN DIM_TEMPO_SAFRA DTS USING(NK_TEMPO_SAFRA)
       INNER JOIN DIM_EMPRESA      DE USING(NK_EMPRESA)
       INNER JOIN DIM_CONSULTOR   DCO USING(NK_CONSULTOR)
       INNER JOIN DIM_CLIENTE      DC USING(NK_CLIENTE)
       INNER JOIN DIM_PROPRIEDADE DPR USING(NK_PROPRIEDADE)
       INNER JOIN DIM_CONCORRENTE  CO ON CO.NK_CONCORRENTE  = S.CONCORRENTE
 GROUP BY 1,2,3,4,5,6,7
*/

SELECT X.SK_TEMPO_SAFRA,
       X.SK_EMPRESA,
       X.SK_CONSULTOR,
       X.SK_CLIENTE,
       X.SK_PROPRIEDADE,
       X.SK_CONCORRENTE,
       X.MERCADO,
       SUM(X.VALOR_DEFENSIVOS)     AS VALOR_DEFENSIVOS,
       SUM(X.VALOR_FERTILIZANTES)  AS VALOR_FERTILIZANTES,
       SUM(X.VALOR_SEMENTES)       AS VALOR_SEMENTES,
       SUM(X.VALOR_ESPECIALIDADES) AS VALOR_ESPECIALIDADES,
       SUM(X.POT_DEFENSIVOS)       AS POT_DEFENSIVOS,
       SUM(X.POT_FERTILIZANTES)    AS POT_FERTILIZANTES,
       SUM(X.POT_SEMENTES)         AS POT_SEMENTES,
       SUM(X.POT_ESPECIALIDADES)   AS POT_ESPECIALIDADES
  FROM (
        SELECT DTS.SK_TEMPO_SAFRA,
               DE.SK_EMPRESA,
               DCO.SK_CONSULTOR,
               DC.SK_CLIENTE,
               DPR.SK_PROPRIEDADE,
               COALESCE(CO.SK_CONCORRENTE, CON.SK_CONCORRENTE)                     AS SK_CONCORRENTE,
               S.MERCADO,
               CASE S.MERCADO 
                   WHEN 'SEM INFORMAÇÃO' THEN SUM(S.POT_DEFENSIVOS)
                   WHEN 'FECHADO'        THEN SUM(S.VALOR_DEFENSIVOS)
                   WHEN 'ABERTO'         THEN SUM(S.POT_DEFENSIVOS) - COALESCE(SUM(S.VALOR_DEFENSIVOS),0)
               END                                             AS VALOR_DEFENSIVOS,
               CASE S.MERCADO 
                   WHEN 'SEM INFORMAÇÃO' THEN SUM(S.POT_FERTILIZANTES)
                   WHEN 'FECHADO'        THEN SUM(S.VALOR_FERTILIZANTES)
                   WHEN 'ABERTO'         THEN SUM(S.POT_FERTILIZANTES) - COALESCE(SUM(S.VALOR_FERTILIZANTES),0)
               END                                             AS VALOR_FERTILIZANTES,
               CASE S.MERCADO 
                   WHEN 'SEM INFORMAÇÃO' THEN SUM(S.POT_SEMENTES)
                   WHEN 'FECHADO'        THEN SUM(S.VALOR_SEMENTES)
                   WHEN 'ABERTO'         THEN SUM(S.POT_SEMENTES) - COALESCE(SUM(S.VALOR_SEMENTES),0)
               END                                             AS VALOR_SEMENTES,
               CASE S.MERCADO 
                   WHEN 'SEM INFORMAÇÃO' THEN SUM(S.POT_ESPECIALIDADES)
                   WHEN 'FECHADO'        THEN SUM(S.VALOR_ESPECIALIDADES)
                   WHEN 'ABERTO'         THEN SUM(S.POT_ESPECIALIDADES) - COALESCE(SUM(S.VALOR_ESPECIALIDADES),0)
               END                                             AS VALOR_ESPECIALIDADES,
               COALESCE(SUM(S.POT_DEFENSIVOS),0)               AS POT_DEFENSIVOS,
               COALESCE(SUM(S.POT_FERTILIZANTES),0)            AS POT_FERTILIZANTES,
               COALESCE(SUM(S.POT_SEMENTES),0)                 AS POT_SEMENTES,
               COALESCE(SUM(S.POT_ESPECIALIDADES),0)           AS POT_ESPECIALIDADES
          FROM STG_VALOR_CONCORRENTE_SAFRA S
               INNER JOIN DIM_TEMPO_SAFRA DTS USING(NK_TEMPO_SAFRA)
               INNER JOIN DIM_EMPRESA      DE USING(NK_EMPRESA)
               INNER JOIN DIM_CONSULTOR   DCO USING(NK_CONSULTOR)
               INNER JOIN DIM_CLIENTE      DC USING(NK_CLIENTE)
               INNER JOIN DIM_PROPRIEDADE DPR USING(NK_PROPRIEDADE)
                LEFT JOIN DIM_CONCORRENTE  CO ON CO.NK_CONCORRENTE  = S.CONCORRENTE
               INNER JOIN DIM_CONCORRENTE CON ON CON.NK_CONCORRENTE = 0
         WHERE DCO.PERFIL_DE_ACESSO_WEB ILIKE 'RTV'
         GROUP BY 1,2,3,4,5,6,7

     UNION ALL

        SELECT DTS.SK_TEMPO_SAFRA,
               DE.SK_EMPRESA,
               DCO.SK_CONSULTOR,
               DC.SK_CLIENTE,
               DPR.SK_PROPRIEDADE,
               COALESCE(CO.SK_CONCORRENTE, CON.SK_CONCORRENTE)                     AS SK_CONCORRENTE,
               'ABERTO'                                                            AS MERCADO,
               SUM(S.POT_DEFENSIVOS) - COALESCE(SUM(S.VALOR_DEFENSIVOS),0)         AS VALOR_DEFENSIVOS,
               SUM(S.POT_FERTILIZANTES) - COALESCE(SUM(S.VALOR_FERTILIZANTES),0)   AS VALOR_FERTILIZANTES,
               SUM(S.POT_SEMENTES) - COALESCE(SUM(S.VALOR_SEMENTES),0)             AS VALOR_SEMENTES,
               SUM(S.POT_ESPECIALIDADES) - COALESCE(SUM(S.VALOR_ESPECIALIDADES),0) AS VALOR_ESPECIALIDADES,
               COALESCE(SUM(S.POT_DEFENSIVOS),0)                                   AS POT_DEFENSIVOS,
               COALESCE(SUM(S.POT_FERTILIZANTES),0)                                AS POT_FERTILIZANTES,
               COALESCE(SUM(S.POT_SEMENTES),0)                                     AS POT_SEMENTES,
               COALESCE(SUM(S.POT_ESPECIALIDADES),0)                               AS POT_ESPECIALIDADES
          FROM STG_VALOR_CONCORRENTE_SAFRA S
               INNER JOIN DIM_TEMPO_SAFRA DTS USING(NK_TEMPO_SAFRA)
               INNER JOIN DIM_EMPRESA      DE USING(NK_EMPRESA)
               INNER JOIN DIM_CONSULTOR   DCO USING(NK_CONSULTOR)
               INNER JOIN DIM_CLIENTE      DC USING(NK_CLIENTE)
               INNER JOIN DIM_PROPRIEDADE DPR USING(NK_PROPRIEDADE)
                LEFT JOIN DIM_CONCORRENTE  CO ON CO.NK_CONCORRENTE  = S.CONCORRENTE
               INNER JOIN DIM_CONCORRENTE CON ON CON.NK_CONCORRENTE = 0
         WHERE S.MERCADO_PARCIAL        ILIKE 'PARCIAL'
           AND DCO.PERFIL_DE_ACESSO_WEB ILIKE 'RTV'
         GROUP BY 1,2,3,4,5,6,7
     )X
 GROUP BY 1,2,3,4,5,6,7</sql>
