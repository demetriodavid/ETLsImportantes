  <step>
    <name>Input API Recimento Grãos</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>Vistra_dw</connection>
    <sql>SELECT X.SK_EMPRESA,
       X.SK_TEMPO_SAFRA,
       X.SK_PRODUTO,
       X.SK_CLIENTE_LOCAL,
       SUM(X.COMPRA_CIF)          AS COMPRA_CIF,
       SUM(X.COMPRA_FOB)          AS COMPRA_FOB,
       SUM(X.VALOR_SERVICO)       AS VALOR_SERVICO,
       SUM(X.COMPRA_CIF_BUNGE)    AS COMPRA_CIF_BUNGE,
       SUM(X.COMPRA_FOB_BUNGE)    AS COMPRA_FOB_BUNGE,
       SUM(X.VALOR_SERVICO_BUNGE) AS VALOR_SERVICO_BUNGE
  FROM (
        SELECT COALESCE(DE.FILIAL_PADRAO, DE.SK_EMPRESA)             AS SK_EMPRESA,
               DTS.SK_TEMPO_SAFRA,
               DP.SK_PRODUTO,
               COALESCE(DCL.SK_CLIENTE_LOCAL, DCL2.SK_CLIENTE_LOCAL) AS SK_CLIENTE_LOCAL,
               SUM(CASE WHEN FTC.COMPRA_OU_SERVICO ILIKE 'COMPRA' AND API.TIPO_FRETE ILIKE 'CIF'
                           THEN COALESCE(API.QTD_LIQUIDA,0)
                        ELSE 0
                   END)                                              AS COMPRA_CIF,
               SUM(CASE WHEN FTC.COMPRA_OU_SERVICO ILIKE 'COMPRA' AND API.TIPO_FRETE ILIKE 'FOB'
                           THEN COALESCE(API.QTD_LIQUIDA,0)
                        ELSE 0
                   END)                                              AS COMPRA_FOB,
               SUM(CASE WHEN FTC.COMPRA_OU_SERVICO ILIKE 'SERVIÇO'
                           THEN COALESCE(API.QTD_RETIRADA,0) + COALESCE(API.QTD_LIQUIDA,0)
                        ELSE 0
                   END)                                              AS VALOR_SERVICO,
               0                                                     AS COMPRA_CIF_BUNGE,
               0                                                     AS COMPRA_FOB_BUNGE,
               0                                                     AS VALOR_SERVICO_BUNGE
          FROM API_RECEBIMENTO_GRAOS API
               INNER JOIN DIM_EMPRESA                               DE ON DE.COD_EMPRESA          = API.EMPRESA::VARCHAR
                                                                      AND DE.CODIGO_LOCAL_FILIAL  = COALESCE(NULLIF(API.IDFILIAL_COMPRADOR, ''), API.IDFILIAL)
               INNER JOIN DIM_TEMPO_SAFRA                          DTS ON DTS.CHAVEERP            = API.SAFRA
               INNER JOIN DIM_PRODUTO                               DP ON DP.COD_PRODUTO          = API.PRODUTO::INTEGER::VARCHAR
               INNER JOIN DIM_PARCEIRO_SAP                          DS ON DS.NK_PARCEIRO          = API.COD_PARCEIRO::INTEGER::VARCHAR||'#'||LEFT(API.EMPRESA::VARCHAR,1) 
                LEFT JOIN DIM_CLIENTE_LOCAL                        DCL ON DCL.CHAVEERP_LOCAL      = API.COD_PARCEIRO
               INNER JOIN DIM_CLIENTE_LOCAL                       DCL2 ON DCL2.NK_CLIENTE_LOCAL   = 'NI#0'
               INNER JOIN FORMULARIO_TIPO_CONTRATOS_COMPRA_SERVICO FTC ON FTC.TIPO_CONTRATO       = API.TIPO_CONTRATO
         WHERE DCL.CLIENTE NOT ILIKE '%BUNGE%'
         GROUP BY 1,2,3,4
        
     UNION ALL
        
        SELECT COALESCE(DE.FILIAL_PADRAO, DE.SK_EMPRESA) AS SK_EMPRESA,
               DTS.SK_TEMPO_SAFRA,
               DP.SK_PRODUTO,
               DCL.SK_CLIENTE_LOCAL,
               0                AS COMPRA_CIF,
               0                AS COMPRA_FOB,
               0                AS VALOR_SERVICO,
               SUM(CASE WHEN FTC.COMPRA_OU_SERVICO ILIKE 'COMPRA' AND API.TIPO_FRETE ILIKE 'CIF'
                           THEN COALESCE(API.QTD_LIQUIDA,0)
                        ELSE 0
                   END)         AS COMPRA_CIF_BUNGE,
               SUM(CASE WHEN FTC.COMPRA_OU_SERVICO ILIKE 'COMPRA' AND API.TIPO_FRETE ILIKE 'FOB'
                           THEN COALESCE(API.QTD_LIQUIDA,0)
                        ELSE 0
                   END)         AS COMPRA_FOB_BUNGE,
               SUM(CASE WHEN FTC.COMPRA_OU_SERVICO ILIKE 'SERVIÇO'
                           THEN COALESCE(API.QTD_RETIRADA,0) + COALESCE(API.QTD_LIQUIDA,0)
                        ELSE 0
                   END)         AS VALOR_SERVICO_BUNGE
          FROM API_RECEBIMENTO_GRAOS API
               INNER JOIN DIM_EMPRESA                               DE ON DE.COD_EMPRESA         = API.EMPRESA::VARCHAR
                                                                      AND DE.CODIGO_LOCAL_FILIAL = COALESCE(NULLIF(API.IDFILIAL_COMPRADOR, ''), API.IDFILIAL)
               INNER JOIN DIM_TEMPO_SAFRA                          DTS ON DTS.CHAVEERP           = API.SAFRA
               INNER JOIN DIM_PRODUTO                               DP ON DP.COD_PRODUTO         = API.PRODUTO::INTEGER::VARCHAR
               INNER JOIN DIM_CLIENTE_LOCAL                        DCL ON DCL.CHAVEERP_LOCAL     = API.COD_PARCEIRO
               INNER JOIN FORMULARIO_TIPO_CONTRATOS_COMPRA_SERVICO FTC ON FTC.TIPO_CONTRATO      = API.TIPO_CONTRATO
         WHERE DCL.CLIENTE ILIKE '%BUNGE%'
         GROUP BY 1,2,3,4
       ) X
 GROUP BY 1,2,3,4</sql>
