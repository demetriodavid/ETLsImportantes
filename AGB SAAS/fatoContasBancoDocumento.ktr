    <connection>Oracle</connection>
    <sql>SELECT CODIGOLANCAMENTO, NUMEROLANCAMENTO, VALORLANCAMENTO, DATA, CONTA, CONCILIADO, TIPODOCUMENTO, COMPLEMENTO FROM (

SELECT l.ctrl_lan CODIGOLANCAMENTO, l.sequ_lan NUMEROLANCAMENTO, ((B.VLOR_BAI+B.MULT_BAI+B.JURO_BAI-B.DESC_BAI+B.ACRE_BAI)*case when td.tipo_tdo = 'D' then 1 else -1 end*case h.tipo_his WHEN 'D' THEN 1 WHEN 'C' THEN -1 END)  VALORLANCAMENTO,
l.data_lan DATA, l.codi_con CONTA, l.conc_lan CONCILIADO, cbr.codi_tdo TIPODOCUMENTO, l.comp_lan COMPLEMENTO

FROM LANCAMEN L INNER JOIN CRCBAIXA B ON L.CTRL_LAN = B.CTRL_LAN 
inner join receber rec on b.ctrl_rec = rec.ctrl_rec
inner join cabrec cbr on rec.ctrl_cbr = cbr.CTRL_CBR
inner join tipdoc td on cbr.codi_tdo = td.codi_tdo
INNER JOIN HISTORICO H ON L.HIST_HIS = H.HIST_HIS

union all 



--lancamento vinculado a doc a pagar
select l.ctrl_lan CODIGOLANCAMENTO, l.sequ_lan NUMEROLANCAMENTO, ((b.vlor_cpb+b.mult_cpb+b.juro_cpb+b.acre_cpb-b.desc_cpb)*case when td.tipo_tdo = 'D' then 1 else -1 end*case h.tipo_his WHEN 'D' THEN 1 WHEN 'C' THEN -1 END) valorLANCAMENTO,
l.data_lan DATA, l.Codi_con CONTA, l.conc_lan CONCILIADO,cpg.codi_tdo TIPODOCUMENTO, l.comp_lan COMPLEMENTO
from lancamen l inner join cpgbaixa b on b.ctrl_lan = l.ctrl_lan
inner join pagar pg on b.ctrl_pag = pg.ctrl_pag
inner join cabpagar cpg on pg.ctrl_cpg = cpg.ctrl_cpg
inner join tipdoc td on cpg.codi_tdo = td.codi_tdo
inner join historico h on l.hist_his = h.hist_his) WHERE DATA>=? ORDER BY DATA</sql>
