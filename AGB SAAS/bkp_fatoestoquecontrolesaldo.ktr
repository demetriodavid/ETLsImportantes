    <sql>SELECT codi_emp, codi_psv, cast(? as date) DATA, cast(? as date) INSERT_DATE, FATORCONVUNDBASICA,
CODI_EMP, CODI_PSV, cast(? as date) DATA1 FROM (
 (SELECT C.CODI_EMP, P.CODI_PSV, CASE P.CEST_PRO WHEN 'B' THEN 1 WHEN 'P' THEN P.QTE1_PRO WHEN 'S' THEN P.QTE2_PRO*P.QTE1_PRO ELSE 1 END FATORCONVUNDBASICA FROM 
 CADEMP C CROSS JOIN PRODUTO P INNER JOIN PRODSERV PS ON P.CODI_PSV = PS.CODI_PSV
 WHERE PS.SITU_PSV = 'A' and (PS.PRSE_PSV = 'P' OR PS.PRSE_PSV = 'K') AND ${codi_emp} and PS.CODI_GPR NOT IN (${excecaogrupoestoque})
 ORDER BY C.CODI_EMP, NLSSORT(P.CODI_PSV,'NLS_SORT=BINARY_AI')))CP

</sql>
