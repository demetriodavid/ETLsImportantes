    <connection>SQL Server</connection>
    <sql>WITH 
DIA AS (
	SELECT CAST(SUBSTRING(CAST(CAST(CURRENT_TIMESTAMP AS DATE) AS VARCHAR(10)), 9, 10) AS INT)-1 AS D
),
MES AS (
	SELECT CAST(SUBSTRING(CAST(CAST(CURRENT_TIMESTAMP AS DATE) AS VARCHAR(10)), 6, 2) AS INT) AS M
),
ANO AS (
	SELECT CAST(SUBSTRING(CAST(CAST(CURRENT_TIMESTAMP AS DATE) AS VARCHAR(10)), 1, 4) AS INT) AS A
),
FORMACAODATA AS (
	SELECT 
		CASE 
			WHEN (SELECT M FROM MES) = 03 
				THEN (SELECT CAST(CONCAT((SELECT A FROM ANO),'-',(SELECT M FROM MES)-1,'-',28) AS DATE))
			WHEN (SELECT D FROM DIA) = 0 AND (SELECT M FROM MES) IN (05,07,08,10,12) 
				THEN (SELECT CAST(CONCAT((SELECT A FROM ANO),'-',(SELECT M FROM MES)-1,'-',30) AS DATE))
			WHEN (SELECT D FROM DIA) = 0 AND (SELECT M FROM MES) = 01 
				THEN (SELECT CAST(CONCAT((SELECT A FROM ANO)-1,'-',12,'-',31) AS DATE))
			WHEN (SELECT D FROM DIA) = 0 AND (SELECT M FROM MES)  NOT IN (03,05,07,08,10,12) 
				THEN (SELECT CAST(CONCAT((SELECT A FROM ANO),'-',(SELECT M FROM MES)-1,'-',31) AS DATE))
			ELSE (SELECT CAST(CONCAT((SELECT A FROM ANO),'-',(SELECT M FROM MES),'-',(SELECT D FROM DIA)) AS DATE))
		END AS DT
		
	--union 
	--	  SELECT CAST(CONCAT((SELECT A FROM ANO),'-',(SELECT M FROM MES),'-',(SELECT D FROM DIA)) AS DATE) 				--LÓGICA COM ERRO COMENTADO 01/06/23
	
	union
	
	SELECT CAST(CURRENT_TIMESTAMP AS DATE)
)			  
SELECT * FROM (
	SELECT CAST(CURRENT_TIMESTAMP AS DATE) AS "DATA", CAST(CURRENT_TIMESTAMP AS DATE) AS data1, CAST(CURRENT_TIMESTAMP AS DATE) AS INSERT_DATE, CAST(CURRENT_TIMESTAMP AS DATE) AS data2
	UNION
	SELECT  (DT), (DT), (CAST(CURRENT_TIMESTAMP AS DATE)), (DT) 
	from FORMACAODATA F
	WHERE not exists (SELECT * FROM fatoestoquecontrolesaldo F2 where "DATA" &lt; INSERT_DATE and  F.DT = F2."DATA")
) X 
WHERE "DATA" IS NOT NULL


</sql>
