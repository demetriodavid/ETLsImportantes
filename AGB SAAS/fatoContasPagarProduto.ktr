  <step>
    <name>GET DATA ERP</name>
    <type>TableInput</type>
    <description>Reads information from table [TABDESC] on database [Oracle]</description>
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>Oracle</connection>
    <sql>SELECT CODIGO, PARCELA, DATA, VENCIMENTO, CODIGONOTA, NUMERONOTA, SERIENOTA, EMPRESA, PARCEIRO, TIPODOCUMENTO, CONDICAOPAGAMENTO, MOEDA, CICLO, TIPODOC, PRODUTO, cast(SUM(QUANTIDADE) as numeric(21,9)) QUANTIDADE, AVG(FATORCONVUNDBASICA) FATORCONVUNDBASICA, cast(AVG(VALORUNITARIO)as numeric(21,9)) VALORUNITARIO, cast(AVG(VALORUNITARIOOM)as numeric(21,9)) VALORUNITARIOOM, RENEGOCIACAO FROM 
(
	SELECT CPG.CTRL_CPG CODIGO
		, PG.CTRL_PAG PARCELA
		, CPG.DMOV_CPG DATA
		, PG.DVEN_PAG VENCIMENTO
		, N.NPRE_NOT CODIGONOTA
		, N.NOTA_NOT NUMERONOTA
		, N.SERI_NOT SERIENOTA
		, N.CODI_EMP EMPRESA
		, N.CODI_TRA PARCEIRO
		, CPG.CODI_TDO TIPODOCUMENTO
		, CPG.COND_CON CONDICAOPAGAMENTO
		, coalesce(CPG.CODI_IND, -1) MOEDA
		, coalesce(CPG.CODI_CIC, -1) CICLO
		, 'NE' TIPODOC
		, I.CODI_PSV PRODUTO
		, CASE WHEN I.QTDE_INO = 0 THEN 0 ELSE I.QTDE_INO *(PG.VLOR_PAG/case when FINANNOTA.TOTAL = 0 then 1 else FINANNOTA.TOTAL end) END * CASE T.TIPO_TDO WHEN 'C' THEN -1 ELSE 1 END QUANTIDADE
		, CASE P.CEST_PRO WHEN 'B' THEN 1 WHEN 'P' THEN P.QTE1_PRO WHEN 'S' THEN P.QTE2_PRO*P.QTE1_PRO ELSE 1 END FATORCONVUNDBASICA
		--, I.VALORUNITARIO
		, case when i.valorunitario = 0 then 0 else I.VALORUNITARIO*(FINANNOTA.TOTAL/TOTALNOTA.TOTAL) end VALORUNITARIO
		, case when i.valorunitario = 0 then 0 else I.VALORUNITARIO*(FINANNOTA.TOTAL/TOTALNOTA.TOTAL) end/CASE WHEN CPG.CODI_IND IS NULL THEN 1 ELSE (select VALOR_IND from table( RET_VLORIND(CPG.CODI_EMP,CPG.CODI_IND,CPG.DATA_VLR))) END VALORUNITARIOOM
		, 'N' RENEGOCIACAO
	FROM CABPAGAR CPG
    INNER JOIN PAGAR PG on PG.CTRL_CPG = CPG.CTRL_CPG

    INNER JOIN TIPDOC T on T.CODI_TDO = CPG.CODI_TDO
    INNER JOIN NOTACPG NC ON CPG.CTRL_CPG = NC.CTRL_CPG
    INNER JOIN NOTA N ON NC.CODI_EMP = N.CODI_EMP AND NC.CODI_TRA = N.CODI_TRA AND NC.NDOC_NCP = N.NOTA_NOT AND NC.SDOC_NCP = N.SERI_NOT AND NC.TDOC_NCP = 'NE'
    INNER JOIN (SELECT NPRE_NOT, CODI_PSV, CCFO_CFO, SUM(QTDE_INO) QTDE_INO, CASE WHEN SUM(QTDE_INO) = 0 THEN 0 ELSE SUM(QTDE_INO*VLIQ_INO)/SUM(QTDE_INO) END VALORUNITARIO FROM INOTA GROUP BY NPRE_NOT, CODI_PSV, CCFO_CFO) I ON N.NPRE_NOT = I.NPRE_NOT 
    INNER JOIN TIPOOPER TOP ON N.CODI_TOP = TOP.CODI_TOP
    INNER JOIN CFO ON I.CCFO_CFO = CFO.CCFO_CFO
    INNER JOIN (select  n.npre_not,sum(f.total) total 
				from nota n 
				inner join (select nc.codi_emp,  nc.ndoc_ncp nota_not, nc.sdoc_ncp seri_not, sum(c.tota_cpg) total 
							from notacpg nc 
							inner join cabpagar c on nc.ctrl_cpg = c.ctrl_cpg 
							WHERE NC.TDOC_NCP = 'NE' 
							group by nc.codi_emp,  nc.ndoc_ncp, nc.sdoc_ncp, nc.ctrl_ncp
							union all 
							select en.codi_emp, en.nota_not, en.seri_not,  sum(vlor_eno) total 
							from entrnota en 
							group by  en.codi_emp,  en.nota_not, en.seri_not) f on n.codi_emp = f.codi_emp  and n.nota_not = f.nota_not and n.seri_not = f.seri_not 
				group by n.npre_not) FINANNOTA ON N.NPRE_NOT = FINANNOTA.NPRE_NOT
	INNER JOIN (select N.NPRE_NOT, SUM(I.QTDE_INO*I.VALORUNITARIO) TOTAL 
				from NOTA N 
				INNER JOIN (SELECT NPRE_NOT, CODI_PSV, CCFO_CFO, SUM(QTDE_INO) QTDE_INO, CASE WHEN SUM(QTDE_INO) = 0 THEN 0 ELSE SUM(QTDE_INO*VLIQ_INO)/SUM(QTDE_INO) END VALORUNITARIO 
							FROM INOTA 
							GROUP BY NPRE_NOT, CODI_PSV, CCFO_CFO) I ON N.NPRE_NOT = I.NPRE_NOT 
				INNER JOIN TIPOOPER TOP ON N.CODI_TOP = TOP.CODI_TOP
				INNER JOIN CFO ON I.CCFO_CFO = CFO.CCFO_CFO 
				WHERE (TOP.TOPT_TOP IS NULL OR TOP.TOPT_TOP = 'N' OR (TOP.TOPT_TOP = 'S' AND CFO.GRFI_CFO = 'S'))
				GROUP BY N.NPRE_NOT) TOTALNOTA ON N.NPRE_NOT = TOTALNOTA.NPRE_NOT
	INNER JOIN PRODUTO P ON P.CODI_PSV = I.CODI_PSV
	LEFT JOIN RCPPAGAR RN ON (RN.CTRL_PAG = PG.CTRL_PAG AND RN.TIPO_RCP='R')

	WHERE (TOP.TOPT_TOP IS NULL OR TOP.TOPT_TOP = 'N' OR (TOP.TOPT_TOP = 'S' AND CFO.GRFI_CFO = 'S'))
	AND RN.CTRL_PAG IS NULL
       
       
    UNION ALL 
       
    SELECT CPG.CTRL_CPG CODIGO
		, PG.CTRL_PAG PARCELA
		, CPG.DMOV_CPG DATA
		, PG.DVEN_PAG VENCIMENTO
		, NF.CTRL_NFE CODIGONOTA
		, NF.NUME_NFE NUMERONOTA
		, NF.SERI_NFE SERIENOTA
		, NF.CODI_EMP EMPRESA
		, NF.CODI_TRA PARCEIRO
		, CPG.CODI_TDO TIPODOCUMENTO
		, CPG.COND_CON CONDICAOPAGAMENTO
		, coalesce(CPG.CODI_IND, -1) MOEDA
		, coalesce(CPG.CODI_CIC, -1) CICLO
		, 'NT' TIPODOC
		, INF.CODI_PSV PRODUTO
		, CASE WHEN INF.QUAN_INF = 0 THEN 0 ELSE INF.QUAN_INF *(PG.VLOR_PAG/case when FINANNFENTRA.TOTAL = 0 then 1 else FINANNFENTRA.TOTAL end) END * CASE T.TIPO_TDO WHEN 'C' THEN -1 ELSE 1 END QUANTIDADE
		, CASE P.CEST_PRO WHEN 'B' THEN 1 WHEN 'P' THEN P.QTE1_PRO WHEN 'S' THEN P.QTE2_PRO*P.QTE1_PRO ELSE 1 END FATORCONVUNDBASICA
		--, INF.VALORUNITARIO
		, CASE WHEN INF.VALORUNITARIO = 0 THEN 0 ELSE INF.VALORUNITARIO*(FINANNFENTRA.TOTAL/TOTALNFENTRA.TOTAL) END VALORUNITARIO
		, CASE WHEN INF.VALORUNITARIO = 0 THEN 0 ELSE INF.VALORUNITARIO*(FINANNFENTRA.TOTAL/TOTALNFENTRA.TOTAL) END /CASE WHEN CPG.CODI_IND IS NULL THEN 1 ELSE (select VALOR_IND from table( RET_VLORIND(CPG.CODI_EMP,CPG.CODI_IND,CPG.DATA_VLR))) END VALORUNITARIOOM
		, 'N' RENEGOCIACAO
	FROM CABPAGAR CPG
    INNER JOIN PAGAR PG on PG.CTRL_CPG = CPG.CTRL_CPG
    INNER JOIN TIPDOC T on T.CODI_TDO = CPG.CODI_TDO
    INNER JOIN NOTACPG NC ON CPG.CTRL_CPG = NC.CTRL_CPG
    INNER JOIN NFENTRA NF ON NC.CTRL_NCP = NF.CTRL_NFE AND NC.TDOC_NCP = 'NT' and nc.ndoc_ncp = NF.NUME_NFE AND NC.sdoc_ncp = NF.SERI_NFE
    INNER JOIN (SELECT CTRL_NFE, CODI_PSV, CCFO_CFO, SUM(QUAN_INF) QUAN_INF, CASE WHEN SUM(QUAN_INF) = 0 THEN 0 ELSE SUM(QUAN_INF*VLIQ_INF)/SUM(QUAN_INF) END VALORUNITARIO, CODI_EMP, NUME_NFE, SERI_NFE, CODI_TRA 
				FROM INFENTRA 
				GROUP BY CTRL_NFE, CODI_PSV, CCFO_CFO, CODI_EMP, NUME_NFE, SERI_NFE, CODI_TRA) INF ON NF.CTRL_NFE = INF.CTRL_NFE AND INF.CODI_EMP=NF.codi_emp AND INF.CODI_TRA=NF.codi_tra AND INF.NUME_NFE=NF.NUME_NFE and INF.SERI_NFE=NF.SERI_NFE
    INNER JOIN TIPOOPER TOP ON NF.CODI_TOP = TOP.CODI_TOP
	INNER JOIN CFO ON INF.CCFO_CFO = CFO.CCFO_CFO
	INNER JOIN (select nf.codi_emp, nf.codi_tra, nf.nume_nfe, nf.seri_nfe, nf.ctrl_nfe,sum(f.total) total 
				from nfentra nf 
				inner join (select nc.codi_emp, nc.codi_tra, nc.ndoc_ncp nume_nfe, nc.sdoc_ncp seri_nfe, nc.ctrl_ncp ctrl_nfe, sum(c.tota_cpg) total 
							from notacpg nc 
							inner join cabpagar c on nc.ctrl_cpg = c.ctrl_cpg 
							where nc.tdoc_ncp = 'NT' 
							group by nc.codi_emp, nc.codi_tra, nc.ndoc_ncp, nc.sdoc_ncp, nc.ctrl_ncp
							union all 
							select en.codi_emp, en.codi_tra, en.nume_nfe, en.seri_nfe, en.ctrl_nfe, sum(vlor_ene) total 
							from entrnfentra en 
							group by  en.codi_emp, en.codi_tra, en.nume_nfe, en.seri_nfe, en.ctrl_nfe) f on nf.codi_emp = f.codi_emp and nf.codi_tra = f.codi_tra and nf.nume_nfe = f.nume_nfe and nf.seri_nfe = f.seri_nfe and nf.ctrl_nfe = f.ctrl_nfe
				group by nf.codi_emp, nf.codi_tra, nf.nume_nfe, nf.seri_nfe, nf.ctrl_nfe, nf.tota_nfe) FINANNFENTRA ON NF.CTRL_NFE = FINANNFENTRA.CTRL_NFE AND NF.codi_emp = FINANNFENTRA.CODI_EMP AND NF.CODI_TRA = FINANNFENTRA.CODI_TRA AND NF.NUME_NFE = FINANNFENTRA.NUME_NFE AND NF.SERI_NFE = FINANNFENTRA.SERI_NFE
	INNER JOIN (SELECT NF.CTRL_NFE, NF.CODI_EMP, NF.CODI_TRA, NF.NUME_NFE, NF.SERI_NFE, SUM(INF.QUAN_INF*INF.VALORUNITARIO) TOTAL 
				FROM NFENTRA NF 
				INNER JOIN (SELECT CTRL_NFE, CODI_EMP, CODI_TRA, NUME_NFE, SERI_NFE, CODI_PSV, CCFO_CFO, SUM(QUAN_INF) QUAN_INF, CASE WHEN SUM(QUAN_INF) = 0 THEN 0 ELSE SUM(QUAN_INF*VLIQ_INF)/SUM(QUAN_INF) END VALORUNITARIO 
							FROM INFENTRA
					        GROUP BY CTRL_NFE, CODI_EMP, CODI_TRA, NUME_NFE, SERI_NFE, CODI_PSV, CCFO_CFO) INF ON NF.CTRL_NFE = INF.CTRL_NFE AND NF.CODI_EMP = INF.CODI_EMP AND NF.CODI_TRA = INF.CODI_TRA AND NF.NUME_NFE = INF.NUME_NFE AND NF.SERI_NFE = INF.SERI_NFE
				INNER JOIN TIPOOPER TOP ON NF.CODI_TOP = TOP.CODI_TOP
				INNER JOIN CFO ON INF.CCFO_CFO = CFO.CCFO_CFO
				WHERE (TOP.TOPT_TOP IS NULL OR TOP.TOPT_TOP = 'N' OR (TOP.TOPT_TOP = 'S' AND CFO.GRFI_CFO = 'S'))  
				GROUP BY NF.CTRL_NFE, NF.CODI_EMP, NF.CODI_TRA, NF.NUME_NFE, NF.SERI_NFE) TOTALNFENTRA ON NF.CTRL_NFE = TOTALNFENTRA.CTRL_NFE AND NF.codi_emp = TOTALNFENTRA.CODI_EMP AND NF.CODI_TRA = TOTALNFENTRA.CODI_TRA AND NF.NUME_NFE = TOTALNFENTRA.NUME_NFE AND NF.SERI_NFE = TOTALNFENTRA.SERI_NFE 
	INNER JOIN PRODUTO P ON P.CODI_PSV = INF.CODI_PSV
	LEFT JOIN RCPPAGAR RN ON (RN.CTRL_PAG = PG.CTRL_PAG AND RN.TIPO_RCP='R')

	WHERE (TOP.TOPT_TOP IS NULL OR TOP.TOPT_TOP = 'N' OR (TOP.TOPT_TOP = 'S' AND CFO.GRFI_CFO = 'S'))
	AND RN.CTRL_PAG IS NULL
)         
GROUP BY CODIGO, PARCELA, DATA, VENCIMENTO, CODIGONOTA, NUMERONOTA, SERIENOTA, EMPRESA, PARCEIRO, TIPODOCUMENTO, CONDICAOPAGAMENTO, MOEDA, CICLO, TIPODOC, PRODUTO, RENEGOCIACAO
order by DATA, PARCELA, NLSSORT(PRODUTO,'NLS_SORT=BINARY_AI')




       
       
       

       
       
       
