    <connection>Oracle</connection>
    <sql>SELECT DISTINCT N.CODI_EMP EMPRESA
	,N.COD1_PES VENDEDOR
	,N.CODI_TRA CLIENTE
	,I.CODI_PSV PRODUTO
	,(
		SELECT EMPR_PED
		FROM INOTA
		WHERE NPRE_NOT = N.NPRE_NOT
			AND ROWNUM = 1
		) EMPRESA_PEDIDO
	,(
		SELECT PEDI_PED
		FROM INOTA
		WHERE NPRE_NOT = N.NPRE_NOT
			AND ROWNUM = 1
		) NUMERO_PEDIDO
	,(
		SELECT SERI_PED
		FROM INOTA
		WHERE NPRE_NOT = N.NPRE_NOT
			AND ROWNUM = 1
		) SERIE_PEDIDO
	,(
		SELECT P.DEMI_PED
		FROM INOTA I
		INNER JOIN PEDIDO P ON (
				I.EMPR_PED = P.CODI_EMP
				AND I.PEDI_PED = P.PEDI_PED
				AND I.SERI_PED = P.SERI_PED
				)
		WHERE NPRE_NOT = N.NPRE_NOT
			AND ROWNUM = 1
		) DATA_PEDIDO
	,N.CODI_EMP EMPRESA_NOTA
	,N.NOTA_NOT NUMERO_NOTA
	,N.SERI_NOT SERIE_NOTA
	,N.DEMI_NOT DATA_NOTA
	,
	--CAST ((I.QTDE_INO*I.VLIQ_INO)AS NUMBER(21,9)) VALOR_ITEM,
	CASE 
		WHEN (
				SELECT COUNT(*)
				FROM CRCBAIXA
				WHERE CTRL_REC = R.CTRL_REC
				) > 1
			AND (
				SELECT COUNT(*)
				FROM CRCBAIPR
				WHERE SEQU_BAI = CRC.SEQU_BAI
				) > 1
			THEN (CAST((
						SELECT QTDP_BPR * I.VLIQ_INO
						FROM CRCBAIPR
						WHERE SEQU_BAI = CRC.SEQU_BAI
							AND CODI_PSV = I.CODI_PSV
						) - ABS(coalesce(CRC.VVCA_BAI,0)) AS NUMBER(21, 9)))
		WHEN (
				SELECT COUNT(*)
				FROM CRCBAIXA
				WHERE CTRL_REC = R.CTRL_REC
				) > 1
			AND (
				SELECT COUNT(*)
				FROM CRCBAIPR
				WHERE SEQU_BAI = CRC.SEQU_BAI
				) &lt;= 1
			THEN CAST((CRC.VLOR_BAI - CRC.JURO_BAI - CRC.DESC_BAI - ABS(coalesce(CRC.VVCA_BAI,0))) AS NUMBER(21, 9))
		WHEN (
				SELECT COUNT(*)
				FROM CRCBAIXA
				WHERE CTRL_REC = R.CTRL_REC
				) = 1
			AND (N.TOTA_NOT) = (
				SELECT VLOR_BAI
				FROM CRCBAIXA
				WHERE CTRL_REC = CRC.CTRL_rec
				)
			THEN (CAST((I.QTDE_INO * I.VLIQ_INO) - ABS(coalesce(CRC.VVCA_BAI,0)) AS NUMBER(21, 9)))
		WHEN (
				SELECT COUNT(*)
				FROM CRCBAIXA
				WHERE CTRL_REC = R.CTRL_REC
				) = 1
			AND (N.TOTA_NOT) &lt;&gt; (
				SELECT VLOR_BAI
				FROM CRCBAIXA
				WHERE CTRL_REC = CRC.CTRL_rec
				)
			THEN (CAST((
						SELECT QTDP_BPR * I.VLIQ_INO
						FROM CRCBAIPR
						WHERE SEQU_BAI = CRC.SEQU_BAI
							AND CODI_PSV = I.CODI_PSV
						) - ABS(coalesce(CRC.VVCA_BAI,0)) AS NUMBER(21, 9)))
		ELSE CAST((CRC.VLOR_BAI - CRC.JURO_BAI - CRC.DESC_BAI - ABS(coalesce(CRC.VVCA_BAI,0))) AS NUMBER(21, 9))
		END VALOR_ITEM
	,
	--CAST ((CRC.VLOR_BAI)AS NUMBER(21,9)) VALOR_ITEM,
	CRC.DPAG_BAI DATA_PAGAMENTO_NOTA
	,CASE 
		WHEN CRC.DPAG_BAI > R.VENC_REC
			THEN 'S'
		ELSE 'N'
		END PAGO_VENCIDO
	,(TO_DATE(CRC.DPAG_BAI) - TO_DATE(R.VENC_REC)) DIAS_VENCIDO
	,CASE 
		WHEN (
				SELECT COUNT(*)
				FROM CABREC C2
				WHERE C2.CODI_TDO = 103
					AND C2.TOTA_CBR = C.TOTA_CBR
					AND C2.CODI_TRA = C.CODI_TRA
				) > 0
			THEN 'S'
		ELSE 'N'
		END ADIANTAMENTO
	,(
		SELECT C4.NUME_CBR || '-' || C4.SERI_CBR
		FROM CABREC C4
		WHERE C4.CODI_TDO = 103
			AND C4.TOTA_CBR = C.TOTA_CBR
			AND C4.CODI_TRA = C.CODI_TRA
			AND C4.SITU_CBR = 'A'
			AND ROWNUM = 1
		) DOCUMENTO_ADIANTAMENTO
	,(
		SELECT C3.DATA_CBR
		FROM CABREC C3
		INNER JOIN RECEBER R2 ON (C3.CTRL_CBR = R2.CTRL_CBR)
		INNER JOIN CRCBAIXA CRC2 ON (
				R2.CTRL_REC = CRC2.CTRL_REC
				AND ROWNUM = 1
				)
		WHERE C3.CODI_TDO = 103
			AND C3.TOTA_CBR = C.TOTA_CBR
			AND C3.CODI_TRA = C.CODI_TRA
			AND C3.SITU_CBR = 'A'
			AND CRC2.SITU_BAI = 'N'
		) DATA_PAGAMENTO_ADIANTAMENTO
	,N.NPRE_NOT CONTROLE
FROM NOTA N
INNER JOIN INOTA I ON (N.NPRE_NOT = I.NPRE_NOT)
INNER JOIN NOTACRC NC ON (
		N.CODI_TRA = NC.CODI_TRA
		AND N.CODI_EMP = NC.CODI_EMP
		AND N.NOTA_NOT = NC.NDOC_NOC
		AND N.SERI_NOT = NC.SDOC_NOC
		)
INNER JOIN CABREC C ON (NC.CTRL_CBR = C.CTRL_CBR)
INNER JOIN RECEBER R ON (C.CTRL_CBR = R.CTRL_CBR)
INNER JOIN CRCBAIXA CRC ON (R.CTRL_REC = CRC.CTRL_REC)
WHERE C.SITU_CBR = 'A'
	AND R.SITU_REC = 'A'
	AND CRC.SITU_BAI = 'N'
	AND NOT EXISTS (
		SELECT *
		FROM NOTAORIG NO
		INNER JOIN NOTA N2 ON (
				NO.NOTD_NFR = N2.NPRE_NOT
				AND NO.CODI_EMP = N2.CODI_EMP
				AND NO.TRAD_NFR = N2.CODI_TRA
				AND N2.CODI_TOP IN (40)
				)
		WHERE NO.NOTO_NFR = N.NPRE_NOT
			AND NO.CODI_EMP = N.CODI_EMP
			AND NO.TRAO_NFR = N.CODI_TRA
		)
	AND N.CODI_TOP NOT IN (40)</sql>
