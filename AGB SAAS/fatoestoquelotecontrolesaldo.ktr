    <connection>Oracle</connection>
    <sql>select EMPRESA, PRODUTO, DEPOSITO, ctrsaldo, LOTE, VALIDADE, ESTOQUE, CUSTOMEDIO, DATA, concluido from (
select c.codi_emp EMPRESA
	, p.codi_psv PRODUTO
	--, -1 DEPOSITO
	, D.CODI_DPT DEPOSITO
	, cr.codi_ctr ctrsaldo
	, COALESCE(L.LOTE_LOT, '-1') LOTE
	, l.valg_lot VALIDADE
	--, (select QTDE from table(SALDO_LOTE_CTR(c.codi_emp, p.codi_psv, l.lote_lot, ?,0, 0, cr.codi_ctr ))) ESTOQUE
	, (select QTDE from table(SALDO_LOTE_CTR(c.codi_emp, p.codi_psv, l.lote_lot, ?,D.CODI_DPT, 0, cr.codi_ctr ))) ESTOQUE
	--, (select CUST_MED from table(CUSTO_MEDIO(c.codi_emp,p.codi_psv,sysdate))) CUSTOMEDIO,
	, 0 CUSTOMEDIO --preparaDados irá dar update no custo
	, ? DATA
	, null concluido
from cademp c, produto p
join prodserv pd on p.codi_psv = pd.CODI_PSV 
INNER JOIN LOTE L ON P.CODI_PSV = L.CODI_PSV
CROSS JOIN DEPOSITO D
cross join CTRLSALDO CR 
where pd.SITU_PSV = 'A' 
	AND L.SITU_LOT = 'A' 
	AND P.CLOT_PRO = 'S'
	--and D.CODI_DPT = 1
	and D.CODI_DPT in (1, 5)
	and CR.codi_ctr in (1,17,9)
) x
where ESTOQUE &lt;&gt; 0</sql>
