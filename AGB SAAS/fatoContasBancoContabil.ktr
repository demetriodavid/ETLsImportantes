    <connection>Oracle</connection>
    <sql>SELECT ctrl_lan CODIGOLANCAMENTO, SEQU_LAN NUMEROLANCAMENTO, CODI_CPC CONTACONTABIL, CODI_PLC PLANOCONTAS, VALORLANCAMENTO, DATA_LAN DATA, CODI_CON CONTA, comp_lan COMPLEMENTO, CONC_LAN CONCILIADO FROM (
select l.ctrl_lan,L.SEQU_LAN, I.CODI_CPC,I.CODI_PLC,
--CASE H.TIPO_HIS WHEN 'C' THEN 'S' WHEN 'D' THEN 'E' ELSE 'N/A' END "E/S" ,
(case when (H.TIPO_HIS = 'C' AND I.TIPO_LCT = 'D') OR (H.TIPO_HIS = 'D' AND I.TIPO_LCT = 'D') THEN -1 ELSE 1 END * i.VLOR_LCT) VALORLANCAMENTO,
--I.TIPO_LCT,
L.DATA_LAN, L.CODI_CON, L.COMP_LAN, L.CONC_LAN
--l.SEQU_LAN, 
--vlor_lan, i.VLOR_LCT 
from lancamen l 
inner join historico h on l.hist_his = h.hist_his
inner join cablanctb c on c.ctrl_clc = l.ctrl_lan and C.orig_clc = 'FC'
inner join lancontab i on i.SEQU_clc = c.SEQU_CLC  
inner join conta con on l.codi_con = con.codi_con
INNER join CONTASPLCONTA cpc on con.codi_con = cpc.codi_con and i.codi_plc = cpc.codi_plc and i.codi_cpc &lt;&gt; cpc.codi_cpc

--GROUP BY L.CTRL_LAN,L.SEQU_LAN, I.CODI_CPC, I.CODI_PLC,L.DATA_LAN, L.CODI_CON, L.COMP_LAN, L.CONC_LAN

UNION ALL

--TRANSFERENCIA (ENTRADA)
SELECT  l.ctrl_lan,L.SEQU_LAN, I.CODI_CPC,I.CODI_PLC,
--CASE H.TIPO_HIS WHEN 'C' THEN 'S' WHEN 'D' THEN 'E' ELSE 'N/A' END "E/S" ,
(case when (H.TIPO_HIS = 'C' AND I.TIPO_LCT = 'D') OR (H.TIPO_HIS = 'D' AND I.TIPO_LCT = 'D') THEN -1 ELSE 1 END * i.VLOR_LCT) VALORLANCAMENTO,
--I.TIPO_LCT,
L.DATA_LAN, L.CODI_CON, L.COMP_LAN, L.CONC_LAN
FROM LANCAMEN L 
inner join historico h on l.hist_his = h.hist_his
INNER JOIN LANCAMEN L2 ON L.CTRL_LAN = L2.CREL_LAN
INNER JOIN CABLANCTB c on c.ctrl_clc = l2.ctrl_lan and C.orig_clc = 'FC'
inner join lancontab i on i.SEQU_clc = c.SEQU_CLC 
WHERE L.SITU_LAN &lt;&gt; 'C'--SOMENTE QUANDO NÃO FOR COMPENSAÇÃO
--GROUP BY L.CTRL_LAN,L.SEQU_LAN, I.CODI_CPC, I.CODI_PLC,L.DATA_LAN, L.CODI_CON, L.COMP_LAN, L.CONC_LAN

UNION ALL

--TRANSFERENCIA (SAIDA)
SELECT  l.ctrl_lan,L.SEQU_LAN, I.CODI_CPC,I.CODI_PLC,
--CASE H.TIPO_HIS WHEN 'C' THEN 'S' WHEN 'D' THEN 'E' ELSE 'N/A' END "E/S" ,
(case when (H.TIPO_HIS = 'C' AND I.TIPO_LCT = 'D') OR (H.TIPO_HIS = 'D' AND I.TIPO_LCT = 'D') THEN -1 ELSE 1 END * i.VLOR_LCT) VALORLANCAMENTO,
--I.TIPO_LCT,
L.DATA_LAN, L.CODI_CON, L.COMP_LAN, L.CONC_LAN
FROM LANCAMEN L 
inner join historico h on l.hist_his = h.hist_his
INNER JOIN LANCAMEN L2 ON L.CREL_LAN = L2.CTRL_LAN
